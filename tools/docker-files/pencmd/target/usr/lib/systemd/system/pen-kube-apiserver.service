[Unit]
Description=Kubernetes API Server

[Service]
User=root
Type=oneshot
EnvironmentFile=-/etc/pensando/kube-apiserver.conf
ExecStart=/usr/bin/docker run --privileged --net=host --name kube-apiserver -d gcr.io/google_containers/kube-apiserver-amd64:v1.6.6 \
			kube-apiserver \
			$INSECURE_ALLOW \
			$INSECURE_ADDR \
			$INSECURE_PORT \
			$ETCD_SERVERS \
			$SERVICE_CLUSTER 
LimitNOFILE=40000
