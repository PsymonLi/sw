{
  "compileOnSave": false,
  "compilerOptions": {
    "newLine": "lf",
    "sourceMap": false,
    "outDir": "./dist/out-tsc",
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ]
  },
  "exclude": [
    "v1",
    "metrics"
  ]
}
