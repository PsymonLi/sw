{
  "name": "web-app-framework",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "ng test",
    "test-prod": "ng test --prod --watch=false --source-map=false --browsers=ChromeHeadlessCI",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "packagr": "ng-packagr -p ng-package.json",
    "bundle-styles": "scss-bundle -c scss-bundle.config.json",
    "pack": "yarn run packagr && yarn run bundle-styles && tar cvf web-app-framework.tgz ./dist",
    "replaceShrinkwrap": "sed -i.bak 's/http:/https:/g' npm-shrinkwrap.json && rm npm-shrinkwrap.json.bak",
    "ngcc": "ngcc"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "9.1.12",
    "@angular/cdk": "^9.1.0",
    "@angular/common": "9.1.12",
    "@angular/compiler": "9.1.12",
    "@angular/core": "9.1.12",
    "@angular/flex-layout": "9.0.0-beta.31",
    "@angular/forms": "9.1.12",
    "@angular/material": "^9.1.0",
    "@angular/platform-browser": "9.1.12",
    "@angular/platform-browser-dynamic": "9.1.12",
    "@angular/router": "9.1.12",
    "@fullcalendar/core": "^5.1.0",
    "ajv": "^6.12.3",
    "chart.js": "^2.9.3",
    "core-js": "^3.6.5",
    "d3": "^5.16.0",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "lodash": "^4.17.5",
