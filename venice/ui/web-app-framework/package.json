    "ng-bullet": "^1.0.3",
    "ng-packagr": "^9.0.0",
    "protractor": "~7.0.0",
    "scss-bundle": "^3.1.2",
    "ts-node": "~8.10.2",
    "tslint": "~6.1.2"
  }
