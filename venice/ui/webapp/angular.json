{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "venice-ui": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist",
            "index": "src/index.html",
            "main": "src/main.ts",
            "tsConfig": "src/tsconfig.app.json",
            "polyfills": "src/polyfills.ts",
            "stylePreprocessorOptions": {
              "includePaths": [
                "src",
                "src/themes"
              ]
            },
            "assets": [
              "src/assets",
              "src/data",
              "src/favicon.ico",
              {
                "glob": "*.svg",
                "input": "node_modules/material-design-icons/sprites/svg-sprite",
                "output": "/assets/svg-icons"
              }
            ],
            "styles": [
              "src/styles.scss",
              "src/theme.scss",
              "src/styles-app-loading.scss",
              "node_modules/font-awesome/css/font-awesome.css",
              "node_modules/primeng/resources/themes/omega/theme.css",
              "node_modules/primeng/resources/primeng.css",
              "node_modules/primeicons/primeicons.css",
              "node_modules/material-design-icons/iconfont/material-icons.css",
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "node_modules/typeface-fira-sans-condensed/index.css",
              "node_modules/typeface-roboto-condensed/index.css"
            ],
            "scripts": [
              "node_modules/plotly.js/dist/plotly.min.js",
              "node_modules/chart.js/dist/Chart.js",
              "node_modules/chartjs-plugin-datalabels/dist/chartjs-plugin-datalabels.js",
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/bootstrap/dist/js/bootstrap.js"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "venice-ui:build",
            "ssl": true,
            "proxyConfig": "proxy.conf.js"
          },
          "configurations": {
            "production": {
              "browserTarget": "venice-ui:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "venice-ui:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "scripts": [
              "node_modules/plotly.js/dist/plotly.min.js",
              "node_modules/chart.js/dist/Chart.js",
              "node_modules/chartjs-plugin-datalabels/dist/chartjs-plugin-datalabels.js",
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/bootstrap/dist/js/bootstrap.js"
            ],
            "styles": [
              "src/styles.scss",
              "src/theme.scss",
              "src/styles-app-loading.scss",
              "node_modules/font-awesome/css/font-awesome.css",
              "node_modules/primeng/resources/themes/omega/theme.css",
              "node_modules/primeng/resources/primeng.css",
              "node_modules/primeicons/primeicons.css",
              "node_modules/material-design-icons/iconfont/material-icons.css",
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "node_modules/typeface-fira-sans-condensed/index.css",
              "node_modules/typeface-roboto-condensed/index.css"
            ],
            "assets": [
              "src/assets",
              "src/data",
              "src/favicon.ico",
              {
                "glob": "*.svg",
                "input": "node_modules/material-design-icons/sprites/svg-sprite",
                "output": "/assets/svg-icons"
              }
            ]
          },
          "configurations": {
            "production": {
              "sourceMap": false,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "venice-ui-e2e": {
      "root": "e2e",
      "sourceRoot": "e2e",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./protractor.conf.js",
            "devServerTarget": "venice-ui:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "e2e/tsconfig.e2e.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "venice-ui",
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "app",
      "styleext": "scss"
    },
    "@schematics/angular:directive": {
      "prefix": "app"
    }
  }
}
