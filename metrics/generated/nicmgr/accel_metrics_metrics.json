          "Name": "Key",
          "DisplayName": "Key indices",
          "Units": "ID",
          "BaseType": ".accel_metrics.AccelHwRingKey"
        },
        {
          "Name": "PIndex",
          "DisplayName": "PIndex",
          "Description": "ring producer index",
          "Units": "ID",
          "BaseType": "Counter"
        },
        {
          "Name": "CIndex",
