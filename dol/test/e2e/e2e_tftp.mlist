# Feature Test Modules List
feature:
    id       : e2e 
    sub      : e2e
    package  : test.e2e.tftp
    module   : tftp
    runorder : 0
    enable   : True
    ignore   : False
    rtl      : False
    args     :

modules:
    - module:
        name    : TFTP_REMOTE_REMOTE
        spec    : tftp.testspec
        selectors :
              src_endpoints :
                    type : filter://remote=True
                    max  : 1
              dst_endpoints : 
                    type : filter://remote=True
                    max  : 1
    - module:
        name    : TFTP_REMOTE_LOCAL
        spec    : tftp.testspec
        selectors :
              src_endpoints :
                    type : filter://remote=True
                    max  : 1
              dst_endpoints : 
                    type : filter://remote=False
                    max  : 1                    
    - module:
        name    : TFTP_LOCAL_LOCAL
        spec    : tftp.testspec
        selectors :
              src_endpoints :
                    type : filter://remote=False
                    max  : 1
              dst_endpoints : 
                    type : filter://remote=False
                    max  : 1
