# Test Spec
config_filter:
    src:
        tenant      : filter://any
        segment     : filter://any
        endpoint    : filter://any
        interface   : filter://any
    dst:
        tenant      : filter://any
        segment     : filter://any
        endpoint    : filter://any
        interface   : filter://any
    flow        : filter://fwtype=L2,type=MAC,label=NETWORKING
    maxflows    : 1 

packets:
    - packet:
        id          : PKT1     # Input packet 1
        payloadsize : ref://factory/payloads/id=PAYLOAD_ZERO64/size
        template    : ref://factory/packets/id=ETH
        encaps      : 
            - ref://factory/packets/id=ENCAP_QTAG
        headers     :
            # Add all possible headers, the final headers will be selected
            # based on the Flow.
            eth:
                src     : macaddr/0000.1111.2222
                dst     : macaddr/0000.3333.4444
                type    : ethertype/0x1234
            qtag:
                vlan    : ref://testcase/config/src/segment/vlan_id
            payload:
                data    : ref://factory/payloads/id=PAYLOAD_ZERO64/data

trigger:
    packets:
        - packet:
            object  : ref://testcase/packets/id=PKT1
            port    : 1

expect:
