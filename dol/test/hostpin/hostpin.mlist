# Feature Test Modules List
feature:
    id          : hostpin
    package     : test.hostpin
    module      : hostpin
    enable      : True
    ignore      : False

modules:
    - module:
        name    : TX
        spec    : tx.testspec
        iterate:
            - flow      : filter://type=IPV4,proto=UDP,label=NETWORKING
              segment   : filter://native=False

    - module:
        name    : UP2UP_DROP
        spec    : up2up.testspec
        iterate:
            - flow      : filter://type=IPV4,proto=UDP,label=NETWORKING
              segment   : filter://native=False

