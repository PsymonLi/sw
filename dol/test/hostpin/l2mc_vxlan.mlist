# Feature Test Modules List
feature:
    id          : pinl2mc
    sub         : vxlan
    package     : test.hostpin
    module      : hostpin
    enable      : True
    ignore      : False

modules:
    - module:
        name    : ENIC
        spec    : l2mc_enic_vxlan.testspec
        ignore  : False
        iterate :
            - id  : L2MC-UPLINK
              flow: filter://type=IPV4,proto=UDP,label=NETWORKING

    - module:
        name    : UPLINK
        spec    : l2mc_uplink_vxlan.testspec
        ignore  : False
        iterate :
            - id  : L2MC-UPLINK
              flow: filter://type=IPV4,proto=UDP,label=NETWORKING

    - module:
        name    : UPLINK_RPF_FAIL
        spec    : l2mc_uplink_vxlan.testspec
        ignore  : False
        iterate :
            - id        : L2MC-UPLINK
              scenario  : RPF_FAILURE
              flow      : filter://type=IPV4,proto=UDP,label=NETWORKING

