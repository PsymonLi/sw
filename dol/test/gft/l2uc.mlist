# Feature Test Modules List
feature:
    id          : gft
    sub         : l2uc
    package     : test.gft
    module      : gft
    enable      : True
    ignore      : False
    spec        : l2uc.testspec

modules:
    - module:
        name    : IPV4_UDP_QTAG_MODIFY
        iterate :
            - flow    : filter://type=IPV4,proto=UDP,label=GFT
              segment : filter://native=False

    - module:
        name    : IPV4_TCP_QTAG_MODIFY
        iterate :
            - flow    : filter://type=IPV4,proto=TCP,label=GFT
              segment : filter://native=False

    - module:
        name    : IPV4_ICMP_QTAG_MODIFY
        iterate :
            - flow    : filter://type=IPV4,proto=ICMP,label=GFT
              segment : filter://native=False

    - module:
        name    : IPV4_UDP_UTAG
        ignore  : True
        iterate :
            - flow    : filter://type=IPV4,proto=UDP,label=GFT
              segment : filter://native=True

    - module:
        name    : IPV4_TCP_UTAG
        ignore  : True
        iterate :
            - flow    : filter://type=IPV4,proto=TCP,label=GFT
              segment : filter://native=True

    - module:
        name    : IPV4_ICMP_UTAG
        ignore  : True
        iterate :
            - flow    : filter://type=IPV4,proto=ICMP,label=GFT
              segment : filter://native=True
