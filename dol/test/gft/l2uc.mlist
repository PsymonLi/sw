# Feature Test Modules List
feature:
    id          : gft
    sub         : l2uc
    package     : test.gft
    module      : gft
    enable      : True
    ignore      : False

modules:
    - module:
        name    : IPV4_UDP_QTAG
        spec    : l2uc.testspec
        perf    : True
        iterate :
            - flow    : filter://type=IPV4,proto=UDP,label=GFT
              segment : filter://native=False

    - module:
        name    : IPV4_TCP_QTAG
        spec    : l2uc.testspec
        perf    : True
        iterate :
            - flow    : filter://type=IPV4,proto=TCP,label=GFT
              segment : filter://native=False

    - module:
        name    : IPV4_ICMP_QTAG
        spec    : l2uc.testspec
        perf    : True
        iterate :
            - flow    : filter://type=IPV4,proto=ICMP,label=GFT
              segment : filter://native=False
