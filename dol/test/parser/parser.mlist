# Feature Test Modules List
feature:
    id      : parser
    sub     : ip
    package : test.parser
    module  : parser
    enable  : True
    ignore  : False

modules:
    - module:
        name    : IP_PROTO255
        spec    : proto255.testspec
        iterate :
            - flow : filter://type=IPV4,proto=PROTO255,label=NETWORKING
            - flow : filter://type=IPV6,proto=PROTO255,label=NETWORKING

    - module:
        name    : TCPSACK
        ignore  : True
        spec    : tcpopts.testspec
        iterate :
            - opttype   : TCP_SACK
              optcount  : 1
            - opttype   : TCP_SACK
              optcount  : 2
            - opttype   : TCP_SACK
              optcount  : 3
            - opttype   : TCP_SACK
              optcount  : 4

    - module:
        name    : TCPNOP
        ignore  : True
        spec    : tcpopts.testspec
        iterate :
            - opttype   : TCP_NOP
              optcount  : 1
            - opttype   : TCP_NOP
              optcount  : 2
            - opttype   : TCP_NOP
              optcount  : 3
            - opttype   : TCP_NOP
              optcount  : 4

