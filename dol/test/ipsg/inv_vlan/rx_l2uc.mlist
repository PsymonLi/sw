# Feature Test Modules List
feature:
    id          : ipsg
    sub         : inv_vlan_rx_l2uc
    package     : test.ipsg.inv_vlan
    module      : ipsg
    spec        : rx_l2uc.testspec
    enable      : True
    ignore      : False


modules:
    - module:
        name    : IPV4_QTAG_UDP
        iterate :
            - flow    : filter://type=IPV4,proto=UDP,label=NETWORKING
              segment : filter://native=False

    - module:
        name    : IPV4_QTAG_TCP
        iterate :
            - flow    : filter://type=IPV4,proto=TCP,label=NETWORKING
              segment : filter://native=False

    - module:
        name    : IPV4_QTAG_ICMP
        iterate :
            - flow    : filter://type=IPV4,proto=ICMP,label=NETWORKING
              segment : filter://native=False

    - module:
        name    : IPV6_QTAG_UDP
        iterate :
            - flow    : filter://type=IPV6,proto=UDP,label=NETWORKING
              segment : filter://native=False

    - module:
        name    : IPV6_QTAG_TCP
        iterate :
            - flow    : filter://type=IPV6,proto=TCP,label=NETWORKING
              segment : filter://native=False

    - module:
        name    : IPV6_QTAG_ICMPV6
        iterate :
            - flow    : filter://type=IPV6,proto=ICMPV6,label=NETWORKING
              segment : filter://native=False

    - module:
        name    : IPV4_UTAG_UDP
        iterate :
            - flow    : filter://type=IPV4,proto=UDP,label=NETWORKING
              segment : filter://native=True

    - module:
        name    : IPV4_UTAG_TCP
        iterate :
            - flow    : filter://type=IPV4,proto=TCP,label=NETWORKING
              segment : filter://native=True

    - module:
        name    : IPV4_UTAG_ICMP
        iterate :
            - flow    : filter://type=IPV4,proto=ICMP,label=NETWORKING
              segment : filter://native=True

    - module:
        name    : IPV6_UTAG_UDP
        iterate :
            - flow    : filter://type=IPV6,proto=UDP,label=NETWORKING
              segment : filter://native=True

    - module:
        name    : IPV6_UTAG_TCP
        iterate :
            - flow    : filter://type=IPV6,proto=TCP,label=NETWORKING
              segment : filter://native=True

    - module:
        name    : IPV6_UTAG_ICMPV6
        iterate :
            - flow    : filter://type=IPV6,proto=ICMPV6,label=NETWORKING
              segment : filter://native=True

    - module:
        name    : IPV4_PTAG_UDP
        iterate :
            - flow    : filter://type=IPV4,proto=UDP,label=NETWORKING
              segment : filter://native=True
              priotag : True

    - module:
        name    : IPV4_PTAG_TCP
        iterate :
            - flow    : filter://type=IPV4,proto=TCP,label=NETWORKING
              segment : filter://native=True
              priotag : True

    - module:
        name    : IPV4_PTAG_ICMP
        iterate :
            - flow    : filter://type=IPV4,proto=ICMP,label=NETWORKING
              segment : filter://native=True
              priotag : True

    - module:
        name    : IPV6_PTAG_UDP
        iterate :
            - flow    : filter://type=IPV6,proto=UDP,label=NETWORKING
              segment : filter://native=True
              priotag : True

    - module:
        name    : IPV6_PTAG_TCP
        iterate :
            - flow    : filter://type=IPV6,proto=TCP,label=NETWORKING
              segment : filter://native=True
              priotag : True

    - module:
        name    : IPV6_PTAG_ICMPV6
        iterate :
            - flow    : filter://type=IPV6,proto=ICMPV6,label=NETWORKING
              segment : filter://native=True
              priotag : True
