# Feature Test Modules List
feature:
    id          : recirc
    package     : test.networking
    module      : recirc
    enable      : True
    ignore      : False

modules:
    - module:
        name    : 21BIT_IPV4
        spec    : l2uc.testspec
        args    :
            maxflows : 1
        iterate:
            - flow    : filter://type=IPV4,proto=TCP,label=RECIRC_21BIT_FLOW_1,direction=IFLOW
            - flow    : filter://type=IPV4,proto=TCP,label=RECIRC_21BIT_FLOW_1,direction=RFLOW
            - flow    : filter://type=IPV4,proto=UDP,label=RECIRC_21BIT_FLOW_1,direction=IFLOW
            - flow    : filter://type=IPV4,proto=UDP,label=RECIRC_21BIT_FLOW_1,direction=RFLOW

    - module:
        name    : 21BIT_IPV6
        spec    : l2uc.testspec
        args    :
            maxflows : 1
        iterate:
            - flow    : filter://type=IPV6,proto=TCP,label=RECIRC_21BIT_FLOW_1,direction=IFLOW
            - flow    : filter://type=IPV6,proto=TCP,label=RECIRC_21BIT_FLOW_1,direction=RFLOW
            - flow    : filter://type=IPV6,proto=UDP,label=RECIRC_21BIT_FLOW_1,direction=IFLOW
            - flow    : filter://type=IPV6,proto=UDP,label=RECIRC_21BIT_FLOW_1,direction=RFLOW

    - module:
        name    : 32BIT_IPV4
        spec    : l2uc.testspec
        args    :
            maxflows : 1
        iterate:
            - flow    : filter://type=IPV4,proto=TCP,label=RECIRC_32BIT_FLOW_1,direction=IFLOW
            - flow    : filter://type=IPV4,proto=TCP,label=RECIRC_32BIT_FLOW_1,direction=RFLOW
            - flow    : filter://type=IPV4,proto=UDP,label=RECIRC_32BIT_FLOW_1,direction=IFLOW
            - flow    : filter://type=IPV4,proto=UDP,label=RECIRC_32BIT_FLOW_1,direction=RFLOW

    - module:
        name    : 32BIT_IPV6
        spec    : l2uc.testspec
        args    :
            maxflows : 1
        iterate:
            - flow    : filter://type=IPV6,proto=TCP,label=RECIRC_32BIT_FLOW_1,direction=IFLOW
            - flow    : filter://type=IPV6,proto=TCP,label=RECIRC_32BIT_FLOW_1,direction=RFLOW
            - flow    : filter://type=IPV6,proto=UDP,label=RECIRC_32BIT_FLOW_1,direction=IFLOW
            - flow    : filter://type=IPV6,proto=UDP,label=RECIRC_32BIT_FLOW_1,direction=RFLOW

