# Feature Test Modules List
feature:
    id          : networking
    sub         : l2uc
    package     : test.networking
    module      : networking
    enable      : True
    ignore      : False

modules:
    - module:
        name    : IPV4_QTAG
        spec    : l2uc.testspec
        perf    : True
        iterate :
            - flow    : filter://type=IPV4,proto=UDP,label=NETWORKING
              segment : filter://native=False
            - flow    : filter://type=IPV4,proto=TCP,label=NETWORKING
              segment : filter://native=False
            - flow    : filter://type=IPV4,proto=ICMP,label=NETWORKING
              segment : filter://native=False

    - module:
        name    : IPV6_QTAG
        spec    : l2uc.testspec
        perf    : True
        iterate :
            - flow    : filter://type=IPV6,proto=UDP,label=NETWORKING
              segment : filter://native=False
            - flow    : filter://type=IPV6,proto=TCP,label=NETWORKING
              segment : filter://native=False
            - flow    : filter://type=IPV6,proto=ICMPV6,label=NETWORKING
              segment : filter://native=False

    - module:
        name    : IPV4_UTAG
        spec    : l2uc.testspec
        iterate :
            - flow    : filter://type=IPV4,proto=UDP,label=NETWORKING
              segment : filter://native=True
            - flow    : filter://type=IPV4,proto=TCP,label=NETWORKING
              segment : filter://native=True
            - flow    : filter://type=IPV4,proto=ICMP,label=NETWORKING
              segment : filter://native=True

    - module:
        name    : IPV6_UTAG
        spec    : l2uc.testspec
        iterate :
            - flow    : filter://type=IPV6,proto=UDP,label=NETWORKING
              segment : filter://native=True
            - flow    : filter://type=IPV6,proto=TCP,label=NETWORKING
              segment : filter://native=True
            - flow    : filter://type=IPV6,proto=ICMPV6,label=NETWORKING
              segment : filter://native=True

    - module:
        name    : IPV4_PTAG
        spec    : l2uc.testspec
        iterate :
            - flow    : filter://type=IPV4,proto=UDP,label=NETWORKING
              segment : filter://native=True
              priotag : True
            - flow    : filter://type=IPV4,proto=TCP,label=NETWORKING
              segment : filter://native=True
              priotag : True
            - flow    : filter://type=IPV4,proto=ICMP,label=NETWORKING
              segment : filter://native=True
              priotag : True

    - module:
        name    : IPV6_PTAG
        spec    : l2uc.testspec
        iterate :
            - flow    : filter://type=IPV6,proto=UDP,label=NETWORKING
              segment : filter://native=True
              priotag : True
            - flow    : filter://type=IPV6,proto=TCP,label=NETWORKING
              segment : filter://native=True
              priotag : True
            - flow    : filter://type=IPV6,proto=ICMPV6,label=NETWORKING
              segment : filter://native=True
              priotag : True

    - module:
        name    : IPV4_QTAG_GFT_DROP
        spec    : l2uc.testspec
        iterate :
            # Tagged Cases
            - flow    : filter://type=IPV4,proto=UDP,label=GFT_DROP
              segment : filter://native=False
            - flow    : filter://type=IPV4,proto=TCP,label=GFT_DROP
              segment : filter://native=False
            - flow    : filter://type=IPV4,proto=ICMP,label=GFT_DROP
              segment : filter://native=False

    - module:
        name    : IPV6_QTAG_GFT_DROP
        spec    : l2uc.testspec
        iterate :
            # Tagged Cases
            - flow    : filter://type=IPV6,proto=UDP,label=GFT_DROP
              segment : filter://native=False
            - flow    : filter://type=IPV6,proto=TCP,label=GFT_DROP
              segment : filter://native=False
            - flow    : filter://type=IPV6,proto=ICMPV6,label=GFT_DROP
              segment : filter://native=False

    - module:
        name    : IPV4_UTAG_GFT_DROP
        spec    : l2uc.testspec
        iterate :
            # Untagged Cases
            - flow    : filter://type=IPV4,proto=UDP,label=GFT_DROP
              segment : filter://native=True
            - flow    : filter://type=IPV4,proto=TCP,label=GFT_DROP
              segment : filter://native=True
            - flow    : filter://type=IPV4,proto=ICMP,label=GFT_DROP
              segment : filter://native=True

    - module:
        name    : IPV6_UTAG_GFT_DROP
        spec    : l2uc.testspec
        iterate :
            # Untagged Cases
            - flow    : filter://type=IPV6,proto=UDP,label=GFT_DROP
              segment : filter://native=True
            - flow    : filter://type=IPV6,proto=TCP,label=GFT_DROP
              segment : filter://native=True
            - flow    : filter://type=IPV6,proto=ICMPV6,label=GFT_DROP
              segment : filter://native=True

    - module:
        name    : IPV4_PTAG_GFT_DROP
        spec    : l2uc.testspec
        iterate :
            # Priority Tagged Cases
            - flow    : filter://type=IPV4,proto=UDP,label=GFT_DROP
              segment : filter://native=True
              priotag : True
            - flow    : filter://type=IPV4,proto=TCP,label=GFT_DROP
              segment : filter://native=True
              priotag : True
            - flow    : filter://type=IPV4,proto=ICMP,label=GFT_DROP
              segment : filter://native=True
              priotag : True

    - module:
        name    : IPV6_PTAG_GFT_DROP
        spec    : l2uc.testspec
        iterate :
            # Priority Tagged Cases
            - flow    : filter://type=IPV6,proto=UDP,label=GFT_DROP
              segment : filter://native=True
              priotag : True
            - flow    : filter://type=IPV6,proto=TCP,label=GFT_DROP
              segment : filter://native=True
              priotag : True
            - flow    : filter://type=IPV6,proto=ICMPV6,label=GFT_DROP
              segment : filter://native=True
              priotag : True

    - module:
        name    : NONIP_QTAG
        spec    : l2uc_nonip.testspec
        perf    : True
        iterate :
            # Tagged Cases
            - segment: filter://native=False
              flow   : filter://label=NETWORKING

    - module:
        name    : NONIP_UTAG
        spec    : l2uc_nonip.testspec
        iterate :
            # Untagged Cases
            - segment: filter://native=True
              flow   : filter://label=NETWORKING

    - module:
        name    : NONIP_PTAG
        spec    : l2uc_nonip.testspec
        iterate :
            # Priority Tagged Cases
            - segment: filter://native=True
              flow   : filter://label=NETWORKING
              priotag : True

    - module:
        name    : NONIP_QTAG_GFT_DROP
        spec    : l2uc_nonip.testspec
        iterate :
            # Tagged Cases
            - segment: filter://native=False
              flow   : filter://label=GFT_DROP

    - module:
        name    : NONIP_UTAG_GFT_DROP
        spec    : l2uc_nonip.testspec
        iterate :
            # Untagged Cases
            - segment: filter://native=True
              flow   : filter://label=GFT_DROP

    - module:
        name    : NONIP_PTAG_GFT_DROP
        spec    : l2uc_nonip.testspec
        iterate :
            # Priority Tagged Cases
            - segment: filter://native=True
