              segment   : filter://native=True
              flow      : filter://fwtype=L2,type=MAC,label=ARP
            - type      : RARP_RESP
              segment   : filter://native=True
              flow      : filter://fwtype=L2,type=MAC,label=ARP

    - module:
        name    : ARP_PTAG
        spec    : arp.testspec
        iterate :
            # Priority Tag Cases
            - type      : ARP_REQ_UC
              segment   : filter://native=True
              flow      : filter://fwtype=L2,type=MAC,label=ARP
              priotag   : True
            - type      : ARP_RESP
              segment   : filter://native=True
              flow      : filter://fwtype=L2,type=MAC,label=ARP
              priotag   : True

    - module:
        name    : RARP_PTAG
        spec    : arp.testspec
        iterate :
            # Priority Tag Cases
            - type      : RARP_REQ_UC
              segment   : filter://native=True
              flow      : filter://fwtype=L2,type=MAC,label=ARP
              priotag   : True
            - type      : RARP_RESP
              segment   : filter://native=True
              flow      : filter://fwtype=L2,type=MAC,label=ARP
              priotag   : True

    - module:
        name    : ARP_QTAG_GFT_DROP
        spec    : arp.testspec
        iterate :
            - type      : ARP_REQ_UC
              segment   : filter://native=False
              flow      : filter://fwtype=L2,type=MAC,label=GFT_DROP
            - type      : ARP_RESP
