    - module:
        name    : ARP_QTAG
        spec    : arp.testspec
        iterate :
            - type      : ARP_REQ_UC
              segment   : filter://native=False
              flow      : filter://fwtype=L2,type=MAC,label=ARP
            - type      : ARP_RESP
              segment   : filter://native=False
              flow      : filter://fwtype=L2,type=MAC,label=ARP

    - module:
        name    : RARP_QTAG
        spec    : arp.testspec
        iterate :
            - type      : RARP_REQ_UC
              segment   : filter://native=False
              flow      : filter://fwtype=L2,type=MAC,label=ARP
            - type      : RARP_RESP
              segment   : filter://native=False
              flow      : filter://fwtype=L2,type=MAC,label=ARP

    - module:
        name    : ARP_UTAG
        spec    : arp.testspec
        iterate :
            - type      : ARP_REQ_UC
              segment   : filter://native=True
              flow      : filter://fwtype=L2,type=MAC,label=ARP
            - type      : ARP_RESP
              segment   : filter://native=True
              flow      : filter://fwtype=L2,type=MAC,label=ARP

    - module:
        name    : RARP_UTAG
        spec    : arp.testspec
        iterate :
