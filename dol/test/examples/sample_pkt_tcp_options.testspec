# Test Spec
config_filter:
    src:
        tenant  : filter/any
        segment : filter/any
        endpoint: filter/any
    dst:
        tenant  : filter/any
        segment : filter/any
        endpoint: filter/any
    flow        : filter/proto=TCP,dport=80
    maxflows    : 1

packets:
    - packet:
        id          : BASE     # Input packet 1
        payloadsize : random/64/128
        template    : ref://factory/packets/id=ETH_IPV4_TCP
        encaps      :
            - ref://factory/packets/id=ENCAP_QTAG
        headers     :
            eth:
                src     : ref://testcase/config/src/endpoint/macaddr
                dst     : ref://testcase/config/dst/endpoint/macaddr
            qtag:
                vlan    : ref://testcase/config/src/segment/vlan_id
            ipv4:
                src     : ref://testcase/config/flow/sip
                dst     : ref://testcase/config/flow/dip
            tcp:
                sport   : ref://testcase/config/flow/sport
                dport   : ref://testcase/config/flow/dport
                options:
                    - kind: MSS
                      data: 1024

    - packet:
        id          : PKT1
        clone       : ref://testcase/packets/id=BASE
        headers     :
            tcp:
                options:
                    - kind: NOP
                      data: None
                    - kind: WScale
                      data: 4
                    - kind: SAck
                      data: 0x30001000 0x30002000 12345678 12345679
                    - kind: SAckOK
                      data: 0
                    - kind: Timestamp
                      data: 12345678 12345679

