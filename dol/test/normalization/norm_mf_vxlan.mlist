# For Usage details: Refer <ws>/dol/examples/sample.mlist.example
feature:
    id      : norm
    sub     : mfvxlan
    package : test.normalization
    module  : normalization
    spec    : norm_mf_vxlan.testspec
    enable  : True
    ignore  : False

modules:
    - module:
        name    : SMAC_ZERO
        iterate :
            - flow: filter://type=IPV4

    - module:
        name    : OUTER_SMAC_ZERO
        ignore  : True
        iterate :
            - flow: filter://type=IPV4

    - module:
        name    : DMAC_ZERO
        iterate :
            - flow: filter://type=IPV4

    - module:
        name    : OUTER_DMAC_ZERO
        ignore  : True
        iterate :
            - flow: filter://type=IPV4

    - module:
        name    : SMAC_MCAST
        iterate :
            - flow: filter://type=IPV4

    - module:
        name    : OUTER_SMAC_MCAST
        ignore  : True
        iterate :
            - flow: filter://type=IPV4

    - module:
        name    : SMAC_BCAST
        iterate :
            - flow: filter://type=IPV4

    - module:
        name    : OUTER_SMAC_BCAST
        ignore  : True
        iterate :
            - flow: filter://type=IPV4

    - module:
        name    : SIP_MCAST_V4
        iterate :
            - flow: filter://type=IPV4

    - module:
        name    : OUTER_SIP_MCAST_V4
        ignore  : True
        iterate :
            - flow: filter://type=IPV4

    - module:
        name    : SIP_MCAST_V6
        iterate :
            - flow: filter://type=IPV6

    - module:
        name    : SIP_BCAST_V4
        iterate :
            - flow: filter://type=IPV4

    - module:
        name    : OUTER_SIP_BCAST_V4
        ignore  : True
        iterate :
            - flow: filter://type=IPV4

    - module:
        name    : SIP_LOOPBACK_V4
        iterate :
            - flow: filter://type=IPV4
 
    - module:
        name    : OUTER_SIP_LOOPBACK_V4
        ignore  : True
        iterate :
            - flow: filter://type=IPV4
   
    - module:
        name    : SIP_LOOPBACK_V6
        iterate :
            - flow: filter://type=IPV6

    - module:
        name    : DIP_LOOPBACK_V4
        iterate :
            - flow: filter://type=IPV4

    - module:
        name    : OUTER_DIP_LOOPBACK_V4
        iterate :
            - flow: filter://type=IPV4

    - module:
        name    : DIP_LOOPBACK_V6
        iterate :
            - flow: filter://type=IPV6

    - module:
        name    : TTL_ZERO_V4
        iterate :
            - flow: filter://type=IPV4

    - module:
        name    : OUTER_TTL_ZERO_V4
