# Test Spec
selectors:
    src:
        tenant      : filter://type=TENANT
        segment     : filter://fabencap=VXLAN
        endpoint    : filter://remote=False
        interface   : filter://any
    dst:
        tenant      : filter://type=TENANT
        segment     : filter://fabencap=VXLAN
        endpoint    : filter://remote=True
        interface   : filter://any
    flow            : filter://fwtype=L2
    maxflows        : 1

packets:
    - packet:
        id          : BASE_PKT # Base packet
        payloadsize : ref://factory/payloads/id=PAYLOAD_ZERO64/size
        template    : callback://networking/packets/GetPacketTemplateByFlow
        encaps      : callback://networking/packets/GetPacketEncaps
        headers     :
            # Add all possible headers, the final headers will be selected
            # based on the Flow.
            eth:
                src     : ref://testcase/config/src/endpoint/macaddr
                dst     : 01:00:5E:01:01:01
            ipv4:
                src     : ref://testcase/config/flow/sip
                dst     : 239.1.1.1
                ttl     : 64
            ipv6:
                src     : ref://testcase/config/flow/sip
                dst     : ref://testcase/config/flow/dip
                hlim    : 64
            udp:
                sport   : ref://testcase/config/flow/sport
                dport   : ref://testcase/config/flow/dport
            tcp:
                sport   : ref://testcase/config/flow/sport
                dport   : ref://testcase/config/flow/dport
            icmp:
                code    : ref://testcase/config/flow/icmpcode
                type    : ref://testcase/config/flow/icmptype
                id      : ref://testcase/config/flow/icmpid
            icmpv6:
                #code    : ref://testcase/config/flow/icmpcode
                #type    : ref://testcase/config/flow/icmptype
                #id      : ref://testcase/config/flow/icmpid
            payload:
                data    : ref://factory/payloads/id=PAYLOAD_ZERO64/data

    - packet:
        id          : TX_PKT_ENIC15     # Input packet
        clone       : ref://testcase/packets/id=BASE_PKT
        headers     :
            qtag    :
                vlan: ref://testcase/config/src/segment/floodlist/oifs/id=Enic22/encap_vlan_id
    - packet:
        id          : EXP_PKT_UPLINK     # Expected packet on Uplinks
        clone       : ref://testcase/packets/id=BASE_PKT
        headers     :
            qtag:
                vlan: ref://testcase/config/src/segment/vlan_id
    - packet:
        id          : EXP_PKT_ENIC16     # Expected packet on Enics
        clone       : ref://testcase/packets/id=BASE_PKT
        headers     :
            qtag:
                vlan: ref://testcase/config/src/segment/floodlist/oifs/id=Enic23/encap_vlan_id
    - packet:
        id          : EXP_PKT_VXLAN1 # Base packet
        clone       : ref://testcase/packets/id=BASE_PKT
        encaps      : callback://networking/packets/GetExpectedPacketEncaps
        headers     :
            outereth    :
                src     : ref://testcase/config/dst/endpoint/intf/macaddr
                dst     : ref://testcase/config/dst/endpoint/intf/rmacaddr
            outeripv4   :
                src     : ref://testcase/config/dst/endpoint/intf/ltep
                dst     : ref://testcase/config/dst/endpoint/intf/rtep
                tos     : 0
                id      : 0
            qtag:
                vlan    : ref://testcase/config/dst/endpoint/intf/vlan_id
                prio    : ref://testcase/config/flow/txqos/cos
            outerudp:
                chksum  : 0
            vxlan       :
                vni     : ref://testcase/config/dst/segment/vxlan_id
            ipv4:
                tos     : ref://testcase/config/flow/txqos/dscp
            ipv6:
                tc      : ref://testcase/config/flow/txqos/dscp

buffers:
    - buffer:
        id      : TX_BUF_ENIC15 # Input Buffer
        template: ref://factory/templates/id=ETH_BUFFER
        fields:
           bind : True
           size : ref://testcase/packets/id=TX_PKT_ENIC15/size
           data : ref://testcase/packets/id=TX_PKT_ENIC15/rawbytes
    - buffer:
        id      : RX_BUF_ENIC16  # Input Buffer
        template: ref://factory/templates/id=ETH_BUFFER
        fields:
            bind: True
            size: 1518
            data: None
    - buffer:
        id      : EXP_BUF_ENIC16  # Expected Buffer
        template: ref://factory/templates/id=ETH_BUFFER
        fields:
            bind  : False
            size  : ref://testcase/packets/id=EXP_PKT_ENIC16/size
            data  : ref://testcase/packets/id=EXP_PKT_ENIC16/rawbytes

descriptors:
    - descriptor:
        id      : TX_DESC_ENIC15 # Input Descriptor
        template: ref://factory/templates/id=DESCR_ETH_TX
        fields:
            _buf  : ref://testcase/buffers/id=TX_BUF_ENIC15
            addr  : ref://testcase/buffers/id=TX_BUF_ENIC15/addr
            len   : ref://testcase/buffers/id=TX_BUF_ENIC15/size
    - descriptor:
        id      : RX_DESC_ENIC16 # Input Descriptor
        template: ref://factory/templates/id=DESCR_ETH_RX
        fields:
            _buf  : ref://testcase/buffers/id=RX_BUF_ENIC16
            addr  : ref://testcase/buffers/id=RX_BUF_ENIC16/addr
            len   : ref://testcase/buffers/id=RX_BUF_ENIC16/size
    - descriptor:
        id      : EXP_DESC_ENIC16 # Expected Descriptor
        template: ref://factory/templates/id=DESCR_ETH_RX_CQ
        fields:
            _buf          : ref://testcase/buffers/id=EXP_BUF_ENIC16
            bytes_written : ref://testcase/buffers/id=EXP_BUF_ENIC16/size

trigger:
    descriptors:
        - descriptor:
            object  : ref://testcase/descriptors/id=TX_DESC_ENIC15
            ring    : ref://testcase/config/src/endpoint/intf/lif/queue_types/id=TX/queues/id=Q0/rings/id=R0
        - descriptor:
            object  : ref://testcase/descriptors/id=RX_DESC_ENIC16
            ring    : ref://testcase/config/src/segment/floodlist/oifs/id=Enic23/lif/queue_types/id=RX/queues/id=Q0/rings/id=R0

    doorbell:
        object: ref://testcase/config/src/endpoint/intf/lif/queue_types/id=TX/queues/id=Q0/rings/id=R0/doorbell

expect:
    delay   : callback://networking/packets/GetExpectDelay

    packets:
        - packet:
            object  : ref://testcase/packets/id=EXP_PKT_VXLAN1
            port    : 1
