# Test Spec
selectors:
    src:
        tenant      : filter://overlay=VXLAN,type=TENANT
        segment     : filter://fabencap=VXLAN
        endpoint    : filter://remote=True
        interface   : filter://any
    dst:
        tenant      : filter://overlay=VXLAN,type=TENANT
        segment     : filter://any
        endpoint    : filter://remote=False
        interface   : filter://any
    flow            : filter://fwtype=L2
    maxflows        : 1

packets:
    - packet:
        id          : BASE_PKT # Base packet
        payloadsize : ref://factory/payloads/id=PAYLOAD_ZERO64/size
        template    : callback://networking/packets/GetPacketTemplateByFlow
        headers     :
            eth:
                src     : ref://testcase/config/src/endpoint/macaddr
                dst     : callback://networking/packets/GetMulticastMacFromIP
            ipv4:
                src     : ref://testcase/config/flow/sip
                dst     : callback://networking/packets/GetMulticastIP
                ttl     : 64
            ipv6:
                src     : ref://testcase/config/flow/sip
                dst     : callback://networking/packets/GetMulticastIP
                hlim    : 64
            udp:
                sport   : ref://testcase/config/flow/sport
                dport   : ref://testcase/config/flow/dport
            igmp:
                type    : 0x11 # Query
                gaddr   : 239.1.1.1
            icmp:
                code    : ref://testcase/config/flow/icmpcode
                type    : ref://testcase/config/flow/icmptype
                id      : ref://testcase/config/flow/icmpid
            icmpv6:
                code    : ref://testcase/config/flow/icmpcode
                type    : ref://testcase/config/flow/icmptype
                id      : ref://testcase/config/flow/icmpid
            payload:
                data    : ref://factory/payloads/id=PAYLOAD_ZERO64/data

    - packet:
        id          : TX_PKT # Input VxLAN Packet from uplink
        clone       : ref://testcase/packets/id=BASE_PKT
        encaps      : callback://networking/packets/GetPacketEncaps
        headers     :
            outereth    :
                src     : ref://testcase/config/src/endpoint/intf/rmacaddr
                dst     : callback://networking/packets/GetGIPoMac
            qtag:
                vlan    : ref://testcase/config/src/endpoint/intf/vlan_id
            outeripv4   :
                src     : ref://testcase/config/src/endpoint/intf/rtep
                dst     : ref://testcase/config/src/segment/gipo
                tos     : 0
                id      : 0
            vxlan       :
                vni     : ref://testcase/config/src/segment/vxlan_id
            tcp:
                flags: ack
    - packet:
        id          : EXP_PKT_ENIC24     # Expected packet on Enic24
        clone       : ref://testcase/packets/id=BASE_PKT
        encaps      : callback://networking/packets/GetExpectedPacketEncaps
        headers     :
            qtag    :
                vlan: ref://testcase/config/src/segment/floodlist/oifs/id=Enic24/encap_vlan_id
    - packet:
        id          : EXP_PKT_ENIC25     # Expected packet on Enic25
        clone       : ref://testcase/packets/id=BASE_PKT
        encaps      : callback://networking/packets/GetExpectedPacketEncaps
        headers     :
            qtag:
                vlan: ref://testcase/config/src/segment/floodlist/oifs/id=Enic25/encap_vlan_id

buffers:
    - buffer:
        id      : RX_BUF_ENIC24  # Input Buffer
        template: ref://factory/templates/id=ETH_BUFFER
        fields:
            bind: True
            size: 1518
            data: None
    - buffer:
        id      : RX_BUF_ENIC25  # Input Buffer
        template: ref://factory/templates/id=ETH_BUFFER
        fields:
            bind: True
            size: 1518
            data: None
    - buffer:
        id      : EXP_BUF_ENIC24  # Expected Buffer
        template: ref://factory/templates/id=ETH_BUFFER
        fields:
            bind  : False
            size  : ref://testcase/packets/id=EXP_PKT_ENIC24/size
            data  : ref://testcase/packets/id=EXP_PKT_ENIC24/rawbytes
    - buffer:
        id      : EXP_BUF_ENIC25  # Expected Buffer
        template: ref://factory/templates/id=ETH_BUFFER
        fields:
            bind  : False
            size  : ref://testcase/packets/id=EXP_PKT_ENIC25/size
            data  : ref://testcase/packets/id=EXP_PKT_ENIC25/rawbytes

descriptors:
    - descriptor:
        id      : RX_DESC_ENIC24 # Input Descriptor
        template: ref://factory/templates/id=DESCR_ETH_RX
        fields:
            _buf  : ref://testcase/buffers/id=RX_BUF_ENIC24
            addr  : ref://testcase/buffers/id=RX_BUF_ENIC24/addr
            len   : ref://testcase/buffers/id=RX_BUF_ENIC24/size
    - descriptor:
        id      : RX_DESC_ENIC25 # Input Descriptor
        template: ref://factory/templates/id=DESCR_ETH_RX
        fields:
            _buf  : ref://testcase/buffers/id=RX_BUF_ENIC25
            addr  : ref://testcase/buffers/id=RX_BUF_ENIC25/addr
            len   : ref://testcase/buffers/id=RX_BUF_ENIC25/size
    - descriptor:
        id      : EXP_DESC_ENIC24 # Expected Descriptor
        template: ref://factory/templates/id=DESCR_ETH_RX_CQ
        fields:
            _buf          : ref://testcase/buffers/id=EXP_BUF_ENIC24
            bytes_written : ref://testcase/buffers/id=EXP_BUF_ENIC24/size
    - descriptor:
        id      : EXP_DESC_ENIC25 # Expected Descriptor
        template: ref://factory/templates/id=DESCR_ETH_RX_CQ
        fields:
            _buf  : ref://testcase/buffers/id=EXP_BUF_ENIC25
            bytes_written : ref://testcase/buffers/id=EXP_BUF_ENIC25/size

trigger:
    descriptors:
        - descriptor:
            object  : ref://testcase/descriptors/id=RX_DESC_ENIC24
            ring    : ref://testcase/config/src/segment/floodlist/oifs/id=Enic24/lif/queue_types/id=RX/queues/id=Q0/rings/id=R0
        - descriptor:
            object  : ref://testcase/descriptors/id=RX_DESC_ENIC25
            ring    : ref://testcase/config/src/segment/floodlist/oifs/id=Enic25/lif/queue_types/id=RX/queues/id=Q0/rings/id=R0

    packets:
        - packet:
            object  : ref://testcase/packets/id=TX_PKT
            port    : ref://testcase/config/src/segment/floodlist/oifs/id=Tunnel23/ports

expect:
    delay   : callback://networking/packets/GetExpectDelay

    descriptors:
        - descriptor:
            object  : ref://testcase/descriptors/id=EXP_DESC_ENIC24
            ring    : ref://testcase/config/src/segment/floodlist/oifs/id=Enic24/lif/queue_types/id=RX/queues/id=Q0/rings/id=R1
        - descriptor:
            object  : ref://testcase/descriptors/id=EXP_DESC_ENIC25
            ring    : ref://testcase/config/src/segment/floodlist/oifs/id=Enic25/lif/queue_types/id=RX/queues/id=Q0/rings/id=R1
