# Feature Test Modules List
feature:
    id          : multicast
    package     : test.multicast
    module      : multicast
    enable      : True
    ignore      : False

modules:
    - module:
        name    : L2MC-UPLINK
        spec    : l2mc-uplink.testspec
        iterate :
            - flow: filter://type=IPV4,proto=UDP,label=NETWORKING
    - module:
        name    : L2MC-UPLINKPC
        spec    : l2mc-uplinkpc.testspec
        iterate :
            - flow: filter://type=IPV4,proto=UDP,label=NETWORKING
    - module:
        name    : L2MC-ENIC
        spec    : l2mc-enic.testspec
        iterate :
            - flow: filter://type=IPV4,proto=UDP,label=NETWORKING
