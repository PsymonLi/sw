# Feature Test Modules List
feature:
    id      : ftevxlan
    sub     : rx
    package : test.eth
    module  : eth
    enable  : True
    ignore  : False
    args    :
        maxflows: 4

modules:
    - module:
        name    : IPV4
        spec    : rx_vxlan.testspec
        iterate:
            - flow      : filter://type=IPV4,proto=UDP,label=FTE,direction=IFLOW
              segment   : filter://native=False
            - flow      : filter://type=IPV4,proto=TCP,label=FTE,direction=IFLOW
              segment   : filter://native=False
            - flow      : filter://type=IPV4,proto=ICMP,label=FTE,direction=IFLOW
              segment   : filter://native=False

    - module:
        name    : IPV6
        spec    : rx_vxlan.testspec
        iterate:
            - flow: filter://type=IPV6,proto=UDP,label=FTE,direction=IFLOW
            - flow: filter://type=IPV6,proto=TCP,label=FTE,direction=IFLOW
            - flow: filter://type=IPV6,proto=ICMPV6,label=FTE,direction=IFLOW

    - module:
         name    : ARP
         spec    : rx_vxlan_arp.testspec
         iterate :
             - type      : ARP_REQ_UC
               flow      : filter://label=ARP,type=MAC
             - type      : ARP_RESP
               flow      : filter://type=MAC,label=ARP

    - module:
         name    : RARP
         spec    : rx_vxlan_arp.testspec
         iterate :
             - type      : RARP_REQ_UC
               flow      : filter://type=MAC,label=ARP
             - type      : RARP_RESP
               flow      : filter://type=MAC,label=ARP

    - module:
         name    : NONIP
         enable  : False
         ignore  : True
         spec    : rx_vxlan_non_ip.testspec
         iterate :
             - flow: filter://label=FTE,type=MAC

    - module:
        name    : L3UC_IPV4
        spec    : rx_vxlan_l3.testspec
        iterate:
            - flow: filter://type=IPV4,proto=UDP,label=FTE,direction=IFLOW
            - flow: filter://type=IPV4,proto=TCP,label=FTE,direction=IFLOW
            - flow: filter://type=IPV4,proto=ICMP,label=FTE,direction=IFLOW

    - module:
        name    : L3UC_IPV6
        spec    : rx_vxlan_l3.testspec
        iterate:
            - flow: filter://type=IPV6,proto=UDP,label=FTE,direction=IFLOW
            - flow: filter://type=IPV6,proto=TCP,label=FTE,direction=IFLOW
            - flow: filter://type=IPV6,proto=ICMPV6,label=FTE,direction=IFLOW
