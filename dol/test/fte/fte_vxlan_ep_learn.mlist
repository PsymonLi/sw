# Feature Test Modules List
feature:
    id       : ftevxlan
    sub      : eplearn
    package  : test.fte.eplearn
    module   : ep_learn
    runorder : 0
    enable   : True
    ignore   : True
    args     :

modules:
    - module:
        name    : NDP_LOCAL_SOLICIT
        spec    : ndp_learn_local_vxlan.testspec
        iterate :
              - type     : NDP_SOLICIT
                root     : ref://store/templates/id=ENDPOINT
                endpoint : filter://remote=False
    - module:
        name    : NDP_REMOTE_SOLICIT
        spec    : ndp_learn_remote_vxlan.testspec
        iterate :
              - type     : NDP_SOLICIT
                root     : ref://store/templates/id=ENDPOINT
                endpoint : filter://remote=True
    - module:
        name    : ARP_REMOTE_REQ
        spec    : arp_learn_remote_vxlan.testspec
        iterate :
              - type     : ARP_REQ
                root     : ref://store/templates/id=ENDPOINT
                endpoint : filter://remote=True
    - module:
        name    : ARP_LOCAL_REQ
        spec    : arp_learn_local_vxlan.testspec
        iterate :
              - type     : ARP_REQ
                root     : ref://store/templates/id=ENDPOINT
                endpoint : filter://remote=False
