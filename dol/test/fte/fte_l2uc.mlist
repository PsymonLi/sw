# Feature Test Modules List
feature:
    id      : fte
    sub     : l2uc
    package : test.networking
    module  : networking
    enable  : True
    ignore  : False

modules:
    - module:
        name    : IP_QTAG
        spec    : l2uc.testspec
        iterate :
            # Tagged Cases
            - flow    : filter://type=IPV4,proto=UDP,label=FTE,direction=IFLOW
              segment : filter://native=False
            - flow    : filter://type=IPV4,proto=TCP,label=FTE,direction=IFLOW
              segment : filter://native=False
            - flow    : filter://type=IPV4,proto=ICMP,label=FTE,direction=IFLOW
              segment : filter://native=False
            - flow    : filter://type=IPV6,proto=UDP,label=FTE,direction=IFLOW
              segment : filter://native=False
            - flow    : filter://type=IPV6,proto=TCP,label=FTE,direction=IFLOW
              segment : filter://native=False
            - flow    : filter://type=IPV6,proto=ICMPV6,label=FTE,direction=IFLOW
              segment : filter://native=False

    - module:
        name    : IP_UTAG
        spec    : l2uc.testspec
        iterate :
            # Untagged Cases
            - flow    : filter://type=IPV4,proto=UDP,label=FTE,direction=IFLOW
              segment : filter://native=True
            - flow    : filter://type=IPV4,proto=TCP,label=FTE,direction=IFLOW
              segment : filter://native=True
            - flow    : filter://type=IPV4,proto=ICMP,label=FTE,direction=IFLOW
              segment : filter://native=True
            - flow    : filter://type=IPV6,proto=UDP,label=FTE,direction=IFLOW
              segment : filter://native=True
            - flow    : filter://type=IPV6,proto=TCP,label=FTE,direction=IFLOW
              segment : filter://native=True
            - flow    : filter://type=IPV6,proto=ICMPV6,label=FTE,direction=IFLOW
              segment : filter://native=True

    - module:
        name    : IP_PTAG
        spec    : l2uc.testspec
        iterate :
            # Priority Tagged Cases
            - flow    : filter://type=IPV4,proto=UDP,label=FTE,direction=IFLOW
              segment : filter://native=True
              priotag : True
            - flow    : filter://type=IPV4,proto=TCP,label=FTE,direction=IFLOW
              segment : filter://native=True
              priotag : True
            - flow    : filter://type=IPV4,proto=ICMP,label=FTE,direction=IFLOW
              segment : filter://native=True
              priotag : True
            - flow    : filter://type=IPV6,proto=UDP,label=FTE,direction=IFLOW
              segment : filter://native=True
              priotag : True
            - flow    : filter://type=IPV6,proto=TCP,label=FTE,direction=IFLOW
              segment : filter://native=True
              priotag : True
            - flow    : filter://type=IPV6,proto=ICMPV6,label=FTE,direction=IFLOW
              segment : filter://native=True
              priotag : True

    - module:
        name    : NONIP_QTAG
        spec    : l2uc_nonip.testspec
        iterate :
            # Tagged Cases
            - segment: filter://native=False
              flow   : filter://label=FTE,direction=IFLOW

    - module:
        name    : NONIP_UTAG
        spec    : l2uc_nonip.testspec
        iterate :
            # Untagged Cases
            - segment: filter://native=True
              flow   : filter://label=FTE,direction=IFLOW

    - module:
        name    : NONIP_PTAG
        spec    : l2uc_nonip.testspec
        iterate :
            # Priority Tagged Cases
            - segment: filter://native=True
              flow   : filter://label=FTE,direction=IFLOW
              priotag: True


    - module:
        name    : ARP_RARP_QTAG
        spec    : arp.testspec
        iterate :
            # Priority Tag Cases
            - type      : ARP_REQ_UC
              segment   : filter://native=False
              flow      : filter://fwtype=L2,type=MAC,label=ARP,direction=IFLOW
            - type      : ARP_RESP
              segment   : filter://native=False
              flow      : filter://fwtype=L2,type=MAC,label=ARP,direction=IFLOW
            - type      : RARP_REQ_UC
              segment   : filter://native=False
              flow      : filter://fwtype=L2,type=MAC,label=ARP,direction=IFLOW
            - type      : RARP_RESP
              segment   : filter://native=False
              flow      : filter://fwtype=L2,type=MAC,label=ARP,direction=IFLOW

    - module:
        name    : ARP_RARP_UTAG
        spec    : arp.testspec
        iterate :
            # Untagged Cases
            - type      : ARP_REQ_UC
              segment   : filter://native=True
              flow      : filter://fwtype=L2,type=MAC,label=ARP,direction=IFLOW
            - type      : ARP_RESP
              segment   : filter://native=True
              flow      : filter://fwtype=L2,type=MAC,label=ARP,direction=IFLOW
            - type      : RARP_REQ_UC
              segment   : filter://native=True
              flow      : filter://fwtype=L2,type=MAC,label=ARP,direction=IFLOW
            - type      : RARP_RESP
              segment   : filter://native=True
              flow      : filter://fwtype=L2,type=MAC,label=ARP,direction=IFLOW

    - module:
        name    : ARP_RARP_PTAG
        spec    : arp.testspec
        iterate :
            # Priority Tag Cases
            - type      : ARP_REQ_UC
              segment   : filter://native=True
              flow      : filter://fwtype=L2,type=MAC,label=ARP,direction=IFLOW
              priotag   : True
            - type      : ARP_RESP
              segment   : filter://native=True
              flow      : filter://fwtype=L2,type=MAC,label=ARP,direction=IFLOW
              priotag   : True
            - type      : RARP_REQ_UC
              segment   : filter://native=True
              flow      : filter://fwtype=L2,type=MAC,label=ARP,direction=IFLOW
              priotag   : True
            - type      : RARP_RESP
              segment   : filter://native=True
              flow      : filter://fwtype=L2,type=MAC,label=ARP,direction=IFLOW
              priotag   : True
