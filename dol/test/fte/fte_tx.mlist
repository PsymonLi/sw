# Feature Test Modules List
feature:
    id      : fte2
    sub     : tx
    package : test.eth
    module  : eth
    enable  : True
    ignore  : False
    spec    : tx.testspec
    args    :
        maxflows: 4

modules:
    - module:
        name    : IPV4_QTAG
        iterate:
            - flow      : filter://type=IPV4,proto=TCP,label=FTE,direction=IFLOW
              segment   : filter://native=False
            - flow      : filter://type=IPV4,proto=UDP,label=FTE,direction=IFLOW
              segment   : filter://native=False
            - flow      : filter://type=IPV4,proto=ICMP,label=FTE,direction=IFLOW
              segment   : filter://native=False

    - module:
        name    : IPV6_QTAG
        iterate:
            - flow      : filter://type=IPV6,proto=TCP,label=FTE,direction=IFLOW
              segment   : filter://native=False
            - flow      : filter://type=IPV6,proto=UDP,label=FTE,direction=IFLOW
              segment   : filter://native=False
            - flow      : filter://type=IPV6,proto=ICMPV6,label=FTE,direction=IFLOW
              segment   : filter://native=False

    - module:
        name    : IPV4_UTAG
        iterate:
            - flow      : filter://type=IPV4,proto=UDP,label=FTE,direction=IFLOW
              segment   : filter://native=True
            - flow      : filter://type=IPV4,proto=TCP,label=FTE,direction=IFLOW
              segment   : filter://native=True
            - flow      : filter://type=IPV4,proto=ICMP,label=FTE,direction=IFLOW
              segment   : filter://native=True

    - module:
        name    : IPV6_UTAG
        iterate:
            - flow      : filter://type=IPV6,proto=TCP,label=FTE,direction=IFLOW
              segment   : filter://native=True
            - flow      : filter://type=IPV6,proto=UDP,label=FTE,direction=IFLOW
              segment   : filter://native=True
            - flow      : filter://type=IPV6,proto=ICMPV6,label=FTE,direction=IFLOW
              segment   : filter://native=True

              
    - module:
        name    : IPV4_PTAG
        iterate:
            - flow      : filter://type=IPV4,proto=TCP,label=FTE,direction=IFLOW
              segment   : filter://native=True
              priotag   : True
            - flow      : filter://type=IPV4,proto=UDP,label=FTE,direction=IFLOW
              segment   : filter://native=True
              priotag   : True
            - flow      : filter://type=IPV4,proto=ICMP,label=FTE,direction=IFLOW
              segment   : filter://native=True
              priotag   : True

              
    - module:
        name    : IPV6_PTAG
        iterate:
            - flow      : filter://type=IPV6,proto=TCP,label=FTE,direction=IFLOW
              segment   : filter://native=True
              priotag   : True
            - flow      : filter://type=IPV6,proto=UDP,label=FTE,direction=IFLOW
              segment   : filter://native=True
              priotag   : True
            - flow      : filter://type=IPV6,proto=ICMPV6,label=FTE,direction=IFLOW
              segment   : filter://native=True
              priotag   : True

    - module:
        name    : NONIP_QTAG
        spec    : tx_nonip.testspec
        iterate:
            # Tagged Flows
            - segment: filter://native=False
              flow   : filter://label=FTE,direction=IFLOW

    - module:
        name    : NONIP_UTAG
        spec    : tx_nonip.testspec
        iterate:
            # Untagged Flows
            - segment: filter://native=True
              flow   : filter://label=FTE,direction=IFLOW

    - module:
        name    : ARP_QTAG
        spec    : tx_arp.testspec
        iterate :
            - type      : ARP_REQ_UC
              segment   : filter://native=False
              flow      : filter://direction=IFLOW
            - type      : ARP_RESP
              segment   : filter://native=False
              flow      : filter://direction=IFLOW

    - module:
        name    : RARP_QTAG
        spec    : tx_arp.testspec
        iterate :
            - type      : RARP_REQ_UC
              segment   : filter://native=False
              flow      : filter://direction=IFLOW
            - type      : RARP_RESP
              segment   : filter://native=False
              flow      : filter://direction=IFLOW

    - module:
        name    : ARP_UTAG
        spec    : tx_arp.testspec
        iterate :
            - type      : ARP_REQ_UC
              segment   : filter://native=True
              flow      : filter://direction=IFLOW
            - type      : ARP_RESP
              segment   : filter://native=True
              flow      : filter://direction=IFLOW

    - module:
        name    : RARP_UTAG
        spec    : tx_arp.testspec
        iterate :
            - type      : RARP_REQ_UC
              segment   : filter://native=True
              flow      : filter://direction=IFLOW
            - type      : RARP_RESP
              segment   : filter://native=True
              flow      : filter://direction=IFLOW
