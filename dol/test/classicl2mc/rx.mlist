# Feature Test Modules List
feature:
    id          : classicl2mc
    sub         : rx
    package     : test.classicl2mc
    module      : classic
    enable      : True
    ignore      : False

modules:
    - module:
        name    : BCAST
        enable  : False
        spec    : rx_bc.testspec
        ignore  : True
        iterate :
            - type  : BCAST
              flow  : filter://label=CLASSICL2BC

    - module:
        name    : BCAST_ACCESS
        enable  : False
        spec    : rx_bc_access.testspec
        ignore  : False
        iterate :
            - type  : BCAST
              flow  : filter://label=CLASSICL2BC

    - module:
        name    : MCAST
        enable  : True
        spec    : rx_mc.testspec
        ignore  : False
        iterate :
            - type      : MCAST
              flow      : filter://type=MAC,label=CLASSICL2MC
              segment   : filter://native=False

    - module:
        name    : MCAST_ACCESS
        enable  : True
        spec    : rx_mc_access.testspec
        ignore  : False
        iterate :
            - type  : MCAST
              flow  : filter://type=MAC,label=CLASSICL2MC
