# Feature Test Modules List
feature:
    id       : e2e_learn
    sub      : e2e_dhcp
    package  : test.e2e_learn.dhcp
    module   : dhcp_ignore
    runorder : 0
    enable   : True
    ignore   : False
    rtl      : False
    args     :

modules:
    - module:
        name    : DHCP_BASIC_REMOTE_LOCAL
        spec    : dhcp_ignore.testspec
        selectors :
              src_endpoints :
                    type : filter://remote=False
                    max  : 1
              dst_endpoints : 
                    type : filter://remote=True
                    max  : 1        
    - module:
        name    : DHCP_BASIC_REMOTE_REMOTE
        spec    : dhcp_ignore.testspec
        selectors :
              src_endpoints :
                    type : filter://remote=True
                    max  : 1
              dst_endpoints : 
                    type : filter://remote=True
                    max  : 1                                                               