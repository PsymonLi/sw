        module  : req_tx_send_only_rexmit_timeout
        spec    : req_tx_send_only_rexmit_timeout.testspec
        ignore  : False
        # Timers support in RTL is being added by ASIC team
        rtl     : False
        selectors:
            maxrdmasessions: 1

