# Feature Test Modules List
feature:
    id          : rdma
    sub         : rdma
    package     : test.rdma
    module      : rdma
    enable      : True
    ignore      : False
    selectors:
        maxrdmasessions: 8

modules:
    - module:
        name    : RDMA_TX_SEND_ONLY
        enable  : True
        package : test.rdma
        module  : req_tx_send_only
        spec    : req_tx_send_only.testspec
        ignore  : False

    - module:
