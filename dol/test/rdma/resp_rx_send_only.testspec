# Test Spec
selectors:
#   src:
#       tenant      : filter://overlay=VLAN,type=TENANT
#       segment     : filter://any
#       endpoint    : filter://remote=True
#       interface   : filter://any
#   dst:
#       tenant      : filter://overlay=VLAN,type=TENANT
#       segment     : filter://any
#       endpoint    : filter://remote=False
#       interface   : filter://any
#   flow            : filter://fwtype=L2,type=IPV4,dport=4791,proto=UDP,label=RDMA
    rdmasession     :   
        base        : filter://any
        lqp         : filter://any
        rqp         : filter://any
#    maxflows    : 1 
    maxrdmasessions     : 1 

packets:
   - packet:
        id          : TRAILER # Base packet
        payloadsize : 0
        template    : ref://factory/packets/id=TRAILER
        headers     :

   - packet:
        id          : BASE_PKT # Base packet
        payloadsize : ref://factory/payloads/id=PAYLOAD_ZERO64/size
        template    : ref://factory/packets/id=PAYLOAD
        headers     :
            payload:
                data    : ref://factory/payloads/id=PAYLOAD_ZERO64/data

   - packet:
        id          : IN_PKT    # Input packet 
        payloadsize : ref://factory/payloads/id=PAYLOAD_ZERO64/size
        template    : ref://factory/packets/id=ETH_IPV4_UDP_BTH 
        encaps      :   
            - ref://factory/packets/id=ENCAP_QTAG
        headers     :
            # Add all possible headers, the final headers will be selected
            # based on the Flow.
            eth:
                src     : ref://testcase/config/rdmasession/session/responder/ep/macaddr
                dst     : ref://testcase/config/rdmasession/session/initiator/ep/macaddr
            qtag:
                vlan    : ref://testcase/config/rdmasession/session/responder/ep/segment/vlan_id
            ipv4:
                src     : ref://testcase/config/rdmasession/session/responder/addr
                dst     : ref://testcase/config/rdmasession/session/initiator/addr
                ttl     : 64
            #ipv6:
            #    src     : ref://testcase/config/flow/sip
            #    dst     : ref://testcase/config/flow/dip
            #    hlim    : 64
            udp:
                sport   : ref://testcase/config/rdmasession/session/iflow/sport
                dport   : ref://testcase/config/rdmasession/session/iflow/dport
            bth:
                opcode  : 4 
                destqp  : ref://testcase/config/rdmasession/lqp/id
            payload:
                data    : ref://factory/payloads/id=PAYLOAD_ZERO64/data

buffers:
    - buffer:
        id      : BUF1
        template: ref://factory/templates/id=RDMA_BUFFER
        fields:
           slab    : ref://testcase/config/rdmasession/lqp/pd/ep/slabs/id=SLAB0000
           offset  : 0
           segments:
               - size    : ref://testcase/packets/id=IN_PKT/size
                 data    : None
                 skip    : ref://testcase/packets/id=TRAILER/size

    - buffer:
        id      : BUF2
        template: ref://factory/templates/id=RDMA_BUFFER
        fields:
           slab    : None
           offset  : 0
           segments:
               - size    : ref://testcase/packets/id=BASE_PKT/size
                 data    : ref://testcase/packets/id=BASE_PKT/rawbytes
                 skip    : ref://testcase/packets/id=TRAILER/size

descriptors:
    - descriptor:
        id      : DESC1
        template: ref://factory/templates/id=DESC_RDMA_RQ
        fields:
            wrid: 5
            num_sges: 1
               
            sges:
                - va  : ref://testcase/buffers/id=BUF1/address
                  len : ref://testcase/buffers/id=BUF1/size
                  l_key: ref://testcase/config/rdmasession/lqp/pd/mrs/id=MR-SLAB0000/lkey

    - descriptor:
        id      : DESC2
        template: ref://factory/templates/id=DESC_RDMA_RQ
        fields:
            buff: ref://testcase/buffers/id=BUF2
            wrid: 5
            num_sges: 1
               
            sges:
                - va  : ref://testcase/buffers/id=BUF1/address
                  len : ref://testcase/buffers/id=BUF1/size
                  l_key: ref://testcase/config/rdmasession/lqp/pd/mrs/id=MR-SLAB0000/lkey

trigger:
    descriptors:
        - descriptor:
            object  : ref://testcase/descriptors/id=DESC1
            //ring    : ref://testcase/config/rdmasession/session/initiator/ep/intf/lif/queue_types/id=RDMA_RQ/queues/id=0/rings/id=RQ
            ring    : ref://testcase/config/rdmasession/lqp/pd/ep/intf/lif/queue_types/id=RDMA_RQ/queues/id=0/rings/id=RQ

    packets:
        - packet:
            object  : ref://testcase/packets/id=IN_PKT
            port    : ref://testcase/config/rdmasession/session/responder/ep/intf/ports

expect:
    descriptors:
        - descriptor:
            object  : ref://testcase/descriptors/id=DESC2
            ring    : ref://testcase/config/rdmasession/lqp/pd/ep/intf/lif/queue_types/id=RDMA_RQ/queues/id=0/rings/id=RQ
            # Expected Buffer on this descriptor
            buffer  : ref://testcase/buffers/id=BUF2
