# Test Spec
selectors:
#   src:
#       tenant      : filter://overlay=VLAN,type=TENANT
#       segment     : filter://any
#       endpoint    : filter://remote=True
#       interface   : filter://any
#   dst:
#       tenant      : filter://overlay=VLAN,type=TENANT
#       segment     : filter://any
#       endpoint    : filter://remote=False
#       interface   : filter://any
#   flow            : filter://fwtype=L2,type=IPV4,dport=4791,proto=UDP,label=RDMA
    rdmasession     :
        lqp         : filter://svc=0
        rqp         : filter://svc=0
#    maxflows    : 1 
#   maxrdmasessions     : 1 

# This testcase tests the quiesce mode functionality.
# Step0: Send 3 atomic requests (AT1, AT2, AT3), followed by send (S1).
#        Verify that all atomic responses and ack are received.
#        Verify that RSQ is totally populated with 3 entries (AT1,AT2,AT3)
# Step1: Resend the old atomic request AT1
#        Verify that it is detected as duplicate and quiesce mode gets turned on.
#        Verify that ONLY atomic response for AT1 is received. Quiesce mode
#        requires that all duplicate requests are received in a lock-step manner.
#        Make sure that rsq_p_index_prime points to AT3.
# Step2: Resend the old atomic request AT2
#        Verify that it is detected as duplicate and quiesce mode is still on.
#        Verify that ONLY atomic response for AT2 is received.
# Step3: Send new atomic request AT4
#        Verify that RSQ is now incremented by one more. Also verify
#        that transfer is complete. But because of quiesce mode
#        being on, no response should be seen. rsq_p_index_prime should increment by one more.
# Step4: Resend the old atomic request AT3
#        Verify that it is detected as duplicate and quiesce mode is still on.
#        Verify that ONLY atomic response for AT3 is received.
# Step5: Resend atomic request AT4
#        Verify that it is detected as duplicate and it is matching rsq_p_index_prime,
#        quiesce mode should get turned off.
#        Verify that atomic response for AT4 is received.
#

packets:
   - packet:
        id          : ORIG_DATA_AT1 # Base packet
        payloadsize : 8
        template    : ref://factory/packets/id=PAYLOAD
        headers     :
            payload:
                data    : 01 02 01 02 01 02 01 02

   - packet:
        id          : ORIG_DATA_AT1_LITTLE # Base packet
        payloadsize : 8
        template    : ref://factory/packets/id=PAYLOAD
        headers     :
            payload:
                data    : 02 01 02 01 02 01 02 01

   - packet:
        id          : ADD_DATA_AT1 # Base packet
        payloadsize : 8
        template    : ref://factory/packets/id=PAYLOAD
        headers     :
            payload:
                data    : 01 01 01 01 01 01 01 01

   - packet:
        id          : FINAL_DATA_AT1 # Base packet
        payloadsize : 8
        template    : ref://factory/packets/id=PAYLOAD
        headers     :
            payload:
                data    : 02 03 02 03 02 03 02 03

   - packet:
        id          : FINAL_DATA_AT1_LITTLE # Base packet
        payloadsize : 8
        template    : ref://factory/packets/id=PAYLOAD
        headers     :
            payload:
                data    : 03 02 03 02 03 02 03 02

   - packet:
        id          : AT1_REQ_PKT    # Input packet 
        payloadsize : 0
        template    : callback://rdma/rdma/PickPktTemplate/v4=ETH_IPV4_UDP_BTH_ATOMICETH,v6=ETH_IPV6_UDP_BTH_ATOMICETH
        icrc        : True
        encaps      :   
            - ref://factory/packets/id=ENCAP_QTAG
        headers     :
            # Add all possible headers, the final headers will be selected
            # based on the Flow.
            eth:
                src     : ref://testcase/config/rdmasession/session/responder/ep/macaddr
                dst     : ref://testcase/config/rdmasession/session/initiator/ep/macaddr
            qtag:
                vlan    : ref://testcase/config/rdmasession/session/responder/ep/segment/vlan_id
            ipv4:
                src     : ref://testcase/config/rdmasession/session/responder/addr
                dst     : ref://testcase/config/rdmasession/session/initiator/addr
                ttl     : 64
            ipv6:
                src     : ref://testcase/config/rdmasession/session/responder/addr
                dst     : ref://testcase/config/rdmasession/session/initiator/addr
                hlim    : 64
            udp:
                sport   : ref://testcase/config/rdmasession/session/iflow/sport
                dport   : ref://testcase/config/rdmasession/session/iflow/dport
            bth:
                opcode  : 20    #RDMA FetchAdd
                destqp  : ref://testcase/config/rdmasession/lqp/id
                psn     : callback://rdma/rdma/GetPktExpPsn/pkt_num=0
                a       : 1
            atomiceth: 
                va      : callback://rdma/rdma/GetVAfromSLAB/slab_id=SLAB0000,offset=0
                r_key   : ref://testcase/config/rdmasession/lqp/pd/mrs/id=MR-SLAB0000/rkey
                swapdt  : callback://rdma/rdma/GetAtomicData/pktid=ADD_DATA_AT1
                #ref://testcase/packets/id=ADD_DATA/rawbytes
                cmpdt   : 0

