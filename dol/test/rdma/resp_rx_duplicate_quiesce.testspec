descriptors:
    - descriptor:
        id      : RQ_DESC
        template: ref://factory/templates/id=DESC_RDMA_RQ
        fields:
            wrid: 5
            num_sges: 1
               
            sges:
                - va  : ref://testcase/buffers/id=RQ_BUF/address
                  len : ref://testcase/buffers/id=RQ_BUF/size
                  l_key: ref://testcase/config/rdmasession/lqp/pd/mrs/id=MR-SLAB0001/lkey

    - descriptor:
        id      : EXP_RQ_DESC
        template: ref://factory/templates/id=DESC_RDMA_RQ
        fields:
            buff: ref://testcase/buffers/id=EXP_RQ_BUF
            wrid: 5
            num_sges: 1
               
            sges:
                - va  : ref://testcase/buffers/id=RQ_BUF/address
                  len : ref://testcase/buffers/id=RQ_BUF/size
                  l_key: ref://testcase/config/rdmasession/lqp/pd/mrs/id=MR-SLAB0001/lkey

    - descriptor:
        id      : EXP_CQ_DESC
        template: ref://factory/templates/id=DESC_RDMA_CQ
        fields:
            wrid: ref://testcase/descriptors/id=RQ_DESC/wrid
            qp: ref://testcase/config/rdmasession/lqp/id
            op_type: 17 #OP_TYPE_SEND_RCVD
            status : 0  #CQ_STATUS_SUCCESS
            color  : callback://rdma/rdma/GetCQExpColor
            smac: callback://rdma/rdma/GetZeroMAC

session:
    - step:
        trigger:
            # post a receive descriptor
            descriptors:
                - descriptor:
                    object  : ref://testcase/descriptors/id=RQ_DESC
                    ring    : ref://testcase/config/rdmasession/lqp/rq/rings/id=RQ

            doorbell:
                object: ref://testcase/config/rdmasession/lqp/rq/rings/id=RQ/doorbell
                fields:
                    upd : 0x3 # DB_SCHED_WR(1)

            packets:
                # send AT1
                - packet:
                    object  : ref://testcase/packets/id=AT1_REQ_PKT
                    port    : ref://testcase/config/rdmasession/session/responder/ep/intf/ports

                # send AT2
                - packet:
                    object  : ref://testcase/packets/id=AT2_REQ_PKT
                    port    : ref://testcase/config/rdmasession/session/responder/ep/intf/ports

                # send AT3
                - packet:
                    object  : ref://testcase/packets/id=AT3_REQ_PKT
                    port    : ref://testcase/config/rdmasession/session/responder/ep/intf/ports

                # send Send request
                - packet:
                    object  : ref://testcase/packets/id=SEND_REQ_PKT
                    port    : ref://testcase/config/rdmasession/session/responder/ep/intf/ports

        expect:
            descriptors:
                # expect atomic data operation and corresponding response
                - descriptor:
                    object : ref://testcase/buffers/id=EXP_BUF_AT1
                    ring   : ref://testcase/buffers/id=XFER_BUF_AT1

                - descriptor:
                    object : ref://testcase/buffers/id=EXP_BUF_AT2
                    ring   : ref://testcase/buffers/id=XFER_BUF_AT2

                - descriptor:
                    object : ref://testcase/buffers/id=EXP_BUF_AT3
                    ring   : ref://testcase/buffers/id=XFER_BUF_AT3

#                # expect receive buffer transfer and completion posting
                - descriptor:
                    object  : ref://testcase/descriptors/id=EXP_RQ_DESC
                    ring    : ref://testcase/config/rdmasession/lqp/rq/rings/id=RQ
                - descriptor:
                    object  : ref://testcase/descriptors/id=EXP_CQ_DESC
                    ring    : ref://testcase/config/rdmasession/lqp/rq_cq/rings/id=CQ

            packets:
                - packet:
                    object  : ref://testcase/packets/id=AT1_RSP_PKT
                    port    : ref://testcase/config/rdmasession/session/responder/ep/intf/ports

                - packet:
                    object  : ref://testcase/packets/id=AT2_RSP_PKT
                    port    : ref://testcase/config/rdmasession/session/responder/ep/intf/ports

                - packet:
                    object  : ref://testcase/packets/id=AT3_RSP_PKT
                    port    : ref://testcase/config/rdmasession/session/responder/ep/intf/ports

                - packet:
                    object  : ref://testcase/packets/id=ACK_PKT
                    port    : ref://testcase/config/rdmasession/session/responder/ep/intf/ports

    - step:
        trigger:
            # now send duplicate AT1
            packets:
                - packet:
                    object  : ref://testcase/packets/id=AT1_REQ_PKT
                    port    : ref://testcase/config/rdmasession/session/responder/ep/intf/ports

        expect:
            packets:
                - packet:
                    object  : ref://testcase/packets/id=DUP_AT1_RSP_PKT
                    port    : ref://testcase/config/rdmasession/session/responder/ep/intf/ports

    - step:
        trigger:
            # now send duplicate AT2
            packets:
                - packet:
                    object  : ref://testcase/packets/id=AT2_REQ_PKT
                    port    : ref://testcase/config/rdmasession/session/responder/ep/intf/ports

        expect:
            packets:
                - packet:
                    object  : ref://testcase/packets/id=DUP_AT2_RSP_PKT
                    port    : ref://testcase/config/rdmasession/session/responder/ep/intf/ports

    - step:
        trigger:
            # now send AT4
            packets:
                - packet:
                    object  : ref://testcase/packets/id=AT4_REQ_PKT
                    port    : ref://testcase/config/rdmasession/session/responder/ep/intf/ports

        expect:
            descriptors:
                # expect atomic data operation and corresponding response
                - descriptor:
                    object : ref://testcase/buffers/id=EXP_BUF_AT4
                    ring   : ref://testcase/buffers/id=XFER_BUF_AT4
            # no packets should be seen at the end of this step.

    - step:
        trigger:
            # now send duplicate AT3
            packets:
                - packet:
                    object  : ref://testcase/packets/id=AT3_REQ_PKT
                    port    : ref://testcase/config/rdmasession/session/responder/ep/intf/ports

        expect:
            packets:
                - packet:
                    object  : ref://testcase/packets/id=DUP_AT3_RSP_PKT
                    port    : ref://testcase/config/rdmasession/session/responder/ep/intf/ports

    - step:
        trigger:
            # now send duplicate AT4
            packets:
                - packet:
                    object  : ref://testcase/packets/id=AT4_REQ_PKT
                    port    : ref://testcase/config/rdmasession/session/responder/ep/intf/ports

        expect:
            packets:
                - packet:
                    object  : ref://testcase/packets/id=AT4_RSP_PKT
                    port    : ref://testcase/config/rdmasession/session/responder/ep/intf/ports

