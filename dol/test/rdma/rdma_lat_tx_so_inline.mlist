# Feature Test Modules List
# For RDMA perf tests
feature:
    id          : rdma_lat_tx_so_inline
    sub         : rdma
    package     : test.rdma
    module      : rdma
    enable      : True
    ignore      : False
    selectors:
        maxrdmasessions: 1

