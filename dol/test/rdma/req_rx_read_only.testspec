    - step: # READ Response Receive
        trigger:
            packets:
                - packet:
                    object  : ref://testcase/packets/id=READ_RESP_ONLY
                    port    : ref://testcase/config/rdmasession/session/responder/ep/intf/ports

        expect:
            descriptors:
