# Test Spec
selectors:
    src:
        tenant      : filter://type=TENANT,label=DOS
        segment     : filter://any
        endpoint    : filter://remote=True
        interface   : filter://any
    dst:
        tenant      : filter://type=TENANT,label=DOS
        segment     : filter://any
        endpoint    : filter://remote=False,label=DOS_TO_WL
        interface   : filter://any
    flow            : filter://fwtype=L2
    maxflows        : 1

packets:
    - packet:
        id          : BASE_PKT # Base packet
        payloadsize : ref://factory/payloads/id=PAYLOAD_ZERO64/size
        template    : callback://networking/packets/GetPacketTemplateByFlow
        headers     :
            eth:
                src     : ref://testcase/config/src/endpoint/macaddr
                dst     : ref://testcase/config/dst/endpoint/macaddr
            ipv4:
                src     : ref://testcase/config/flow/sip
                dst     : ref://testcase/config/flow/dip
                ttl     : 64
            ipv6:
                src     : ref://testcase/config/flow/sip
                dst     : ref://testcase/config/flow/dip
                lim    : 64
            udp:
                sport   : ref://testcase/config/flow/sport
                dport   : 47274
            tcp:
                sport   : ref://testcase/config/flow/sport
                dport   : 47274
            icmp:
                type    : 8
                code    : ref://testcase/config/flow/icmpcode
                id      : ref://testcase/config/flow/icmpid
            icmpv6:
                type    : 128
                code    : ref://testcase/config/flow/icmpcode
                id      : ref://testcase/config/flow/icmpid
            payload:
                data    : ref://factory/payloads/id=PAYLOAD_ZERO64/data

    - packet:
        id          : PKT1 # Input Packet
        clone       : ref://testcase/packets/id=BASE_PKT
        encaps      : callback://networking/packets/GetPacketEncaps
        headers     :
            qtag:
                vlan    : callback://networking/packets/GetVlanId
            tcp:
                flags: syn

    - packet:
        id          : PKT2 # Expected Packet
        clone       : ref://testcase/packets/id=BASE_PKT
        encaps      : callback://networking/packets/GetExpectedPacketEncaps
        headers     :
            qtag:
                vlan: ref://testcase/config/dst/endpoint/intf/encap_vlan_id
                prio: callback://networking/packets/GetExpectedPacketCos
            ipv4:
                tos : ref://testcase/config/flow/txqos/dscp
            ipv6:
                tc  : ref://testcase/config/flow/txqos/dscp
            tcp:
                flags: syn

    - packet:
        id          : PKT3 # Input Packet2
        clone       : ref://testcase/packets/id=BASE_PKT
        encaps      : callback://networking/packets/GetPacketEncaps
        headers     :
            qtag:
                vlan    : callback://networking/packets/GetVlanId
            ipv4:
                src : 2.2.2.2
            ipv6:
                src : 3000::1
            tcp:
                flags: syn
                sport: 1000
            udp:
                sport: 1000
            icmp:
                id    : 30
            icmpv6:
                id    : 30

buffers:
    - buffer:
        id      : BUF1  # Input Buffer
        template: ref://factory/templates/id=ETH_BUFFER
        fields:
            bind: True
            size: 1518
            data: None

    - buffer:
        id      : BUF2  # Expected Buffer
        template: ref://factory/templates/id=ETH_BUFFER
        fields:
            bind  : False
            size  : ref://testcase/packets/id=PKT2/size
            data  : ref://testcase/packets/id=PKT2/rawbytes

    - buffer:
        id      : BUF3  # Input Buffer
        template: ref://factory/templates/id=ETH_BUFFER
        fields:
            bind: True
            size: 1518
            data: None

    - buffer:
        id      : BUF4  # Expected Buffer
        template: ref://factory/templates/id=ETH_BUFFER
        fields:
            bind  : False
            size  : ref://testcase/packets/id=PKT3/size
            #data  : ref://testcase/packets/id=PKT3/rawbytes
            data  : None

descriptors:
    - descriptor:
        id      : DESC1 # Input Descriptor
        template: ref://factory/templates/id=DESCR_ETH_RX
        fields:
            _buf  : ref://testcase/buffers/id=BUF1
            addr  : ref://testcase/buffers/id=BUF1/addr
            len   : ref://testcase/buffers/id=BUF1/size

    - descriptor:
        id      : DESC2 # Expected Descriptor
        template: ref://factory/templates/id=DESCR_ETH_RX_CQ
        fields:
            _buf          : ref://testcase/buffers/id=BUF2
            bytes_written : ref://testcase/buffers/id=BUF2/size

    - descriptor:
        id      : DESC3 # Input Descriptor
        template: ref://factory/templates/id=DESCR_ETH_RX
        fields:
            _buf  : ref://testcase/buffers/id=BUF3
            addr  : ref://testcase/buffers/id=BUF3/addr
            len   : ref://testcase/buffers/id=BUF3/size

    - descriptor:
        id      : DESC4 # Expected Descriptor
        template: ref://factory/templates/id=DESCR_ETH_RX_CQ
        fields:
            _buf          : ref://testcase/buffers/id=BUF4
            bytes_written : 0

session:
    - step:
        trigger:
            descriptors:
                - descriptor:
                    object  : ref://testcase/descriptors/id=DESC1
                    ring    : ref://testcase/config/dst/endpoint/intf/lif/queue_types/id=RX/queues/id=Q0/rings/id=R0
        
            packets:
                - packet:
                    object  : ref://testcase/packets/id=PKT1
                    port    : callback://networking/fwmode/GetRxTriggerPorts
        
        expect:
            delay   : callback://networking/packets/GetExpectDelay
        
            packets:
                - packet:
                    object  : callback://networking/packets/GetCpuPacket/expktid=PKT1
                    port    : 128
        
            descriptors:
                - descriptor:
                    object  : ref://testcase/descriptors/id=DESC2
                    ring    : ref://testcase/config/dst/endpoint/intf/lif/queue_types/id=RX/queues/id=Q0/rings/id=R1

    - step:
        trigger:
            descriptors:
                - descriptor:
                    object  : ref://testcase/descriptors/id=DESC3
                    ring    : ref://testcase/config/dst/endpoint/intf/lif/queue_types/id=RX/queues/id=Q0/rings/id=R0
        
            packets:
                - packet:
                    object  : ref://testcase/packets/id=PKT3
                    port    : callback://networking/fwmode/GetRxTriggerPorts
        
        expect:
            delay   : callback://networking/packets/GetExpectDelay

            descriptors:
                - descriptor:
                    object  : ref://testcase/descriptors/id=DESC4
                    ring    : ref://testcase/config/dst/endpoint/intf/lif/queue_types/id=RX/queues/id=Q0/rings/id=R1
