# Feature Test Modules List
feature:
    id          : dolut
    sub         : dolut
    package     : test.dolut
    module      : dolut
    enable      : True
    ignore      : False

modules:
    - module:
        name    : PKT_TO_PKT_MATCH
        spec    : sample_pkt_to_pkt.testspec

    - module:
        name    : PKT_TO_PKT_MISMATCH
        spec    : sample_pkt_to_pkt_mismatch.testspec

    - module:
        name    : PKT_TO_PKT_MISSING
        spec    : sample_pkt_to_pkt_missing.testspec

    - module:
        name    : PKT_TO_PKT_EXTRA
        spec    : sample_pkt_to_pkt_extra.testspec

    - module:
        name    : PKT_TO_PKT
        spec    : sample_pkt_to_pkt.testspec

    - module:
        name    : PKT_SESSION
        spec    : sample_pkt_to_pkt_session.testspec

    - module:
        name    : PKT_SESSION_FAIL
        spec    : sample_pkt_to_pkt_session_fail.testspec

    - module:
        name    : PKT_TCP_OPTS
        spec    : sample_pkt_tcp_options.testspec

    - module:
        name    : PKT_VXLAN
        spec    : sample_pkt_vxlan.testspec

    - module:
        name    : PKT_SLICER
        spec    : sample_pkt_slicer.testspec

    - module:
        name    : PKT_BUILDER
        spec    : sample_pkt_builder_callback.testspec

    - module:
        name    : DESCR_BUILDER
        spec    : sample_descr_builder.testspec

    - module:
        name    : PKT_VERIFY_FAILURE
        ignore  : True
        spec    : sample_pkt_to_pkt.testspec
        module  : dolut_verify_fail
