        module  : in_order
        rtl     : True
        args    :
            maxflows: 2
        iterate :
            - flow: filter://type=IPV4

    - module:
        name    : PKT_TO_SERQ_V4
        spec    : one_pkt.testspec
        module  : pkt_to_serq
        rtl     : True
        args    :
            maxflows: 1
        iterate :
