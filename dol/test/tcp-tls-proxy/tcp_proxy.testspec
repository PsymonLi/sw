# TCP Proxy RX Inorder Sample TestSpec file.
config_filter:
    # Update: Vikasd
    # Choose the config, that only selects flows matching below packet
    # templates.
    src:
        tenant      : filter://overlay=VLAN,type=TENANT
        segment : filter://any
        endpoint    : filter://remote=True
        qpair   : filter://any # Local QPair
    dst:
        tenant      : filter://overlay=VLAN,type=TENANT
        segment : filter://any
        endpoint    : filter://remote=True
        qpair   : filter://any 
    #flow        : filter://sip=64.1.0.2,dip=64.1.0.1,proto=TCP,dport=80
    flow        : filter://proto=TCP,label=TCP-PROXY
    maxflows    : 1

# Packet section defines the set of input and output packets. Input packets
# will be used in triggers and output packets will be used in expect
packets:
    - packet:
        id          : PKT1
        template    : ref://factory/packets/id=ETH_IPV4_TCP
        payloadsize : ref://factory/payloads/id=PAYLOAD_ZERO64/size
        encaps      :
            - ref://factory/packets/id=ENCAP_QTAG
        headers     :
            eth:
                src : callback://tcpcb/tcpcb/GetSMac
                dst : callback://tcpcb/tcpcb/GetDMac
            qtag:
                vlan    : callback://tcpcb/tcpcb/GetVlanId
            ipv4:
                src : callback://tcpcb/tcpcb/GetSip
                dst : callback://tcpcb/tcpcb/GetDip
            tcp:
                sport   : callback://tcpcb/tcpcb/GetSPort
                dport   : callback://tcpcb/tcpcb/GetDPort
                seq     : callback://tcpcb/tcpcb/GetSeqNum
                ack     : callback://tcpcb/tcpcb/GetAckNum
            payload:
                data: ref://factory/payloads/id=PAYLOAD_ZERO64/data 

trigger:
    packets:
        - packet:
            object  : ref://testcase/packets/id=PKT1
            port    : ref://testcase/config/src/endpoint/intf/ports

expect:
