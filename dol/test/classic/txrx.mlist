# Feature Test Modules List
feature:
    id          : classic
    sub         : txrx
    package     : test.classic
    module      : classic
    spec        : txrx.testspec
    enable      : True
    ignore      : False

modules:
    - module:
        name    : IPV4_QTAG
        iterate:
            - flow      : filter://type=IPV4,proto=UDP
              segment   : filter://native=False
            - flow      : filter://type=IPV4,proto=TCP
              segment   : filter://native=False
            - flow      : filter://type=IPV4,proto=ICMP
              segment   : filter://native=False

    - module:
        name    : IPV6_QTAG
        iterate:
            - flow      : filter://type=IPV6,proto=UDP
              segment   : filter://native=False
            - flow      : filter://type=IPV6,proto=TCP
              segment   : filter://native=False
            - flow      : filter://type=IPV6,proto=ICMPV6
              segment   : filter://native=False

    - module:
        name    : NONIP_QTAG
        spec    : txrx_nonip.testspec
        iterate:
            - segment   : filter://native=False


    - module:
        name    : IPV4_UTAG
        iterate:
            - flow      : filter://type=IPV4,proto=UDP
              segment   : filter://native=True
            - flow      : filter://type=IPV4,proto=TCP
              segment   : filter://native=True
            - flow      : filter://type=IPV4,proto=ICMP
              segment   : filter://native=True

    - module:
        name    : IPV6_UTAG
        iterate:
            - flow      : filter://type=IPV6,proto=UDP
              segment   : filter://native=True
            - flow      : filter://type=IPV6,proto=TCP
              segment   : filter://native=True
            - flow      : filter://type=IPV6,proto=ICMPV6
              segment   : filter://native=True

    - module:
        name    : NONIP_UTAG
        spec    : txrx_nonip.testspec
        iterate:
            - segment   : filter://native=True
