# Feature Test Modules List
feature:
    id          : classic
    sub         : rxstrip
    package     : test.classic
    module      : rx_vlan_strip
    spec        : rx_vlan_strip.testspec
    enable      : True
    ignore      : False
    runorder    : 100

modules:
    - module:
        name    : IPV4_QTAG
        perf    : True
        iterate:
            - flow      : filter://type=IPV4,proto=UDP,label=CLASSIC
              segment   : filter://native=False
            - flow      : filter://type=IPV4,proto=TCP,label=CLASSIC
              segment   : filter://native=False
    
    - module:
        name    : ICMP_QTAG
        iterate:
            - flow      : filter://type=IPV4,proto=ICMP,label=CLASSIC
              segment   : filter://native=False

    - module:
        name    : IPV6_QTAG
        perf    : True
        iterate:
            - flow      : filter://type=IPV6,proto=UDP,label=CLASSIC
              segment   : filter://native=False
            - flow      : filter://type=IPV6,proto=TCP,label=CLASSIC
              segment   : filter://native=False

    - module:
        name    : ICMPV6_QTAG
        iterate:
            - flow      : filter://type=IPV6,proto=ICMPV6,label=CLASSIC
              segment   : filter://native=False

    - module:
        name    : NONIP_QTAG
        spec    : rx_nonip_vlan_strip.testspec
        iterate:
            - segment   : filter://native=False
              flow      : filter://label=CLASSIC

    - module:
        name    : IPV4_UTAG
        iterate:
            - flow      : filter://type=IPV4,proto=UDP,label=CLASSIC
              segment   : filter://native=True
            - flow      : filter://type=IPV4,proto=TCP,label=CLASSIC
              segment   : filter://native=True

    - module:
        name    : ICMP_UTAG
        iterate:
            - flow      : filter://type=IPV4,proto=ICMP,label=CLASSIC
              segment   : filter://native=True

    - module:
        name    : IPV6_UTAG
        iterate:
            - flow      : filter://type=IPV6,proto=UDP,label=CLASSIC
              segment   : filter://native=True
            - flow      : filter://type=IPV6,proto=TCP,label=CLASSIC
              segment   : filter://native=True

    - module:
        name    : ICMPV6_UTAG
        iterate:
            - flow      : filter://type=IPV6,proto=ICMPV6,label=CLASSIC
              segment   : filter://native=True

    - module:
        name    : NONIP_UTAG
        spec    : rx_nonip_vlan_strip.testspec
        iterate:
            - segment   : filter://native=True
              flow      : filter://label=CLASSIC
