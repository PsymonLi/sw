# This file contains the list of DOL test modules.
modules:
    - module:
        name    : L2UC_IPV4_UDP
        enable  : True
        package : test.networking
        module  : networking
        spec    : l2uc.testspec
        args    :
            flow: filter://type=IPV4,proto=UDP
    - module:
        name    : L2UC_IPV4_TCP
        enable  : True
        package : test.networking
        module  : networking
        spec    : l2uc.testspec
        args    :
            flow: filter://type=IPV4,proto=TCP
    - module:
        name    : L2UC_IPV4_ICMP
        enable  : False
        package : test.networking
        module  : networking
        spec    : l2uc.testspec
        args    :
            flow: filter://type=IPV4,proto=ICMP
    - module:
        name    : L2UC_IPV6_UDP
        enable  : True
        package : test.networking
        module  : networking
        spec    : l2uc.testspec
        args    :
            flow: filter://type=IPV6,proto=UDP
    - module:
        name    : L2UC_IPV6_TCP
        enable  : True
        package : test.networking
        module  : networking
        spec    : l2uc.testspec
        args    :
            flow: filter://type=IPV6,proto=TCP
    - module:
        name    : L2UC_NONIP
        enable  : False
        package : test.networking
        module  : networking
        spec    : l2uc_nonip.testspec
        args    : None
    - module:
        name    : NORMALIZATION
        enable  : False
        package : test.normalization
        module  : None
        spec    : normalization.testspec
