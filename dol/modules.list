# This file contains the list of DOL test modules.
modules:
    - module:
        name    : L2UC_IPV4_UDP
        enable  : True
        package : test.networking
        module  : networking
        spec    : l2uc.testspec
        args    :
            flow: filter://type=IPV4,proto=UDP
    - module:
        name    : L2UC_IPV4_UDP_PC_TO_PC
        enable  : True
        package : test.networking
        module  : networking
        spec    : l2uc.testspec
        ignore  : True
        args    :
            flow: filter://type=IPV4,proto=UDP
            srcif: filter://type=UPLINK_PC
            dstif: filter://type=UPLINK_PC
    - module:
        name    : L2UC_IPV4_TCP
        enable  : True
        package : test.networking
        module  : networking
        spec    : l2uc.testspec
        args    :
            flow: filter://type=IPV4,proto=TCP
    - module:
        name    : L2UC_IPV4_ICMP
        enable  : True
        package : test.networking
        module  : networking
        spec    : l2uc.testspec
        ignore  : True
        args    :
            flow: filter://type=IPV4,proto=ICMP
    - module:
        name    : L2UC_IPV6_UDP
        enable  : True
        package : test.networking
        module  : networking
        spec    : l2uc.testspec
        args    :
            flow: filter://type=IPV6,proto=UDP
    - module:
        name    : L2UC_IPV6_TCP
        enable  : True
        package : test.networking
        module  : networking
        spec    : l2uc.testspec
        args    :
            flow: filter://type=IPV6,proto=TCP
    - module:
        name    : L2UC_NONIP
        enable  : True
        package : test.networking
        module  : networking
        spec    : l2uc_nonip.testspec
        ignore  : False
        args    : None
    - module:
        name    : NORMALIZATION
        enable  : False
        package : test.normalization
        module  : None
        spec    : normalization.testspec
        ignore  : True
    - module:
        name    : L3UC_IPV4_UDP
        enable  : True
        package : test.networking
        module  : networking
        spec    : l3uc.testspec
        ignore  : True
        args    :
            flow: filter://type=IPV4,proto=UDP
    - module:
        name    : L3UC_IPV6_UDP
        enable  : True
        package : test.networking
        module  : networking
        spec    : l3uc.testspec
        ignore  : True
        args    :
            flow: filter://type=IPV6,proto=UDP

