# For Usage details: Refer <ws>/dol/examples/sample.mlist.example
feature:
    id      : norm
    sub     : mfvxlan
    package : test.normalization
    module  : normalization
    spec    : norm_mf_vxlan.testspec
    enable  : True
    ignore  : False

modules:
    - module:
        name    : SMAC_ZERO
        iterate :
            - flow          : filter://type=IPV4
              drop_stats    : True
              drop_reasons  : [ drop_malformed_pkt ]

    - module:
        name    : OUTER_SMAC_ZERO
        iterate :
            - flow          : filter://type=IPV4
              drop_stats    : True
              drop_reasons  : [ drop_malformed_pkt ]

    - module:
        name    : DMAC_ZERO
        iterate :
            - flow          : filter://type=IPV4
              drop_stats    : True
              drop_reasons  : [ drop_malformed_pkt ]

    - module:
        name    : OUTER_DMAC_ZERO
        iterate :
            - flow          : filter://type=IPV4
              drop_stats    : True
              drop_reasons  : [ drop_malformed_pkt ]

    - module:
        name    : SMAC_MCAST
        iterate :
            - flow          : filter://type=IPV4
              drop_stats    : True
              drop_reasons  : [ drop_malformed_pkt ]

    - module:
