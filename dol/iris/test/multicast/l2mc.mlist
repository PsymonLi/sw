# Feature Test Modules List
feature:
    id          : multicast
    package     : test.multicast
    module      : multicast
    enable      : True
    ignore      : False

modules:
    - module:
        name    : L2MC-ENIC
        spec    : l2mc-enic.testspec
        iterate :
            - id  : L2MC-ENIC
              flow: filter://type=IPV4,proto=UDP,label=NETWORKING
            - id  : L2MCV6-ENIC
              flow: filter://type=IPV6,proto=UDP,label=NETWORKING
    - module:
        name    : L2MC-UPLINK
