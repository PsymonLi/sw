# Test Spec
selectors:
#    src:
#        tenant      : filter://overlay=VLAN,type=TENANT
#        segment     : filter://any
#        endpoint    : filter://remote=False
#        interface   : filter://any
#    dst:
#        tenant      : filter://overlay=VLAN,type=TENANT
#        segment     : filter://any
#        endpoint    : filter://remote=True
#        interface   : filter://any
#    flow            : filter://fwtype=L2,type=IPV4,dport=4791,proto=UDP,label=RDMA
    rdmasession     :
        lqp         : filter://svc=0
        rqp         : filter://svc=0
#    maxflows    : 1
#    maxrdmasessions     : 1

packets:
    - packet:
        id          : SEND_FIRST_PYLD # Base packet
        payloadsize : 1024
        template    : ref://factory/packets/id=PAYLOAD
        headers     :
            payload:
                data    : pattern-random://0/255/1

    - packet:
        id          : SEND_MID_PYLD # Base packet
        payloadsize : 1024
        template    : ref://factory/packets/id=PAYLOAD
        headers     :
            payload:
                data    : pattern-random://0/255/1

    - packet:
        id          : SEND_LAST_PYLD # Base packet
        payloadsize : 64
        template    : ref://factory/packets/id=PAYLOAD
        headers     :
            payload:
                data    : pattern-random://0/255/1

    - packet:
        id          : EXP_SEND_FIRST_PKT # Base packet
        payloadsize : ref://testcase/packets/id=SEND_FIRST_PYLD/size
        template    : callback://rdma/rdma/PickPktTemplate/v4=ETH_IPV4_UDP_BTH,v6=ETH_IPV6_UDP_BTH
        icrc        : True
        encaps      :
            - ref://factory/packets/id=ENCAP_QTAG
        headers     :
            # Add all possible headers, the final headers will be selected
            # based on the Flow.
            eth:
                src     : ref://testcase/config/rdmasession/session/initiator/ep/macaddr
                dst     : ref://testcase/config/rdmasession/session/responder/ep/macaddr
            qtag:
                vlan    : ref://testcase/config/rdmasession/session/initiator/ep/segment/vlan_id
            ipv4:
                src     : ref://testcase/config/rdmasession/session/initiator/addr
                dst     : ref://testcase/config/rdmasession/session/responder/addr
                ttl     : 64
            ipv6:
                src     : ref://testcase/config/rdmasession/session/initiator/addr
                dst     : ref://testcase/config/rdmasession/session/responder/addr
                hlim    : 64
            udp:
                sport   : ref://testcase/config/rdmasession/session/iflow/sport
                dport   : ref://testcase/config/rdmasession/session/iflow/dport
            bth:
                opcode  : 0 # RDMA SEND First
                destqp  : ref://testcase/config/rdmasession/rqp/id
                psn     : callback://rdma/rdma/GetPktTxPsn/pkt_num=3
            payload:
                data    : ref://testcase/packets/id=SEND_FIRST_PYLD/rawbytes

    - packet:
        id          : EXP_SEND_MID_PKT # Base packet
        payloadsize : ref://testcase/packets/id=SEND_MID_PYLD/size
        template    : callback://rdma/rdma/PickPktTemplate/v4=ETH_IPV4_UDP_BTH,v6=ETH_IPV6_UDP_BTH
        icrc        : True
        encaps      :
            - ref://factory/packets/id=ENCAP_QTAG
        headers     :
            # Add all possible headers, the final headers will be selected
            # based on the Flow.
            eth:
                src     : ref://testcase/config/rdmasession/session/initiator/ep/macaddr
                dst     : ref://testcase/config/rdmasession/session/responder/ep/macaddr
            qtag:
                vlan    : ref://testcase/config/rdmasession/session/initiator/ep/segment/vlan_id
            ipv4:
                src     : ref://testcase/config/rdmasession/session/initiator/addr
                dst     : ref://testcase/config/rdmasession/session/responder/addr
                ttl     : 64
            ipv6:
                src     : ref://testcase/config/rdmasession/session/initiator/addr
                dst     : ref://testcase/config/rdmasession/session/responder/addr
                hlim    : 64
            udp:
                sport   : ref://testcase/config/rdmasession/session/iflow/sport
                dport   : ref://testcase/config/rdmasession/session/iflow/dport
            bth:
                opcode  : 1 # RDMA SEND Middle
                destqp  : ref://testcase/config/rdmasession/rqp/id
                psn     : callback://rdma/rdma/GetPktTxPsn/pkt_num=4
            payload:
                data    : ref://testcase/packets/id=SEND_MID_PYLD/rawbytes

    - packet:
        id          : EXP_SEND_LAST_PKT # Base packet
        payloadsize : ref://testcase/packets/id=SEND_LAST_PYLD/size
        template    : callback://rdma/rdma/PickPktTemplate/v4=ETH_IPV4_UDP_BTH,v6=ETH_IPV6_UDP_BTH
        icrc        : True
        encaps      :
            - ref://factory/packets/id=ENCAP_QTAG
        headers     :
            # Add all possible headers, the final headers will be selected
            # based on the Flow.
            eth:
                src     : ref://testcase/config/rdmasession/session/initiator/ep/macaddr
                dst     : ref://testcase/config/rdmasession/session/responder/ep/macaddr
            qtag:
                vlan    : ref://testcase/config/rdmasession/session/initiator/ep/segment/vlan_id
            ipv4:
                src     : ref://testcase/config/rdmasession/session/initiator/addr
                dst     : ref://testcase/config/rdmasession/session/responder/addr
                ttl     : 64
            ipv6:
                src     : ref://testcase/config/rdmasession/session/initiator/addr
                dst     : ref://testcase/config/rdmasession/session/responder/addr
                hlim    : 64
            udp:
                sport   : ref://testcase/config/rdmasession/session/iflow/sport
                dport   : ref://testcase/config/rdmasession/session/iflow/dport
            bth:
                opcode  : 2 # RDMA SEND Last
                destqp  : ref://testcase/config/rdmasession/rqp/id
                psn     : callback://rdma/rdma/GetPktTxPsn/pkt_num=5
                a       : 1
            payload:
                data    : ref://testcase/packets/id=SEND_LAST_PYLD/rawbytes

    - packet:
        id          : READ_REQ # Transmit packet
        payloadsize : 0
        template    : callback://rdma/rdma/PickPktTemplate/v4=ETH_IPV4_UDP_BTH_RETH,v6=ETH_IPV6_UDP_BTH_RETH
        icrc        : True
        encaps      :
            - ref://factory/packets/id=ENCAP_QTAG
        headers     :
            # Add all possible headers, the final headers will be selected
            # based on the Flow.
            eth:
                src     : ref://testcase/config/rdmasession/session/initiator/ep/macaddr
                dst     : ref://testcase/config/rdmasession/session/responder/ep/macaddr
            qtag:
                vlan    : ref://testcase/config/rdmasession/session/initiator/ep/segment/vlan_id
            ipv4:
                src     : ref://testcase/config/rdmasession/session/initiator/addr
                dst     : ref://testcase/config/rdmasession/session/responder/addr
                ttl     : 64
            ipv6:
                src     : ref://testcase/config/rdmasession/session/initiator/addr
                dst     : ref://testcase/config/rdmasession/session/responder/addr
                hlim    : 64
            udp:
                sport   : ref://testcase/config/rdmasession/session/iflow/sport
                dport   : ref://testcase/config/rdmasession/session/iflow/dport
            bth:
                opcode  : 12 # RDMA READ Request
                destqp  : ref://testcase/config/rdmasession/rqp/id
                psn     : ref://testcase/pvtdata/sq_pre_qstate/tx_psn
            reth:
                va      : ref://testcase/pvtdata/va
                r_key   : ref://testcase/pvtdata/r_key
                dma_len : ref://testcase/pvtdata/dma_len
            payload:
                data    : None

    - packet:
         id          : READ_RESP_FIRST_PYLD 
         payloadsize : 1024
         template    : ref://factory/packets/id=PAYLOAD
         headers     :
             payload:
                 data    : pattern-random://0/255/1

    - packet:
         id          : READ_RESP_MID_PYLD 
         payloadsize : 1024
         template    : ref://factory/packets/id=PAYLOAD
         headers     :
             payload:
                 data    : pattern-random://0/255/1
 
    - packet:
         id          : READ_RESP_LAST_PYLD # Receive payload
         payloadsize : 64
         template    : ref://factory/packets/id=PAYLOAD
         headers     :
             payload:
                 data    : pattern-random://0/255/1
    
    - packet:
         id          : READ_RESP_FIRST    # Receive packet 
         payloadsize : ref://testcase/packets/id=READ_RESP_FIRST_PYLD/size
         template    : callback://rdma/rdma/PickPktTemplate/v4=ETH_IPV4_UDP_BTH_AETH,v6=ETH_IPV6_UDP_BTH_AETH
         icrc        : True
         encaps      :
             - ref://factory/packets/id=ENCAP_QTAG
         headers     :
             # Add all possible headers, the final headers will be selected
             # based on the Flow.
             eth:
                 src     : ref://testcase/config/rdmasession/session/responder/ep/macaddr
                 dst     : ref://testcase/config/rdmasession/session/initiator/ep/macaddr
             qtag:
                 vlan    : ref://testcase/config/rdmasession/session/responder/ep/segment/vlan_id
             ipv4:
                 src     : ref://testcase/config/rdmasession/session/responder/addr
                 dst     : ref://testcase/config/rdmasession/session/initiator/addr
                 ttl     : 64
             ipv6:
                 src     : ref://testcase/config/rdmasession/session/responder/addr
                 dst     : ref://testcase/config/rdmasession/session/initiator/addr
                 hlim    : 64
             udp:
                 sport   : ref://testcase/config/rdmasession/session/iflow/sport
                 dport   : ref://testcase/config/rdmasession/session/iflow/dport
             bth:
                 opcode  : 13
                 destqp  : ref://testcase/config/rdmasession/lqp/id
                 psn     : callback://rdma/rdma/GetPktTxPsn/pkt_num=0
             aeth:
                syndrome : 14 # TODO - credits
                msn      : callback://rdma/rdma/GetPktMsn/ack_num=0
             payload:
                 data    : ref://testcase/packets/id=READ_RESP_FIRST_PYLD/rawbytes
   

    - packet:
         id          : READ_RESP_MID      # Receive packet 
         payloadsize : ref://testcase/packets/id=READ_RESP_MID_PYLD/size
         template    : callback://rdma/rdma/PickPktTemplate/v4=ETH_IPV4_UDP_BTH,v6=ETH_IPV6_UDP_BTH
         icrc        : True
         encaps      :
             - ref://factory/packets/id=ENCAP_QTAG
         headers     :
             # Add all possible headers, the final headers will be selected
             # based on the Flow.
             eth:
                 src     : ref://testcase/config/rdmasession/session/responder/ep/macaddr
                 dst     : ref://testcase/config/rdmasession/session/initiator/ep/macaddr
             qtag:
                 vlan    : ref://testcase/config/rdmasession/session/responder/ep/segment/vlan_id
             ipv4:
                 src     : ref://testcase/config/rdmasession/session/responder/addr
                 dst     : ref://testcase/config/rdmasession/session/initiator/addr
                 ttl     : 64
             ipv6:
                 src     : ref://testcase/config/rdmasession/session/responder/addr
                 dst     : ref://testcase/config/rdmasession/session/initiator/addr
                 hlim    : 64
             udp:
                 sport   : ref://testcase/config/rdmasession/session/iflow/sport
                 dport   : ref://testcase/config/rdmasession/session/iflow/dport
             bth:
                 opcode  : 14
                 destqp  : ref://testcase/config/rdmasession/lqp/id
                 psn     : callback://rdma/rdma/GetPktTxPsn/pkt_num=1
             payload:
                 data    : ref://testcase/packets/id=READ_RESP_MID_PYLD/rawbytes
   
    - packet:
         id          : READ_RESP_LAST     # Receive packet 
         payloadsize : ref://testcase/packets/id=READ_RESP_LAST_PYLD/size
         template    : callback://rdma/rdma/PickPktTemplate/v4=ETH_IPV4_UDP_BTH_AETH,v6=ETH_IPV6_UDP_BTH_AETH
         icrc        : True
         encaps      :
             - ref://factory/packets/id=ENCAP_QTAG
         headers     :
             # Add all possible headers, the final headers will be selected
             # based on the Flow.
             eth:
                 src     : ref://testcase/config/rdmasession/session/responder/ep/macaddr
                 dst     : ref://testcase/config/rdmasession/session/initiator/ep/macaddr
             qtag:
                 vlan    : ref://testcase/config/rdmasession/session/responder/ep/segment/vlan_id
             ipv4:
                 src     : ref://testcase/config/rdmasession/session/responder/addr
                 dst     : ref://testcase/config/rdmasession/session/initiator/addr
                 ttl     : 64
             ipv6:
                 src     : ref://testcase/config/rdmasession/session/responder/addr
                 dst     : ref://testcase/config/rdmasession/session/initiator/addr
                 hlim    : 64
             udp:
                 sport   : ref://testcase/config/rdmasession/session/iflow/sport
                 dport   : ref://testcase/config/rdmasession/session/iflow/dport
             bth:
                 opcode  : 15
                 destqp  : ref://testcase/config/rdmasession/lqp/id
                 psn     : callback://rdma/rdma/GetPktTxPsn/pkt_num=2
             aeth:
                syndrome : 14 # TODO - credits
                msn      : callback://rdma/rdma/GetPktMsn/ack_num=0
             payload:
                 data    : ref://testcase/packets/id=READ_RESP_LAST_PYLD/rawbytes

    - packet:
        id          : ACK_PKT_BASE # Base packet
        payloadsize : 0
        template    : callback://rdma/rdma/PickPktTemplate/v4=ETH_IPV4_UDP_BTH_AETH,v6=ETH_IPV6_UDP_BTH_AETH
        icrc        : True
        headers     :
            # Add all possible headers, the final headers will be selected
            # based on the Flow.
            eth:
                src     : ref://testcase/config/rdmasession/session/responder/ep/macaddr
                dst     : ref://testcase/config/rdmasession/session/initiator/ep/macaddr
            ipv4:
                src     : ref://testcase/config/rdmasession/session/responder/addr
                dst     : ref://testcase/config/rdmasession/session/initiator/addr
                ttl     : 64
            ipv6:
                src     : ref://testcase/config/rdmasession/session/responder/addr
                dst     : ref://testcase/config/rdmasession/session/initiator/addr
                hlim    : 64
            udp:
                sport   : ref://testcase/config/rdmasession/session/iflow/sport
                dport   : ref://testcase/config/rdmasession/session/iflow/dport
            bth:
                opcode  : 17 # Acknowledge
                destqp  : ref://testcase/config/rdmasession/lqp/id
                psn     : callback://rdma/rdma/GetPktTxPsn/pkt_num=5
            aeth:
               syndrome : 14 # TODO - credits
               msn      : callback://rdma/rdma/GetPktMsn/ack_num=1
            payload:
                data    : None

    - packet:
        id          : ACK_PKT # Base packet
        encaps      : callback://rdma/rdma/GetPacketEncaps
        clone       : ref://testcase/packets/id=ACK_PKT_BASE
        headers     :
            # Add all possible headers, the final headers will be selected
            # based on the Flow.
            outereth    :
                src     : ref://testcase/config/rdmasession/rqp/pd/ep/intf/rmacaddr
                dst     : ref://testcase/config/rdmasession/rqp/pd/ep/intf/macaddr
            outeripv4   :
                src     : ref://testcase/config/rdmasession/rqp/pd/ep/intf/rtep
                dst     : ref://testcase/config/rdmasession/rqp/pd/ep/intf/ltep
                tos     : 0
                id      : 0
            vxlan       :
                vni     : ref://testcase/config/rdmasession/rqp/pd/ep/segment/vxlan_id
            qtag:
                vlan    : callback://rdma/rdma/GetPacketQtag
                prio    : 0

buffers:
    - buffer:
        id      : SEND_BUF
        template: ref://factory/templates/id=RDMA_BUFFER
        fields:
           slab    : callback://rdma/rdma/GetNewSlab
           offset  : 0
           segments:
               - size    : ref://testcase/packets/id=SEND_FIRST_PYLD/size
                 data    : ref://testcase/packets/id=SEND_FIRST_PYLD/rawbytes
                 #skip    : ref://testcase/packets/id=TRAILER/size

               - size    : ref://testcase/packets/id=SEND_MID_PYLD/size
                 data    : ref://testcase/packets/id=SEND_MID_PYLD/rawbytes
                 #skip    : ref://testcase/packets/id=TRAILER/size

               - size    : ref://testcase/packets/id=SEND_LAST_PYLD/size
                 data    : ref://testcase/packets/id=SEND_LAST_PYLD/rawbytes
                 #skip    : ref://testcase/packets/id=TRAILER/size

    - buffer:
        id      : READ_BUF
        template: ref://factory/templates/id=RDMA_BUFFER
        fields:
           slab    : callback://rdma/rdma/GetNewSlab
           offset  : 0
           segments:
               - size    : ref://testcase/packets/id=READ_RESP_FIRST_PYLD/size
                 data    : None
               - size    : ref://testcase/packets/id=READ_RESP_MID_PYLD/size
                 data    : None
               - size    : ref://testcase/packets/id=READ_RESP_LAST_PYLD/size
                 data    : None

    - buffer:
        id      : EXP_READ_BUF
        template: ref://factory/templates/id=RDMA_BUFFER
        fields:
           slab    : None
           offset  : 0
           segments:
               - size    : ref://testcase/packets/id=READ_RESP_FIRST_PYLD/size
                 data    : ref://testcase/packets/id=READ_RESP_FIRST_PYLD/rawbytes
               - size    : ref://testcase/packets/id=READ_RESP_MID_PYLD/size
                 data    : ref://testcase/packets/id=READ_RESP_MID_PYLD/rawbytes
               - size    : ref://testcase/packets/id=READ_RESP_LAST_PYLD/size
                 data    : ref://testcase/packets/id=READ_RESP_LAST_PYLD/rawbytes

descriptors:
    - descriptor:
        id      : READ_WQE
        template: ref://factory/templates/id=DESC_RDMA_SQ
        fields:
            op_type : 3  # OP_TYPE_READ
            wrid    : 0x1230
            num_sges: 1

            read    :
                va    : ref://testcase/pvtdata/va
                len   : ref://testcase/pvtdata/dma_len
                r_key : ref://testcase/pvtdata/r_key

            sges:
                - va  : ref://testcase/buffers/id=READ_BUF/address
                  len : ref://testcase/buffers/id=READ_BUF/size
                  l_key: callback://rdma/rdma/GetMrSlabLkey/id=READ_BUF

    - descriptor:
        id      : EXP_READ_WQE
        template: ref://factory/templates/id=DESC_RDMA_SQ
        fields:
            buff: ref://testcase/buffers/id=EXP_READ_BUF
            op_type : 3  # OP_TYPE_READ
            wrid    : 0x1230
            num_sges: 1

            read    :
                va    : ref://testcase/pvtdata/va
                len   : ref://testcase/pvtdata/dma_len
                r_key : ref://testcase/pvtdata/r_key

            sges:

    - descriptor:
        id      : EXP_READ_CQ_DESC
        template: ref://factory/templates/id=DESC_RDMA_CQ_SEND
        fields:
            msn:  callback://rdma/rdma/GetCQMsn/sq_wqe_num=0
            qid: ref://testcase/config/rdmasession/lqp/id
            #op_type: 3 #OP_TYPE_READ
            status : 0  #CQ_STATUS_SUCCESS
            color  : callback://rdma/rdma/GetReqRxCQExpColor/cq_wqe_num=0
            error  : 0
            type   : 2 #CQE_TYPE_SEND_MSN

    - descriptor:
        id      : SEND_DESC
        template: ref://factory/templates/id=DESC_RDMA_SQ
        fields:
            op_type : 0  # OP_TYPE_SEND
            wrid    : 6
            num_sges: 1
            fence   : 1

            send    :

            sges:
                - va  : ref://testcase/buffers/id=SEND_BUF/address
                  len : ref://testcase/buffers/id=SEND_BUF/size
                  l_key: callback://rdma/rdma/GetMrSlabLkey/id=SEND_BUF

    - descriptor:
        id      : EXP_SEND_CQ_DESC
        template: ref://factory/templates/id=DESC_RDMA_CQ_SEND
        fields:
            msn:  callback://rdma/rdma/GetCQMsn/sq_wqe_num=1
            qid: ref://testcase/config/rdmasession/lqp/id
            op_type : 0  # OP_TYPE_SEND
            status : 0  #CQ_STATUS_SUCCESS
            color  : callback://rdma/rdma/GetReqRxCQExpColor/cq_wqe_num=1
            error  : 0
            type   : 2 #CQE_TYPE_SEND_MSN


session:
    - step: # READ Request Transmit
        trigger:
            descriptors:
                - descriptor:
                    object  : ref://testcase/descriptors/id=READ_WQE
                    ring    : ref://testcase/config/rdmasession/lqp/sq/rings/id=SQ

                - descriptor:
                    object  : ref://testcase/descriptors/id=SEND_DESC
                    ring    : ref://testcase/config/rdmasession/lqp/sq/rings/id=SQ
            
            doorbell:
                object: ref://testcase/config/rdmasession/lqp/sq/rings/id=SQ/doorbell
                fields:
                    upd : 0x9 # PI from DB data and DB_SCHED_EVAL
        
        expect:
            packets:
                - packet:
                    object  : ref://testcase/packets/id=READ_REQ
                    port    : ref://testcase/config/rdmasession/session/responder/ep/intf/ports

                - packet:
                    object  : ref://testcase/packets/id=EXP_SEND_FIRST_PKT
                    port    : ref://testcase/config/rdmasession/session/responder/ep/intf/ports

                - packet:
                    object  : ref://testcase/packets/id=EXP_SEND_MID_PKT
                    port    : ref://testcase/config/rdmasession/session/responder/ep/intf/ports

                - packet:
                    object  : ref://testcase/packets/id=EXP_SEND_LAST_PKT
                    port    : ref://testcase/config/rdmasession/session/responder/ep/intf/ports


    - step: # READ Response Receive
        trigger:
            packets:
                - packet:
                    object  : ref://testcase/packets/id=READ_RESP_FIRST
                    port    : ref://testcase/config/rdmasession/session/responder/ep/intf/ports
                - packet:
                    object  : ref://testcase/packets/id=READ_RESP_MID
                    port    : ref://testcase/config/rdmasession/session/responder/ep/intf/ports
                - packet:
                    object  : ref://testcase/packets/id=READ_RESP_LAST
                    port    : ref://testcase/config/rdmasession/session/responder/ep/intf/ports


        expect:
            descriptors:
#                - descriptor:
#                    object  : ref://testcase/descriptors/id=EXP_READ_WQE
#                    ring    : ref://testcase/config/rdmasession/lqp/sq/rings/id=SQ
                - descriptor:
                    object  : ref://testcase/descriptors/id=EXP_READ_CQ_DESC
                    ring    : ref://testcase/config/rdmasession/lqp/sq_cq/rings/id=CQ

    - step: # SEND_ACK
        trigger:
            packets:
                - packet:
                    object  : ref://testcase/packets/id=ACK_PKT
                    port    : ref://testcase/config/rdmasession/session/responder/ep/intf/ports
        expect:
            descriptors:
                - descriptor:
                    object  : ref://testcase/descriptors/id=EXP_SEND_CQ_DESC
                    ring    : ref://testcase/config/rdmasession/lqp/sq_cq/rings/id=CQ
       
    #set CI for CQ ring to that of proxy CI in DB data
    - step:

        trigger:
            doorbell:
                object: ref://testcase/config/rdmasession/lqp/sq_cq/rings/id=CQ/doorbell
                fields:
                    upd : 0x4 #Set CI

        expect:


