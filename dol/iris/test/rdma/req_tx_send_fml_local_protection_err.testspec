# Test Spec
selectors:
#    src:
#        tenant      : filter://overlay=VLAN,type=TENANT
#        segment     : filter://any
#        endpoint    : filter://remote=False
#        interface   : filter://any
#    dst:
#        tenant      : filter://overlay=VLAN,type=TENANT
#        segment     : filter://any
#        endpoint    : filter://remote=True
#        interface   : filter://any
#    flow            : filter://fwtype=L2,type=IPV4,dport=4791,proto=UDP,label=RDMA
    rdmasession     :
        lqp         : filter://svc=0,tiny=False
        rqp         : filter://svc=0
#    maxflows    : 1 
#    maxrdmasessions     : 4

packets:
    - packet:
        id          : BASE_PKT1 # Base packet
        payloadsize : 1024
        template    : ref://factory/packets/id=PAYLOAD
        headers     :
            payload:
                data    : pattern-random://0/255/1

    - packet:
        id          : BASE_PKT2 # Base packet
        payloadsize : 1024
        template    : ref://factory/packets/id=PAYLOAD
        headers     :
            payload:
                data    : pattern-random://0/255/1

    - packet:
        id          : BASE_PKT3 # Base packet
        payloadsize : 940
        template    : ref://factory/packets/id=PAYLOAD
        headers     :
            payload:
                data    : pattern-random://0/255/1

    - packet:
        id          : EXP_PKT1 # Base packet
        payloadsize : ref://testcase/packets/id=BASE_PKT1/size
        template    : callback://rdma/rdma/PickPktTemplate/v4=ETH_IPV4_UDP_BTH,v6=ETH_IPV6_UDP_BTH
        icrc        : True
        encaps      :   
            - ref://factory/packets/id=ENCAP_QTAG
        headers     :
            # Add all possible headers, the final headers will be selected
            # based on the Flow.
            eth:
                src     : ref://testcase/config/rdmasession/session/initiator/ep/macaddr
                dst     : ref://testcase/config/rdmasession/session/responder/ep/macaddr
            qtag:
                vlan    : ref://testcase/config/rdmasession/session/initiator/ep/segment/vlan_id
            ipv4:
                src     : ref://testcase/config/rdmasession/session/initiator/addr
                dst     : ref://testcase/config/rdmasession/session/responder/addr
                ttl     : 64
            ipv6:
                src     : ref://testcase/config/rdmasession/session/initiator/addr
                dst     : ref://testcase/config/rdmasession/session/responder/addr
                hlim    : 64
            udp:
                sport   : ref://testcase/config/rdmasession/session/iflow/sport
                dport   : ref://testcase/config/rdmasession/session/iflow/dport
            bth:
                opcode  : 0 # RDMA SEND First
                destqp  : ref://testcase/config/rdmasession/rqp/id
                psn     : callback://rdma/rdma/GetPktTxPsn/pkt_num=0
            payload:
                data    : ref://testcase/packets/id=BASE_PKT1/rawbytes

    - packet:
        id          : EXP_PKT2 # Base packet
        payloadsize : ref://testcase/packets/id=BASE_PKT2/size
        template    : callback://rdma/rdma/PickPktTemplate/v4=ETH_IPV4_UDP_BTH,v6=ETH_IPV6_UDP_BTH
        icrc        : True
        encaps      :   
            - ref://factory/packets/id=ENCAP_QTAG
        headers     :
            # Add all possible headers, the final headers will be selected
            # based on the Flow.
            eth:
                src     : ref://testcase/config/rdmasession/session/initiator/ep/macaddr
                dst     : ref://testcase/config/rdmasession/session/responder/ep/macaddr
            qtag:
                vlan    : ref://testcase/config/rdmasession/session/initiator/ep/segment/vlan_id
            ipv4:
                src     : ref://testcase/config/rdmasession/session/initiator/addr
                dst     : ref://testcase/config/rdmasession/session/responder/addr
                ttl     : 64
            ipv6:
                src     : ref://testcase/config/rdmasession/session/initiator/addr
                dst     : ref://testcase/config/rdmasession/session/responder/addr
                hlim    : 64
            udp:
                sport   : ref://testcase/config/rdmasession/session/iflow/sport
                dport   : ref://testcase/config/rdmasession/session/iflow/dport
            bth:
                opcode  : 1 # RDMA SEND Middle
                destqp  : ref://testcase/config/rdmasession/rqp/id
                psn     : callback://rdma/rdma/GetPktTxPsn/pkt_num=1
            payload:
                data    : ref://testcase/packets/id=BASE_PKT2/rawbytes

    - packet:
        id          : EXP_PKT3 # Base packet
        payloadsize : ref://testcase/packets/id=BASE_PKT3/size
        template    : callback://rdma/rdma/PickPktTemplate/v4=ETH_IPV4_UDP_BTH,v6=ETH_IPV6_UDP_BTH
        icrc        : True
        encaps      :   
            - ref://factory/packets/id=ENCAP_QTAG
        headers     :
            # Add all possible headers, the final headers will be selected
            # based on the Flow.
            eth:
                src     : ref://testcase/config/rdmasession/session/initiator/ep/macaddr
                dst     : ref://testcase/config/rdmasession/session/responder/ep/macaddr
            qtag:
                vlan    : ref://testcase/config/rdmasession/session/initiator/ep/segment/vlan_id
            ipv4:
                src     : ref://testcase/config/rdmasession/session/initiator/addr
                dst     : ref://testcase/config/rdmasession/session/responder/addr
                ttl     : 64
            ipv6:
                src     : ref://testcase/config/rdmasession/session/initiator/addr
                dst     : ref://testcase/config/rdmasession/session/responder/addr
                hlim    : 64
            udp:
                sport   : ref://testcase/config/rdmasession/session/iflow/sport
                dport   : ref://testcase/config/rdmasession/session/iflow/dport
            bth:
                opcode  : 2 # RDMA SEND Last
                destqp  : ref://testcase/config/rdmasession/rqp/id
                psn     : callback://rdma/rdma/GetPktTxPsn/pkt_num=2
                a       : 1                
            payload:
                data    : ref://testcase/packets/id=BASE_PKT3/rawbytes

buffers:
    - buffer:
        id      : BUF1
        template: ref://factory/templates/id=RDMA_BUFFER
        fields:
           slab    : callback://rdma/rdma/GetNewSlab
           offset  : 0
           segments:
               - size    : ref://testcase/packets/id=BASE_PKT1/size
                 data    : ref://testcase/packets/id=BASE_PKT1/rawbytes

               - size    : ref://testcase/packets/id=BASE_PKT2/size
                 data    : ref://testcase/packets/id=BASE_PKT2/rawbytes

               - size    : 199
                 data    : ref://testcase/packets/id=BASE_PKT3/rawbytes

    - buffer:
        id      : BUF2
        template: ref://factory/templates/id=RDMA_BUFFER
        fields:
           slab    : callback://rdma/rdma/GetSlab/id=BUF1
           offset  : 2247 
           segments:
               - size    : 321
                 data    : ref://testcase/packets/id=BASE_PKT3/rawbytes
                 offset  : 199

    - buffer:
        id      : BUF3
        template: ref://factory/templates/id=RDMA_BUFFER
        fields:
           slab    : callback://rdma/rdma/GetNewSlab
           offset  : 0
           segments:
               - size    : 187
                 data    : ref://testcase/packets/id=BASE_PKT3/rawbytes
                 offset  : 520

    - buffer:
        id      : BUF4
        template: ref://factory/templates/id=RDMA_BUFFER
        fields:
           slab    : callback://rdma/rdma/GetSlab/id=BUF3
           offset  : 187
           segments:
               - size    : 233
                 data    : ref://testcase/packets/id=BASE_PKT3/rawbytes
                 offset  : 707

descriptors:
    - descriptor:
        id      : DESC
        template: ref://factory/templates/id=DESC_RDMA_SQ
        fields:
            op_type : 0  # OP_TYPE_SEND
            wrid    : ref://testcase/pvtdata/wrid
            num_sges: 4

            send    :
               
            sges:
                - va  : ref://testcase/buffers/id=BUF1/address
                  len : ref://testcase/buffers/id=BUF1/size
                  l_key: callback://rdma/rdma/GetMrSlabLkey/id=BUF1
                - va  : ref://testcase/buffers/id=BUF2/address
                  len : ref://testcase/buffers/id=BUF2/size
                  l_key: callback://rdma/rdma/GetMrSlabLkey/id=BUF2
                - va  : ref://testcase/buffers/id=BUF3/address
                  len : ref://testcase/buffers/id=BUF3/size
                  l_key: callback://rdma/rdma/GetMrSlabLkey/id=BUF3
                - va  : ref://testcase/buffers/id=BUF4/address
                  len : ref://testcase/buffers/id=BUF4/size
                  l_key: callback://rdma/rdma/GetMrSlabLkey/id=BUF4

    - descriptor:
        id      : EXP_CQ_DESC
        template: ref://factory/templates/id=DESC_RDMA_CQ_SEND
        fields:
            wrid   : ref://testcase/pvtdata/wrid 
            qid    : ref://testcase/config/rdmasession/lqp/id
            status : 3  # CQ_STATUS_LOCAL_PROT_ERR
            color  : callback://rdma/rdma/GetReqRxCQExpColor/cq_wqe_num=0
            error  : 1
            type   : 3 #CQE_TYPE_SEND_NPG

    - descriptor:
        id      : EXP_RQ_FLUSH_CQ_DESC
        template: ref://factory/templates/id=DESC_RDMA_CQ_RECV
        fields:
            wrid   : 0
            qid: ref://testcase/config/rdmasession/lqp/id
            status : 4  # CQ_STATUS_WQE_FLUSHED_ERR
            color  : callback://rdma/rdma/GetReqRxCQExpColor/cq_wqe_num=1
            error  : 1
            smac: callback://rdma/rdma/GetZeroMAC

session:
    - step:
        trigger:
            descriptors:
                - descriptor:
                    object  : ref://testcase/descriptors/id=DESC
                    ring    : ref://testcase/config/rdmasession/lqp/sq/rings/id=SQ

            doorbell:
                object: ref://testcase/config/rdmasession/lqp/sq/rings/id=SQ/doorbell
                fields:
                    upd : 0x9 # PI from DB data and DB_SCHED_EVAL

        expect:
            packets: # LOCAL_PROT_ERR happens with last packet.
                - packet:
                    object  : ref://testcase/packets/id=EXP_PKT1
                    port    : ref://testcase/config/rdmasession/session/responder/ep/intf/ports

                - packet:
                    object  : ref://testcase/packets/id=EXP_PKT2
                    port    : ref://testcase/config/rdmasession/session/responder/ep/intf/ports

            descriptors:
                - descriptor:
                    object  : ref://testcase/descriptors/id=EXP_CQ_DESC
                    ring    : ref://testcase/config/rdmasession/lqp/sq_cq/rings/id=CQ

                - descriptor:
                    object  : ref://testcase/descriptors/id=EXP_RQ_FLUSH_CQ_DESC
                    ring    : ref://testcase/config/rdmasession/lqp/rq_cq/rings/id=CQ

    #set CI for CQ ring to that of proxy CI in DB data
    - step:

        trigger:
            doorbell:
                object: ref://testcase/config/rdmasession/lqp/sq_cq/rings/id=CQ/doorbell
                fields:
                    upd : 0x4 #Set CI

        expect:
