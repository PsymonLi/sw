# Test Spec
selectors:
    root: ref://store/templates/id=NVME_SESSION

packets:
    - packet:
        id          : BASE_PKT # Base packet
        payloadsize : callback://nvme/nvme/GetDataSize/nlb=10
        template    : ref://factory/packets/id=PAYLOAD
        headers     :
            payload:
                data    : pattern-random://0/255/1

buffers:
    - buffer:
        id      : BUF
        template: ref://factory/templates/id=NVME_BUFFER
        fields:
           #slab is auto created
           session   : ref://testcase/config/nvmesession
           offset    : 0
           size      : ref://testcase/packets/id=BASE_PKT/size
           data      : ref://testcase/packets/id=BASE_PKT/rawbytes

descriptors:
    - descriptor:
        id      : DESC
        template: ref://factory/templates/id=DESC_NVME_SQ
        fields:
            session : ref://testcase/config/nvmesession
            opc     :   1   #WRITE
            cid     :   1
            nsid    :   ref://testcase/config/nvmesession/nsid
            psdt    :   0 # PRPs
            data    :   ref://testcase/buffers/id=BUF
            prp2_offset :  callback://nvme/nvme/GetPrp2Offset/nentries=9
            write:
                slba    :   100

trigger:
    descriptors:
        - descriptor:
            object  : ref://testcase/descriptors/id=DESC
            ring    : ref://testcase/config/nvmesession/sq/rings/id=SQ

    doorbell:
        object: ref://testcase/config/nvmesession/sq/rings/id=SQ/doorbell
        fields:
            upd : 0x9 # PI from DB data and DB_SCHED_EVAL

expect:
