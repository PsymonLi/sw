# Feature networking Modules List for Local to Remote scenario
feature:
    id          : networking
    sub         : local2remote
    package     : test.networking
    module      : networking
    enable      : True
    ignore      : False

modules:
    - module:
        name    : L3_IPV4_UDP_QTAG
        spec    : local2remote.testspec
        iterate :
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L3,TunFamily=IPV4,Label=NETWORKING

