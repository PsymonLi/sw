    - module:
        name    : MIRROR_IPV4_TCP_RSPAN_TX_RX_0
        spec    : mirror_l2l.testspec
        iterate :
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L2L,L2LType=SAME_VNIC,Label=NETWORKING
              vnic : filter://VnicEncapType=none,TxRspanMirrorCount=0,RxRspanMirrorCount=0,TxErspanMirrorCount=0,RxErspanMirrorCount=0
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L2L,L2LType=SAME_VNIC,Label=NETWORKING
              vnic : filter://VnicEncapType=dot1q,TxRspanMirrorCount=0,RxRspanMirrorCount=0,TxErspanMirrorCount=0,RxErspanMirrorCount=0
