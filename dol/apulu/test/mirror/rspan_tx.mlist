              vnic : filter://VnicEncapType=dot1q,TxRspanMirrorCount=2,TxErspanMirrorCount=0

    - module:
        name    : L2IGW_V4_TCP_3_RSPAN_SESS 
        spec    : mirror_l2igw.testspec
        iterate :
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=IGW,Label=NETWORKING
              vnic : filter://VnicEncapType=none,TxRspanMirrorCount=3,TxErspanMirrorCount=0
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=IGW,Label=NETWORKING
              vnic : filter://VnicEncapType=dot1q,TxRspanMirrorCount=3,TxErspanMirrorCount=0

    - module:
        name    : L2IGW_V4_TCP_4_RSPAN_SESS 
        spec    : mirror_l2igw.testspec
        iterate :
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=IGW,Label=NETWORKING
              vnic : filter://VnicEncapType=none,TxRspanMirrorCount=4,TxErspanMirrorCount=0
