            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=IGW,Label=NETWORKING
              vnic : filter://VnicEncapType=dot1q,TxRspanMirrorCount=1,TxErspanMirrorCount=0
              
    - module:
