feature:
    id          : mirror
    sub         : multi_mirror_rx 
    package     : test.mirror
    module      : mirror
    enable      : True
    ignore      : True 
    runorder    : 101 

modules:

    - module:
        name    : MIRROR_IPV4_TCP_MULTI_MIRROR_RX_1
        spec    : mirror_r2l.testspec
        iterate :
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L2,Label=NETWORKING
              vnic : filter://VnicEncapType=none,RxRspanMirrorCount=1,RxErspanMirrorCount=1
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L2,Label=NETWORKING
              vnic : filter://VnicEncapType=dot1q,RxRspanMirrorCount=1,RxErspanMirrorCount=1
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L3,Label=NETWORKING
              vnic : filter://VnicEncapType=none,RxRspanMirrorCount=1,RxErspanMirrorCount=1
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L3,Label=NETWORKING
              vnic : filter://VnicEncapType=dot1q,RxRspanMirrorCount=1,RxErspanMirrorCount=1
              
