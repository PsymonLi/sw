# Feature Tx Mirror Modules List
feature:
    id          : mirror
    sub         : tx_mirror
    package     : test.mirror
    module      : mirror
    enable      : True
    ignore      : False
    runorder    : 1300

modules:

    - module:
        name    : MIRROR_L2_QTAG_IPV4_TCP
        spec    : mirror_l2r.testspec
        iterate :
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L2,Label=NETWORKING
              vnic : filter://VnicEncapType=dot1q

    - module:
        name    : MIRROR_L2_IPV4_TCP 
        spec    : mirror_l2r.testspec
        iterate :
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L2,Label=NETWORKING
              vnic : filter://VnicEncapType=none

    - module:
        name    : MIRROR_L3_QTAG_IPV4_TCP
        spec    : mirror_l2r.testspec
        iterate :
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L3,Label=NETWORKING
              vnic : filter://VnicEncapType=dot1q

    - module:
        name    : MIRROR_L3_IPV4_TCP
        spec    : mirror_l2r.testspec
        iterate :
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L3,Label=NETWORKING
              vnic : filter://VnicEncapType=none
