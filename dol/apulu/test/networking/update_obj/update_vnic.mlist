feature:
    id          : networking
    sub         : update_vnic
    package     : test.networking
    module      : networking
    runorder    : 8020
    enable      : True
    ignore      : False

modules:
# specify all the filters that are used in the packet testcases that will follow
    - module:
        name    : VNIC_WORKFLOW_START
        spec    : crud_workflow.testspec
        iterate :
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L2,Label=NETWORKING
              vnic : filter://VnicEncapType=none,SourceGuard=False
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L3,Label=NETWORKING
              vnic : filter://VnicEncapType=none,SourceGuard=False
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L2,Label=NETWORKING
              vnic : filter://VnicEncapType=dot1q,SourceGuard=False
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L3,Label=NETWORKING
              vnic : filter://VnicEncapType=dot1q,SourceGuard=False
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L2,Label=NETWORKING
              vnic : filter://VnicEncapType=none,SourceGuard=True
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L3,Label=NETWORKING
              vnic : filter://VnicEncapType=none,SourceGuard=True
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L2,Label=NETWORKING
              vnic : filter://VnicEncapType=dot1q,SourceGuard=True
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L3,Label=NETWORKING
              vnic : filter://VnicEncapType=dot1q,SourceGuard=True

    - module:
        name    : UPD_VNIC
        spec    : crud_cfg_object.testspec
        args    :
            - object    : vnic
              operation : Update
        iterate :
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L2,Label=NETWORKING
              vnic : filter://VnicEncapType=none,SourceGuard=False
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L3,Label=NETWORKING
              vnic : filter://VnicEncapType=none,SourceGuard=False
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L2,Label=NETWORKING
              vnic : filter://VnicEncapType=dot1q,SourceGuard=False
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L3,Label=NETWORKING
              vnic : filter://VnicEncapType=dot1q,SourceGuard=False
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L2,Label=NETWORKING
              vnic : filter://VnicEncapType=none,SourceGuard=True
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L3,Label=NETWORKING
              vnic : filter://VnicEncapType=none,SourceGuard=True
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L2,Label=NETWORKING
              vnic : filter://VnicEncapType=dot1q,SourceGuard=True
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L3,Label=NETWORKING
              vnic : filter://VnicEncapType=dot1q,SourceGuard=True

    - module:
        name    : LOCAL2REMOTE
        mlist   : local2remote.mlist

    - module:
        name    : SOURCEGUARD
        mlist   : sourceguard.mlist

# this triggers a packet with old values which is expected to fail since hwHabitant is False
    - module:
        name    : PREPARE_ROLLBACK
        spec    : crud_cfg_object.testspec
        args    :
            - object    : vnic
              operation : PrepareRollbackUpdate
        iterate :
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L2,Label=NETWORKING
              vnic : filter://VnicEncapType=none,SourceGuard=False
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L3,Label=NETWORKING
              vnic : filter://VnicEncapType=none,SourceGuard=False
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L2,Label=NETWORKING
              vnic : filter://VnicEncapType=dot1q,SourceGuard=False
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L3,Label=NETWORKING
              vnic : filter://VnicEncapType=dot1q,SourceGuard=False
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L2,Label=NETWORKING
              vnic : filter://VnicEncapType=none,SourceGuard=True
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L3,Label=NETWORKING
              vnic : filter://VnicEncapType=none,SourceGuard=True
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L2,Label=NETWORKING
              vnic : filter://VnicEncapType=dot1q,SourceGuard=True
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L3,Label=NETWORKING
              vnic : filter://VnicEncapType=dot1q,SourceGuard=True

    - module:
        name    : COMMIT_ROLLBACK
        spec    : crud_cfg_object.testspec
        args    :
            - object    : vnic
              operation : CommitUpdate
        iterate :
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L2,Label=NETWORKING
              vnic : filter://VnicEncapType=none,SourceGuard=False
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L3,Label=NETWORKING
              vnic : filter://VnicEncapType=none,SourceGuard=False
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L2,Label=NETWORKING
              vnic : filter://VnicEncapType=dot1q,SourceGuard=False
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L3,Label=NETWORKING
              vnic : filter://VnicEncapType=dot1q,SourceGuard=False
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L2,Label=NETWORKING
              vnic : filter://VnicEncapType=none,SourceGuard=True
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L3,Label=NETWORKING
              vnic : filter://VnicEncapType=none,SourceGuard=True
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L2,Label=NETWORKING
              vnic : filter://VnicEncapType=dot1q,SourceGuard=True
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L3,Label=NETWORKING
              vnic : filter://VnicEncapType=dot1q,SourceGuard=True

    - module:
        name    : LOCAL2REMOTE
        mlist   : local2remote.mlist

    - module:
        name    : SOURCEGUARD
        mlist   : sourceguard.mlist

    - module:
        name    : VNIC_WORKFLOW_END
        spec    : crud_workflow.testspec
        iterate :
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L2,Label=NETWORKING
              vnic : filter://VnicEncapType=none,SourceGuard=False
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L3,Label=NETWORKING
              vnic : filter://VnicEncapType=none,SourceGuard=False
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L2,Label=NETWORKING
              vnic : filter://VnicEncapType=dot1q,SourceGuard=False
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L3,Label=NETWORKING
              vnic : filter://VnicEncapType=dot1q,SourceGuard=False
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L2,Label=NETWORKING
              vnic : filter://VnicEncapType=none,SourceGuard=True
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L3,Label=NETWORKING
              vnic : filter://VnicEncapType=none,SourceGuard=True
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L2,Label=NETWORKING
              vnic : filter://VnicEncapType=dot1q,SourceGuard=True
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L3,Label=NETWORKING
              vnic : filter://VnicEncapType=dot1q,SourceGuard=True

