selectors:
    root: ref://store/templates/id=NODE
    maxlimits: 1
    node      : filter://NodeType=DUT

trigger:
    configs:
        - object: ref://testcase/config/root
          method: Run
          fields: callback://common/config/GetModuleArgs

expect:
    configs:
        - object: ref://testcase/config/root
          method: ReconfigRead