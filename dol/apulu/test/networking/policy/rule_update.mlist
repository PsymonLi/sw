feature:
    id          : policy
    sub         : rule_update 
    package     : test.networking
    module      : policy
    runorder    : 8080
    enable      : True
    ignore      : True
    argslist    : rule_update.alist

modules:

    - module:
        name        : RULE_UPD_TC_START
        spec        : load_topo.testspec

    - module:
        name        : PACKET
        mlist       : packet.mlist

    - module:
        name        : RULE_UPD_TC_TEARDOWN
        spec        : teardown_topo.testspec