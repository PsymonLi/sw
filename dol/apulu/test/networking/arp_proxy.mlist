# Feature arp_proxy plugin in vpp modules list

feature:
    id          : networking
    sub         : arp_proxy
    package     : test.networking
    module      : networking
    runorder    : 15000
    enable      : True
    ignore      : True

modules:
    - module:
        name    : IPV4_ARP_PROXY
        spec    : arp_proxy.testspec
        iterate :
            - flow : filter://FlType=MAPPING,FwdMode=L2L,Label=NETWORKING
