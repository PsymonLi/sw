# Feature networking Modules List for Local to Remote scenario
feature:
    id          : networking
    sub         : local2remote
    package     : test.networking
    module      : networking
    runorder    : 100
    enable      : True
    ignore      : False

modules:
    # L2 switching based on MAC filtering
    # L2 switching within same subnet
    - module:
        name    : L2_IPV4_TCP
        spec    : local2remote.testspec
        iterate :
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L2,Label=NETWORKING
              vnic : filter://VnicEncapType=none

    - module:
        name    : L2_IPV4_TCP_QTAG
        spec    : local2remote.testspec
        iterate :
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L2,Label=NETWORKING
              vnic : filter://VnicEncapType=dot1q

    - module:
        name    : L3_IPV4_TCP
        spec    : local2remote.testspec
        iterate :
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L3,Label=NETWORKING
              vnic : filter://VnicEncapType=none

    - module:
        name    : L3_IPV4_TCP_QTAG
        spec    : local2remote.testspec
        iterate :
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L3,Label=NETWORKING
              vnic : filter://VnicEncapType=dot1q

    - module:
        name    : L2_IPV4_ICMP_ECHO_QTAG
        spec    : local2remote_icmp_echo_request.testspec
        iterate :
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L2,Label=NETWORKING

    - module:
        name    : L2_IPV4_ICMP_ECHO_REPLY_QTAG
        spec    : local2remote_icmp_echo_reply.testspec
        iterate :
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L2,Label=NETWORKING

    - module:
        name    : L3_IPV4_ICMP_ECHO_QTAG
        spec    : local2remote_icmp_echo_request.testspec
        iterate :
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L3,Label=NETWORKING

    - module:
        name    : L3_IPV4_ICMP_PORT_UNREACHABLE_QTAG
        spec    : local2remote_icmp_port_unreachable.testspec
        iterate :
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L3,Label=NETWORKING

    - module:
        name    : L3_IPV4_ICMP_ECHO_INVALID_CODE_QTAG
        spec    : local2remote_icmp_echo_invalid_code.testspec
        iterate :
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L3,Label=NETWORKING

    - module:
        name    : L3_IPV4_ICMP_ECHO_REPLY_QTAG
        spec    : local2remote_icmp_echo_reply.testspec
        iterate :
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L3,Label=NETWORKING

    - module:
        name    : L2_L3_IPV4_VRIP_ICMP
        spec    : local2vrip.testspec
        args    :
            - proto: ICMP
        iterate :
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L2,Label=NETWORKING
              vnic : filter://VnicEncapType=none
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L3,Label=NETWORKING
              vnic : filter://VnicEncapType=none

    - module:
        name    : L2_L3_IPV4_VRIP_ICMP_QTAG
        spec    : local2vrip.testspec
        args    :
            - proto: ICMP
        iterate :
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L2,Label=NETWORKING
              vnic : filter://VnicEncapType=dot1q
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L3,Label=NETWORKING
              vnic : filter://VnicEncapType=dot1q

    - module:
        name    : L2_IPV4_VRIP_TCP_QTAG
        spec    : local2vrip.testspec
        args    :
            - proto: TCP
        iterate :
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L2,Label=NETWORKING

    - module:
        name    : L3_IPV4_VRIP_TCP_QTAG
        spec    : local2vrip.testspec
        args    :
            - proto: TCP
        iterate :
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L3,Label=NETWORKING

    - module:
        name    : L2_IPV4_VRIP_UDP_QTAG
        spec    : local2vrip.testspec
        args    :
            - proto: UDP
        iterate :
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L2,Label=NETWORKING

    - module:
        name    : L3_IPV4_VRIP_UDP_QTAG
        spec    : local2vrip.testspec
        args    :
            - proto: UDP
        iterate :
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L3,Label=NETWORKING

    - module:
        name    : L3_IPV4_IGMP
        spec    : local2remote_igmp_group_query.testspec
        iterate :
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L3,Label=NETWORKING
              id   : IGMP

    - module:
        name    : L2_MCAST
        spec    : local2remote_l2mcast.testspec
        iterate :
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L3,Label=NETWORKING


