# Feature networking module list for L2R and L2L sourceguard scenario
feature:
    id          : networking
    sub         : sourceguard
    package     : test.networking
    module      : networking
    runorder    : 999
    enable      : True
    ignore      : False

modules:
    - module:
        name    : L2R_IPV4_TCP_SRCGUARD_ENABLE
        spec    : local2remote_srcguard_fail.testspec
        iterate :
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L2,Label=NETWORKING
              vnic : filter://VnicEncapType=dot1q,SourceGuard=True
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L3,Label=NETWORKING
              vnic : filter://VnicEncapType=dot1q,SourceGuard=True
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L2,Label=NETWORKING
              vnic : filter://VnicEncapType=none,SourceGuard=True
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L3,Label=NETWORKING
              vnic : filter://VnicEncapType=none,SourceGuard=True

    - module:
        name    : L2R_IPV4_TCP_SRCGUARD_DISABLE
        spec    : local2remote_srcguard_pass.testspec
        iterate :
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L2,Label=NETWORKING
              vnic : filter://VnicEncapType=dot1q,SourceGuard=False
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L3,Label=NETWORKING
              vnic : filter://VnicEncapType=dot1q,SourceGuard=False
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L2,Label=NETWORKING
              vnic : filter://VnicEncapType=none,SourceGuard=False
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L3,Label=NETWORKING
              vnic : filter://VnicEncapType=none,SourceGuard=False

    - module:
        name    : L2L_IPV4_TCP_SRCGUARD_DISABLE
        spec    : local2local_srcguard_pass.testspec
        iterate :
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L2L,L2LType=SAME_VNIC,Label=NETWORKING
              vnic : filter://VnicEncapType=dot1q,SourceGuard=False
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L2L,L2LType=SAME_VNIC,Label=NETWORKING
              vnic : filter://VnicEncapType=none,SourceGuard=False

    - module:
        name    : L2L_IPV4_TCP_SRCGUARD_ENABLE
        spec    : local2local_srcguard_fail.testspec
        iterate :
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L2L,L2LType=SAME_VNIC,Label=NETWORKING
              vnic : filter://VnicEncapType=dot1q,SourceGuard=True
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L2L,L2LType=SAME_VNIC,Label=NETWORKING
              vnic : filter://VnicEncapType=none,SourceGuard=True

