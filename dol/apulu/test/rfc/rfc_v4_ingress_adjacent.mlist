# Apulu RFC IPv4 Ingress modules list for adjacent policy
feature:
    id          : rfc
    sub         : ipv4_ingress_adjacent
    package     : test.rfc
    module      : rfc
    enable      : True
    ignore      : True

modules:

    - module:
        name    : ADJACENT_RANDOM
        spec    : rfc_ingress.testspec
        iterate :
            - policy : filter://AddrFamily=IPV4,OverlapType=adjacent
            - policy : filter://AddrFamily=IPV4,OverlapType=adjacent_pfx
            - policy : filter://AddrFamily=IPV4,OverlapType=adjacent_pfx_with_default
            - policy : filter://AddrFamily=IPV4,OverlapType=pfx_adjacent

    - module:
        name    : ADJACENT_FIRST
        spec    : rfc_ingress.testspec
        args    :
            - prefix : first
            - sport  : first
            - dport  : first
        iterate :
            - policy : filter://AddrFamily=IPV4,OverlapType=adjacent
            - policy : filter://AddrFamily=IPV4,OverlapType=adjacent_pfx
            - policy : filter://AddrFamily=IPV4,OverlapType=adjacent_pfx_with_default
            - policy : filter://AddrFamily=IPV4,OverlapType=pfx_adjacent

    - module:
        name    : ADJACENT_LAST
        spec    : rfc_ingress.testspec
        args    :
            - prefix : last
            - sport  : last
            - dport  : last
        iterate :
            - policy : filter://AddrFamily=IPV4,OverlapType=adjacent
            - policy : filter://AddrFamily=IPV4,OverlapType=adjacent_pfx
            - policy : filter://AddrFamily=IPV4,OverlapType=adjacent_pfx_with_default
            - policy : filter://AddrFamily=IPV4,OverlapType=pfx_adjacent

    - module:
        name    : ADJACENT_LEFT
        spec    : rfc_ingress.testspec
        args    :
            - prefix : left
            - sport  : left
            - dport  : left
        iterate :
            - policy : filter://AddrFamily=IPV4,OverlapType=adjacent
            - policy : filter://AddrFamily=IPV4,OverlapType=adjacent_pfx
            - policy : filter://AddrFamily=IPV4,OverlapType=adjacent_pfx_with_default
            - policy : filter://AddrFamily=IPV4,OverlapType=pfx_adjacent

    - module:
        name    : ADJACENT_RIGHT
        spec    : rfc_ingress.testspec
        args    :
            - prefix : right
            - sport  : right
            - dport  : right
        iterate :
            - policy : filter://AddrFamily=IPV4,OverlapType=adjacent
            - policy : filter://AddrFamily=IPV4,OverlapType=adjacent_pfx
            - policy : filter://AddrFamily=IPV4,OverlapType=adjacent_pfx_with_default
            - policy : filter://AddrFamily=IPV4,OverlapType=pfx_adjacent
