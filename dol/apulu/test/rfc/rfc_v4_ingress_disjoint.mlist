# Apulu RFC IPv4 Ingress modules list for disjoint policy
feature:
    id          : rfc
    sub         : ipv4_ingress_disjoint
    package     : test.rfc
    module      : rfc
    enable      : True
    ignore      : True

modules:

    - module:
        name    : SPECIFIC
        spec    : rfc_ingress.testspec
        iterate :
            - policy : filter://AddrFamily=IPV4,PolicyType=user_specific
            #- policy : filter://AddrFamily=IPV4,PolicyType=icmp_specific

    - module:
        name    : DISJOINT_RANDOM
        spec    : rfc_ingress.testspec
        iterate :
            - policy : filter://AddrFamily=IPV4,OverlapType=none

    - module:
        name    : DISJOINT_FIRST
        spec    : rfc_ingress.testspec
        args    :
            - prefix : first
            - sport  : first
            - dport  : first
        iterate :
            - policy : filter://AddrFamily=IPV4,OverlapType=none

    - module:
        name    : DISJOINT_LAST
        spec    : rfc_ingress.testspec
        args    :
            - prefix : last
            - sport  : last
            - dport  : last
        iterate :
            - policy : filter://AddrFamily=IPV4,OverlapType=none

    - module:
        name    : DISJOINT_LEFT
        spec    : rfc_ingress.testspec
        args    :
            - prefix : left
            - sport  : left
            - dport  : left
        iterate :
            - policy : filter://AddrFamily=IPV4,OverlapType=none

    - module:
        name    : DISJOINT_RIGHT
        spec    : rfc_ingress.testspec
        args    :
            - prefix : right
            - sport  : right
            - dport  : right
        iterate :
            - policy : filter://AddrFamily=IPV4,OverlapType=none
