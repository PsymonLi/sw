              ruleindex : 1
            - policy    : filter://AddrFamily=IPV4,OverlapType=adjacent_dst_tag
              ruleindex : 2
            - policy    : filter://AddrFamily=IPV4,OverlapType=adjacent_dst_tag
              ruleindex : 3
            - policy    : filter://AddrFamily=IPV4,OverlapType=adjacent_dst_tag
              ruleindex : 4

