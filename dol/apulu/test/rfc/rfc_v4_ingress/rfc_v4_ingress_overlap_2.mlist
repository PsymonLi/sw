# Apulu RFC IPv4 Ingress modules list for overlap policy
feature:
    id          : rfc
    sub         : ipv4_ingress_overlap_2
    package     : test.rfc
    module      : rfc
    runorder    : 23000
    enable      : True
    ignore      : True

modules:

    - module:
        name    : OVERLAP_RANDOM
        spec    : rfc_ingress.testspec
        iterate :
            - policy : filter://AddrFamily=IPV4,OverlapType=pfx_overlap
            - policy : filter://AddrFamily=IPV4,OverlapType=sport
            - policy : filter://AddrFamily=IPV4,OverlapType=sport_full
            - policy : filter://AddrFamily=IPV4,OverlapType=dport
            - policy : filter://AddrFamily=IPV4,OverlapType=dport_full

    - module:
        name    : OVERLAP_FIRST
        spec    : rfc_ingress.testspec
        args    :
            - prefix : first
            - sport  : first
            - dport  : first
        iterate :
            - policy : filter://AddrFamily=IPV4,OverlapType=pfx_overlap
            - policy : filter://AddrFamily=IPV4,OverlapType=sport
            - policy : filter://AddrFamily=IPV4,OverlapType=sport_full
            - policy : filter://AddrFamily=IPV4,OverlapType=dport
            - policy : filter://AddrFamily=IPV4,OverlapType=dport_full

    - module:
        name    : OVERLAP_LAST
        spec    : rfc_ingress.testspec
        args    :
            - prefix : last
            - sport  : last
            - dport  : last
        iterate :
            - policy : filter://AddrFamily=IPV4,OverlapType=pfx_overlap
            - policy : filter://AddrFamily=IPV4,OverlapType=sport
            - policy : filter://AddrFamily=IPV4,OverlapType=sport_full
            - policy : filter://AddrFamily=IPV4,OverlapType=dport
            - policy : filter://AddrFamily=IPV4,OverlapType=dport_full

    - module:
        name    : OVERLAP_LEFT
        spec    : rfc_ingress.testspec
        args    :
            - prefix : left
            - sport  : left
            - dport  : left
        iterate :
            - policy : filter://AddrFamily=IPV4,OverlapType=pfx_overlap
            - policy : filter://AddrFamily=IPV4,OverlapType=sport
            - policy : filter://AddrFamily=IPV4,OverlapType=sport_full
            - policy : filter://AddrFamily=IPV4,OverlapType=dport
            - policy : filter://AddrFamily=IPV4,OverlapType=dport_full

    - module:
        name    : OVERLAP_RIGHT
        spec    : rfc_ingress.testspec
        args    :
            - prefix : right
            - sport  : right
            - dport  : right
        iterate :
            - policy : filter://AddrFamily=IPV4,OverlapType=pfx_overlap
            - policy : filter://AddrFamily=IPV4,OverlapType=sport
            - policy : filter://AddrFamily=IPV4,OverlapType=sport_full
            - policy : filter://AddrFamily=IPV4,OverlapType=dport
            - policy : filter://AddrFamily=IPV4,OverlapType=dport_full
