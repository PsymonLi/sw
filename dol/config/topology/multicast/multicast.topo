# Topology Specification
meta:
    id: TOPO_MULTICAST

uplink:
    - entry:
        id      : Uplink1
        port    : 1
        mode    : TRUNK
        sriov   : True
        status  : UP
    - entry:
        id      : Uplink2
        port    : 2
        mode    : TRUNK
        sriov   : True
        status  : UP
    - entry:
        id      : Uplink3
        port    : 3
        mode    : PC_MEMBER
        sriov   : False
        status  : UP
    - entry:
        id      : Uplink4
        port    : 4
        mode    : PC_MEMBER
        sriov   : False
        status  : UP
    - entry:
        id      : Uplink5
        port    : 5
        mode    : TRUNK
        sriov   : True
        status  : UP
    - entry:
        id      : Uplink6
        port    : 6
        mode    : PC_MEMBER
        sriov   : False
        status  : UP
    - entry:
        id      : Uplink7
        port    : 7
        mode    : PC_MEMBER
        sriov   : False
        status  : UP
    - entry:
        id      : Uplink8
        port    : 8
        mode    : PC_MEMBER
        sriov   : False
        status  : UP
uplinkpc:
    - entry:
        id      : UplinkPc1
        port    : 3
        mode    : TRUNK
        members :
            - ref://store/objects/id=Uplink3
            - ref://store/objects/id=Uplink4

    - entry:
        id      : UplinkPc2
        port    : 6
        mode    : TRUNK
        members :
            - ref://store/objects/id=Uplink6
            - ref://store/objects/id=Uplink7
            - ref://store/objects/id=Uplink8

acls: ref://store/specs/id=ACL

security_profiles: ref://store/specs/id=SECURITY_PROFILES_ACTIVE

tenants:
    -   spec    : ref://store/specs/id=TENANT_MULTICAST
        count   : 1
        lifns   : range/101/104
    -   spec    : ref://store/specs/id=TENANT_MULTICAST_INFRA
        count   : 1
        lifns   : range/201/204
    -   spec    : ref://store/specs/id=TENANT_MULTICAST_VXLAN
        count   : 1
        lifns   : range/301/304
    -   spec    : ref://store/specs/id=TENANT_SPAN_VLAN
        count   : 1
        lifns   : range/401/401

cpu:
    - entry:
        id      : Cpu1
        lif_id  : 1003
