# Feature RFC IPV6 Modules List
feature:
    id          : rfc
    sub         : ipv6
    package     : test.rfc
    module      : rfc
    enable      : False
    ignore      : True

modules:
    - module:
        name    : IPV6_EGRESS_ALLOW_ALL
        spec    : rfc_egress.testspec
        iterate :
            - policy : filter://AddrFamily=IPV6,PolicyType=allow_all

    # testcases for policies with stateless rules only
    - module:
        name    : IPV6_EGRESS_DISJOINT_PFX
        spec    : rfc_egress.testspec
        iterate :
            - policy : filter://AddrFamily=IPV6,PolicyType=stateless,OverlapType=disjoint_pfx

    - module:
        name    : IPV6_EGRESS_DISJOINT
        spec    : rfc_egress.testspec
        iterate :
            - policy : filter://AddrFamily=IPV6,PolicyType=stateless,OverlapType=none

    - module:
        name    : IPV6_EGRESS_OVERLAP_PFX
        spec    : rfc_egress.testspec
        iterate :
            - policy : filter://AddrFamily=IPV6,PolicyType=stateless,OverlapType=pfx

    - module:
        name    : IPV6_EGRESS_OVERLAP_FULL_PFX
        spec    : rfc_egress.testspec
        iterate :
            - policy : filter://AddrFamily=IPV6,PolicyType=stateless,OverlapType=full_pfx

    - module:
        name    : IPV6_EGRESS_OVERLAP_PFX_FIRST
        spec    : rfc_egress.testspec
        args    :
            - prefix : first
        iterate :
            - policy : filter://AddrFamily=IPV6,PolicyType=stateless,OverlapType=pfx

    - module:
        name    : IPV6_EGRESS_OVERLAP_PFX_LAST
        spec    : rfc_egress.testspec
        args    :
            - prefix : last
        iterate :
            - policy : filter://AddrFamily=IPV6,PolicyType=stateless,OverlapType=pfx

    - module:
        name    : IPV6_EGRESS_ADJACENT_PFX_FIRST
        spec    : rfc_egress.testspec
        args    :
            - prefix : first
        iterate :
            - policy : filter://AddrFamily=IPV6,PolicyType=stateless,OverlapType=adjacent_pfx

    - module:
        name    : IPV6_EGRESS_ADJACENT_PFX_LAST
        spec    : rfc_egress.testspec
        args    :
            - prefix : last
        iterate :
            - policy : filter://AddrFamily=IPV6,PolicyType=stateless,OverlapType=adjacent_pfx

    - module:
        name    : IPV6_EGRESS_ADJACENT_DEFAULT_PFX_FIRST
        spec    : rfc_egress.testspec
        args    :
            - prefix : first
        iterate :
            - policy : filter://AddrFamily=IPV6,PolicyType=stateless,OverlapType=adjacent_pfx_with_default

    - module:
        name    : IPV6_EGRESS_ADJACENT_DEFAULT_PFX_LAST
        spec    : rfc_egress.testspec
        args    :
            - prefix : last
        iterate :
            - policy : filter://AddrFamily=IPV6,PolicyType=stateless,OverlapType=adjacent_pfx_with_default

    - module:
        name    : IPV6_EGRESS_CORNER_SPORT_FIRST
        spec    : rfc_egress.testspec
        args    :
            - sport : first
        iterate :
            - policy : filter://AddrFamily=IPV6,PolicyType=stateless,OverlapType=none

    - module:
        name    : IPV6_EGRESS_CORNER_SPORT_LAST
        spec    : rfc_egress.testspec
        args    :
            - sport : last
        iterate :
            - policy : filter://AddrFamily=IPV6,PolicyType=stateless,OverlapType=none

    - module:
        name    : IPV6_EGRESS_CORNER_DPORT_FIRST
        spec    : rfc_egress.testspec
        args    :
            - dport : first
        iterate :
            - policy : filter://AddrFamily=IPV6,PolicyType=stateless,OverlapType=none

    - module:
        name    : IPV6_EGRESS_SPORT_OVERLAP
        spec    : rfc_egress.testspec
        iterate :
            - policy : filter://AddrFamily=IPV6,PolicyType=stateless,OverlapType=sport

    - module:
        name    : IPV6_EGRESS_SPORT_OVERLAP_FULL
        spec    : rfc_egress.testspec
        iterate :
            - policy : filter://AddrFamily=IPV6,PolicyType=stateless,OverlapType=sport_full

    - module:
        name    : IPV6_EGRESS_DPORT_OVERLAP
        spec    : rfc_egress.testspec
        iterate :
            - policy : filter://AddrFamily=IPV6,PolicyType=stateless,OverlapType=dport

    - module:
        name    : IPV6_EGRESS_DPORT_OVERLAP_FULL
        spec    : rfc_egress.testspec
        iterate :
            - policy : filter://AddrFamily=IPV6,PolicyType=stateless,OverlapType=dport_full

    - module:
        name    : IPV6_INGRESS_ALLOW_ALL
        spec    : rfc_ingress.testspec
        iterate :
            - policy : filter://AddrFamily=IPV6,PolicyType=allow_all

    - module:
        name    : IPV6_INGRESS_DISJOINT_PFX
        spec    : rfc_ingress.testspec
        iterate :
            - policy : filter://AddrFamily=IPV6,PolicyType=stateless,OverlapType=disjoint_pfx

    - module:
        name    : IPV6_INGRESS_DISJOINT
        spec    : rfc_ingress.testspec
        iterate :
            - policy : filter://AddrFamily=IPV6,PolicyType=stateless,OverlapType=none
