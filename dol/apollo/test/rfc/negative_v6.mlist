            - dport : left
        iterate :
            - policy : filter://AddrFamily=IPV6,PolicyType=stateless,OverlapType=none

    - module:
        name    : IPV6_EGRESS_LEFT_INVALID
        spec    : rfc_egress_negative.testspec
