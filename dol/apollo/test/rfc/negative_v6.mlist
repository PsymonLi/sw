            - sport  : right
            - dport  : right
        iterate :
            - policy : filter://AddrFamily=IPV6,PolicyType=stateless,OverlapType=none

    - module:
        name    : IPV6_INGRESS_LEFT_INVALID_SPORT
        spec    : rfc_ingress_negative.testspec
