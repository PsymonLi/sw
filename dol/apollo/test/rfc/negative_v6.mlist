            - prefix : left
            - sport  : left
            - dport  : left
        iterate :
            - policy : filter://AddrFamily=IPV6,PolicyType=stateless,OverlapType=none

    - module:
        name    : IPV6_INGRESS_RIGHT_INVALID_SPORT
        spec    : rfc_ingress_negative.testspec
        args    :
            - sport : right
        iterate :
            - policy : filter://AddrFamily=IPV6,PolicyType=stateless,OverlapType=none

    - module:
        name    : IPV6_INGRESS_RIGHT_INVALID_DPORT
