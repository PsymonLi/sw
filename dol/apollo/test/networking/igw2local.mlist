# Feature Test Modules List
feature:
    id          : networking
    sub         : igw2local
    package     : test.networking
    module      : networking
    enable      : True
    ignore      : True   # TODO , Sai please fix this for msft and enable

modules:
    - module:
        name    : IPV4_UDP_ENCAP
        spec    : igw2local.testspec
        iterate :
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=IGW,Label=NETWORKING

    - module:
        name    : IPV6_UDP_ENCAP
        spec    : igw2local.testspec
        iterate :
            - flow : filter://FlType=MAPPING,AddrFamily=IPV6,FwdMode=IGW,Label=NETWORKING

