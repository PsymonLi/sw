# Feature Test Modules List
feature:
    id          : networking
    sub         : mapping
    package     : test.networking
    module      : networking
    enable      : True
    ignore      : False

modules:
    - module:
        name    : L2_IPV4_TCP_QTAG
        spec    : networking.testspec
        iterate :
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L2,Label=NETWORKING

    - module:
        name    : L2_IPV6_TCP_QTAG
        spec    : networking.testspec
        iterate :
            - flow : filter://FlType=MAPPING,AddrFamily=IPV6,FwdMode=L2,Label=NETWORKING

    - module:
        name    : L3_IPV4_TCP_QTAG
        spec    : networking.testspec
        iterate :
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L3,Label=NETWORKING

    - module:
        name    : L3_IPV6_TCP_QTAG
        spec    : networking.testspec
        iterate :
            - flow : filter://FlType=MAPPING,AddrFamily=IPV6,FwdMode=L3,Label=NETWORKING

    - module:
        name    : L2_IPV4_TCP_QTAG_INVSRCDST_CHKTRUE
        spec    : networking_srcdst_check_fail.testspec
        iterate :
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L2,Label=NETWORKING

    - module:
        name    : L2_IPV4_TCP_QTAG_INVSRCDST_CHKFALSE
        spec    : networking_srcdst_check_pass.testspec
        iterate :
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=L2,Label=NETWORKING

    - module:
        name    : L2_IPV6_TCP_QTAG_INVSRCDST_CHKTRUE
        spec    : networking_srcdst_check_fail.testspec
        iterate :
            - flow : filter://FlType=MAPPING,AddrFamily=IPV6,FwdMode=L2,Label=NETWORKING

    - module:
        name    : L2_IPV6_TCP_QTAG_INVSRCDST_CHKFALSE
        spec    : networking_srcdst_check_pass.testspec
        iterate :
            - flow : filter://FlType=MAPPING,AddrFamily=IPV6,FwdMode=L2,Label=NETWORKING
