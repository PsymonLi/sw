# Feature Test Modules List
feature:
    id          : networking
    sub         : local2igw
    package     : test.networking
    module      : networking
    enable      : True
    # keeping it in ignore mode until NAT issues are fixed
    ignore      : True

modules:
    - module:
        name    : IPV4_UDP_QTAG
        spec    : local2igw.testspec
        iterate :
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=IGW,Label=NETWORKING

    - module:
        name    : IPV6_UDP_QTAG
        spec    : local2igw.testspec
        iterate :
            - flow : filter://FlType=MAPPING,AddrFamily=IPV6,FwdMode=IGW,Label=NETWORKING

    - module:
        name    : IPV4_SNAT
        spec    : local2igw_snat.testspec
        iterate :
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=IGW,Label=NETWORKING

    - module:
        name    : IPV6_SNAT
        spec    : local2igw_snat.testspec
        iterate :
            - flow : filter://FlType=MAPPING,AddrFamily=IPV6,FwdMode=IGW,Label=NETWORKING
