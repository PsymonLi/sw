#Test Spec for Tx Mirror session in local2remote scenario
selectors:
    root: ref://store/templates/id=FLOW
    maxlimits: 2
    flow: filter://any

packets:
    - packet:
        id          : BASE_PKT # Base packet
        payloadsize : ref://factory/payloads/id=PAYLOAD_ZERO64/size
        template    : callback://networking/packets/GetPacketTemplateFromMapping/proto=TCP
        headers     :
            eth:
                src     : ref://testcase/config/localmapping/VNIC/MACAddr
                dst     : ref://testcase/config/remotemapping/MACAddr
            ipv4:
                src     : ref://testcase/config/localmapping/IP
                dst     : ref://testcase/config/remotemapping/IP
                ttl     : 64
            ipv6:
                src     : ref://testcase/config/localmapping/IP
                dst     : ref://testcase/config/remotemapping/IP
                hlim    : 64
            tcp:
                sport   : 100
                dport   : 200
            payload:
                data    : ref://factory/payloads/id=PAYLOAD_ZERO64/data

    - packet:
        id          : FROM_HOST_PKT # Input packet
        payloadsize : ref://factory/payloads/id=PAYLOAD_ZERO64/size
        clone       : ref://testcase/packets/id=BASE_PKT
        encaps      :
            - ref://factory/packets/id=ENCAP_QTAG
        headers     :
            eth:
                src     : ref://testcase/config/localmapping/VNIC/MACAddr
                dst     : ref://testcase/config/remotemapping/MACAddr
            qtag:
                vlan    : ref://testcase/config/localmapping/VNIC/VlanId

    - packet:
        id          : TO_SWITCH_PKT # Expected packet
        clone       : ref://testcase/packets/id=FROM_HOST_PKT
        encaps      : callback://networking/packets/GetPacketEncapFromMapping
        headers     :
            outereth:
                src     : ref://testcase/config/devicecfg/MACAddr
                dst     : 00:02:0b:0a:0d:0e
            outeripv4:
                src     : ref://testcase/config/devicecfg/IP
                dst     : ref://testcase/config/remotemapping/TunIP
                ttl     : 64
                tos     : 0
                id      : 0
            outermpls:
                label   : ref://testcase/config/localmapping/VNIC/MplsSlot
                ttl     : 0
            mpls:
                label   : ref://testcase/config/remotemapping/MplsSlot
                ttl     : 0
            vxlan:
                vni     : ref://testcase/config/remotemapping/Vnid

    - packet:
        id          : MIRROR_PKT_TX_1 # Expected tx mirrored packet
        clone       : ref://testcase/packets/id=BASE_PKT
        encaps      :
            - ref://factory/packets/id=ENCAP_QINQ
        headers     :
            eth:
                src     : ref://testcase/config/localmapping/VNIC/MACAddr
                dst     : ref://testcase/config/remotemapping/MACAddr
            outerqtag:
                vlan    : callback://mirror/packets/GetRSPANVlanID/direction=tx,id=1
                prio    : 0
            qtag:
                vlan    : ref://testcase/config/localmapping/VNIC/VlanId

    - packet:
        id          : MIRROR_PKT_TX_2 # Expected tx mirrored packet
        clone       : ref://testcase/packets/id=BASE_PKT
        encaps      :
            - ref://factory/packets/id=ENCAP_QINQ
        headers     :
            eth:
                src     : ref://testcase/config/localmapping/VNIC/MACAddr
                dst     : ref://testcase/config/remotemapping/MACAddr
            outerqtag:
                vlan    : callback://mirror/packets/GetRSPANVlanID/direction=tx,id=2
                prio    : 0
            qtag:
                vlan    : ref://testcase/config/localmapping/VNIC/VlanId

    - packet:
        id          : MIRROR_PKT_TX_3 # Expected tx mirrored packet
        clone       : ref://testcase/packets/id=BASE_PKT
        encaps      :
            - ref://factory/packets/id=ENCAP_QINQ
        headers     :
            eth:
                src     : ref://testcase/config/localmapping/VNIC/MACAddr
                dst     : ref://testcase/config/remotemapping/MACAddr
            outerqtag:
                vlan    : callback://mirror/packets/GetRSPANVlanID/direction=tx,id=3
                prio    : 0
            qtag:
                vlan    : ref://testcase/config/localmapping/VNIC/VlanId

    - packet:
        id          : MIRROR_PKT_TX_4 # Expected tx mirrored packet
        clone       : ref://testcase/packets/id=BASE_PKT
        encaps      :
            - ref://factory/packets/id=ENCAP_QINQ
        headers     :
            eth:
                src     : ref://testcase/config/localmapping/VNIC/MACAddr
                dst     : ref://testcase/config/remotemapping/MACAddr
            outerqtag:
                vlan    : callback://mirror/packets/GetRSPANVlanID/direction=tx,id=4
                prio    : 0
            qtag:
                vlan    : ref://testcase/config/localmapping/VNIC/VlanId

    - packet:
        id          : MIRROR_PKT_TX_5 # Expected tx mirrored packet
        clone       : ref://testcase/packets/id=BASE_PKT
        encaps      :
            - ref://factory/packets/id=ENCAP_QINQ
        headers     :
            eth:
                src     : ref://testcase/config/localmapping/VNIC/MACAddr
                dst     : ref://testcase/config/remotemapping/MACAddr
            outerqtag:
                vlan    : callback://mirror/packets/GetRSPANVlanID/direction=tx,id=5
                prio    : 0
            qtag:
                vlan    : ref://testcase/config/localmapping/VNIC/VlanId

    - packet:
        id          : MIRROR_PKT_TX_6 # Expected tx mirrored packet
        clone       : ref://testcase/packets/id=BASE_PKT
        encaps      :
            - ref://factory/packets/id=ENCAP_QINQ
        headers     :
            eth:
                src     : ref://testcase/config/localmapping/VNIC/MACAddr
                dst     : ref://testcase/config/remotemapping/MACAddr
            outerqtag:
                vlan    : callback://mirror/packets/GetRSPANVlanID/direction=tx,id=6
                prio    : 0
            qtag:
                vlan    : ref://testcase/config/localmapping/VNIC/VlanId

    - packet:
        id          : MIRROR_PKT_TX_7 # Expected tx mirrored packet
        clone       : ref://testcase/packets/id=BASE_PKT
        encaps      :
            - ref://factory/packets/id=ENCAP_QINQ
        headers     :
            eth:
                src     : ref://testcase/config/localmapping/VNIC/MACAddr
                dst     : ref://testcase/config/remotemapping/MACAddr
            outerqtag:
                vlan    : callback://mirror/packets/GetRSPANVlanID/direction=tx,id=7
                prio    : 0
            qtag:
                vlan    : ref://testcase/config/localmapping/VNIC/VlanId

    - packet:
        id          : MIRROR_PKT_TX_8 # Expected tx mirrored packet
        clone       : ref://testcase/packets/id=BASE_PKT
        encaps      :
            - ref://factory/packets/id=ENCAP_QINQ
        headers     :
            eth:
                src     : ref://testcase/config/localmapping/VNIC/MACAddr
                dst     : ref://testcase/config/remotemapping/MACAddr
            outerqtag:
                vlan    : callback://mirror/packets/GetRSPANVlanID/direction=tx,id=8
                prio    : 0
            qtag:
                vlan    : ref://testcase/config/localmapping/VNIC/VlanId

trigger:
    packets:
        - packet:
            object  : ref://testcase/packets/id=FROM_HOST_PKT
            port    : ref://testcase/config/hostport

expect:
    packets:
        - packet:
            object  : ref://testcase/packets/id=TO_SWITCH_PKT
            port    : ref://testcase/config/switchport

        - packet:
            object  : callback://mirror/packets/GetExpectedPacket/direction=tx,id=1
            port    : callback://mirror/packets/GetRSPANPortID/direction=tx,id=1

        - packet:
            object  : callback://mirror/packets/GetExpectedPacket/direction=tx,id=2
            port    : callback://mirror/packets/GetRSPANPortID/direction=tx,id=2

        - packet:
            object  : callback://mirror/packets/GetExpectedPacket/direction=tx,id=3
            port    : callback://mirror/packets/GetRSPANPortID/direction=tx,id=3

        - packet:
            object  : callback://mirror/packets/GetExpectedPacket/direction=tx,id=4
            port    : callback://mirror/packets/GetRSPANPortID/direction=tx,id=4

        - packet:
            object  : callback://mirror/packets/GetExpectedPacket/direction=tx,id=5
            port    : callback://mirror/packets/GetRSPANPortID/direction=tx,id=5

        - packet:
            object  : callback://mirror/packets/GetExpectedPacket/direction=tx,id=6
            port    : callback://mirror/packets/GetRSPANPortID/direction=tx,id=6

        - packet:
            object  : callback://mirror/packets/GetExpectedPacket/direction=tx,id=7
            port    : callback://mirror/packets/GetRSPANPortID/direction=tx,id=7

        - packet:
            object  : callback://mirror/packets/GetExpectedPacket/direction=tx,id=8
            port    : callback://mirror/packets/GetRSPANPortID/direction=tx,id=8
