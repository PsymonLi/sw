# Feature Rx Mirror Modules List
feature:
    id          : mirror
    sub         : rx_mirror
    package     : test.mirror
    module      : mirror
    enable      : True
    ignore      : True

modules:

    - module:
        name    : IPV4_IGW2LOCAL
        spec    : igw2local_rx.testspec
        iterate :
            - flow : filter://FlType=MAPPING,AddrFamily=IPV4,FwdMode=IGW,Label=NETWORKING

    - module:
        name    : IPV6_IGW2LOCAL
        spec    : igw2local_rx.testspec
        iterate :
            - flow : filter://FlType=MAPPING,AddrFamily=IPV6,FwdMode=IGW,Label=NETWORKING
