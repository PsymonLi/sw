    - packet:
        id          : ERSPAN_MIRROR_PKT_TX_BASE # Expected ERSPAN tx mirrored packet base
        payloadsize : ref://factory/payloads/id=PAYLOAD_ZERO70/size
        template    : ref://testcase/packets/id=RAW
        encaps      :
            - ref://factory/packets/id=ENCAP_ERSPAN
        headers     :
            erspaneth:
                src    : 01:00:00:01:02:00 #TODO: fix in impl ref://testcase/config/devicecfg/MACAddr
                dst    : 00:02:0b:0a:0d:0e
            erspanqtag:
                prio   : 0
                vlan   : 0
            erspanipv4:
                tos    : 0
                id     : 0
            erspangre:
            erspan:
                sessionid : 0
                ver       : const/2
                d         : callback://mirror/packets/GetERSPANDirection/direction=TX
                gra       : 0x3
                t         : 0
                vlan      : ref://testcase/config/localmapping/VNIC/VlanId
                cos       : callback://mirror/packets/GetERSPANCos/pktid=FROM_HOST_PKT
            payload :
                data    : ref://factory/payloads/id=PAYLOAD_ZERO70/data

    - packet:
        id          : ERSPAN_MIRROR_PKT_TX_1 # Expected RSPAN tx mirrored packet for session 1
        clone       : ref://testcase/packets/id=ERSPAN_MIRROR_PKT_TX_BASE
        payloadsize : callback://mirror/packets/GetERSPANPayloadSize/basepktid=FROM_HOST_PKT,direction=TX,id=1
        headers     :
            erspaneth:
                dst    : callback://mirror/packets/GetERSPANDstMac/direction=TX,id=1
            erspanqtag:
                vlan   : callback://mirror/packets/GetSPANVlanID/direction=TX,id=1
            erspanipv4:
                tos    : callback://mirror/packets/GetERSPANDscp/direction=TX,id=1
                src    : callback://mirror/packets/GetERSPANSrcIP/direction=TX,id=1
                dst    : callback://mirror/packets/GetERSPANDstIP/direction=TX,id=1
            erspangre:
            erspan:
                sessionid : callback://mirror/packets/GetERSPANSessionId/direction=TX,id=1
                t         : callback://mirror/packets/GetERSPANTruncate/basepktid=FROM_HOST_PKT,direction=RX,id=1
            payload :
                data    : callback://mirror/packets/GetERSPANPayload/basepktid=FROM_HOST_PKT,direction=TX,id=1

    - packet:
        id          : ERSPAN_MIRROR_PKT_TX_2 # Expected RSPAN tx mirrored packet for session 2
        clone       : ref://testcase/packets/id=ERSPAN_MIRROR_PKT_TX_BASE
