#Test Spec for Tx Mirror session in local2igw scenario
selectors:
    root: ref://store/templates/id=FLOW
    maxlimits: 2
    flow: filter://SourceGuard=False
    route: filter://VPCPeeringEnabled=False

packets:
    - packet:
        id          : BASE_PKT # Base Packet
        payloadsize : ref://factory/payloads/id=PAYLOAD_ZERO64/size
        template    : callback://networking/packets/GetPacketTemplateFromMapping/proto=UDP
        headers     :
            eth:
                src     : ref://testcase/config/localmapping/VNIC/MACAddr
                dst     : 00:02:00:00:00:00
            qtag:
                vlan    : ref://testcase/config/localmapping/VNIC/VlanId
            ipv4:
                src     : ref://testcase/config/localmapping/IP
                dst     : callback://networking/packets/GetUsableHostFromRoute
                ttl     : 64
            ipv6:
                src     : ref://testcase/config/localmapping/IP
                dst     : callback://networking/packets/GetUsableHostFromRoute
                hlim    : 64
            udp:
                sport   : 100
                dport   : 200
            payload:
                data    : ref://factory/payloads/id=PAYLOAD_ZERO64/data

    - packet:
        id          : FROM_HOST_PKT # Input packet from host
        clone       : ref://testcase/packets/id=BASE_PKT
        encaps      :
            - ref://factory/packets/id=ENCAP_QTAG

    - packet:
        id          : TO_SWITCH_PKT # Expected packet to network
        clone       : ref://testcase/packets/id=BASE_PKT
        encaps      : callback://networking/packets/GetPacketEncapFromMapping
        headers     :
            outereth:
                src     : ref://testcase/config/devicecfg/MACAddr
                dst     : 00:02:0b:0a:0d:0e
            outeripv4:
                src     : ref://testcase/config/devicecfg/IP
                dst     : ref://testcase/config/route/TunIP
                ttl     : 64
                tos     : 0
                id      : 0
            mpls:
                label   : ref://testcase/config/route/Tunnel/EncapValue
                ttl     : 0
                s       : 1
            vxlan:
                vni     : ref://testcase/config/route/Tunnel/EncapValue
            ipv4:
                src     : callback://networking/packets/GetIPFromLocalMapping
            ipv6:
                src     : callback://networking/packets/GetIPFromLocalMapping

    - packet:
        id          : RSPAN_MIRROR_PKT_TX_BASE # Expected RSPAN tx mirrored packet
        clone       : ref://testcase/packets/id=BASE_PKT
        encaps      :
            - ref://factory/packets/id=ENCAP_QINQ
        headers     :
            outerqtag:
                vlan    : 0
                prio    : 0

    - packet:
        id          : RSPAN_MIRROR_PKT_TX_1 # Expected RSPAN tx mirrored packet for session 1
        clone       : ref://testcase/packets/id=RSPAN_MIRROR_PKT_TX_BASE
        headers     :
            outerqtag:
                vlan    : callback://mirror/packets/GetSPANVlanID/direction=TX,id=1

    - packet:
        id          : RSPAN_MIRROR_PKT_TX_2 # Expected RSPAN tx mirrored packet for session 2
        clone       : ref://testcase/packets/id=RSPAN_MIRROR_PKT_TX_BASE
        headers     :
            outerqtag:
                vlan    : callback://mirror/packets/GetSPANVlanID/direction=TX,id=2

    - packet:
        id          : RSPAN_MIRROR_PKT_TX_3 # Expected RSPAN tx mirrored packet for session 3
        clone       : ref://testcase/packets/id=RSPAN_MIRROR_PKT_TX_BASE
        headers     :
            outerqtag:
                vlan    : callback://mirror/packets/GetSPANVlanID/direction=TX,id=3

    - packet:
        id          : RSPAN_MIRROR_PKT_TX_4 # Expected RSPAN tx mirrored packet for session 4
        clone       : ref://testcase/packets/id=RSPAN_MIRROR_PKT_TX_BASE
        headers     :
            outerqtag:
                vlan    : callback://mirror/packets/GetSPANVlanID/direction=TX,id=4

    - packet:
        id          : RSPAN_MIRROR_PKT_TX_5 # Expected RSPAN tx mirrored packet for session 5
        clone       : ref://testcase/packets/id=RSPAN_MIRROR_PKT_TX_BASE
        headers     :
            outerqtag:
                vlan    : callback://mirror/packets/GetSPANVlanID/direction=TX,id=5

    - packet:
        id          : RSPAN_MIRROR_PKT_TX_6 # Expected RSPAN tx mirrored packet for session 6
        clone       : ref://testcase/packets/id=RSPAN_MIRROR_PKT_TX_BASE
