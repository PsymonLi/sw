# Test Spec for Tx, Rx Mirror session in local2local scenario
selectors:
    root: ref://store/templates/id=FLOW
    maxlimits: 2
    flow: filter://any

packets:
    - packet:
        id          : FROM_HOST_PKT # Input packet
        payloadsize : ref://factory/payloads/id=PAYLOAD_ZERO64/size
        template    : callback://networking/packets/GetPacketTemplateFromMapping/proto=TCP
        encaps      :
            - ref://factory/packets/id=ENCAP_QTAG
        headers     :
            eth:
                src     : ref://testcase/config/localmapping/VNIC/MACAddr
                dst     : ref://testcase/config/remotemapping/VNIC/MACAddr
            qtag:
                vlan    : ref://testcase/config/localmapping/VNIC/VlanId
                prio    : 0
            ipv4:
                src     : ref://testcase/config/localmapping/IP
                dst     : ref://testcase/config/remotemapping/IP
                ttl     : 64
            ipv6:
                src     : ref://testcase/config/localmapping/IP
                dst     : ref://testcase/config/remotemapping/IP
                hlim    : 64
            tcp:
                sport   : 100
                dport   : 200
            payload:
                data    : ref://factory/payloads/id=PAYLOAD_ZERO64/data


    - packet:
        id          : TO_HOST_PKT # Expected packet
        clone       : ref://testcase/packets/id=FROM_HOST_PKT
        headers     :
            eth:
                src     : callback://networking/packets/GetPacketSrcMacAddrFromMapping
                dst     : ref://testcase/config/remotemapping/VNIC/MACAddr
            qtag:
                vlan    : ref://testcase/config/remotemapping/VNIC/VlanId
                prio    : 0

    - packet:
        id          : MIRROR_PKT_TX_BASE # Expected rx mirror packet base
        payloadsize : ref://factory/payloads/id=PAYLOAD_ZERO64/size
        template    : callback://networking/packets/GetPacketTemplateFromMapping/proto=TCP
        encaps      :
            - ref://factory/packets/id=ENCAP_QINQ
        headers     :
            eth:
                src     : ref://testcase/config/localmapping/VNIC/MACAddr
                dst     : ref://testcase/config/remotemapping/VNIC/MACAddr
            qtag:
                vlan    : ref://testcase/config/localmapping/VNIC/VlanId
                prio    : 0
            ipv4:
                src     : ref://testcase/config/localmapping/IP
                dst     : ref://testcase/config/remotemapping/IP
                ttl     : 64
            ipv6:
                src     : ref://testcase/config/localmapping/IP
                dst     : ref://testcase/config/remotemapping/IP
                hlim    : 64
            tcp:
                sport   : 100
                dport   : 200
            payload:
                data    : ref://factory/payloads/id=PAYLOAD_ZERO64/data

    - packet:
        id          : MIRROR_PKT_RX_BASE # Expected tx mirrored packetbase
        payloadsize : ref://factory/payloads/id=PAYLOAD_ZERO64/size
        template    : callback://networking/packets/GetPacketTemplateFromMapping/proto=TCP
        encaps      :
            - ref://factory/packets/id=ENCAP_QINQ
        headers     :
            eth:
                src     : callback://networking/packets/GetPacketSrcMacAddrFromMapping
                dst     : ref://testcase/config/remotemapping/VNIC/MACAddr
            qtag:
                vlan    : ref://testcase/config/localmapping/VNIC/VlanId
                prio    : 0
            ipv4:
                src     : ref://testcase/config/localmapping/IP
                dst     : ref://testcase/config/remotemapping/IP
                ttl     : 64
            ipv6:
                src     : ref://testcase/config/localmapping/IP
                dst     : ref://testcase/config/remotemapping/IP
                hlim    : 64
            tcp:
                sport   : 100
                dport   : 200
            payload:
                data    : ref://factory/payloads/id=PAYLOAD_ZERO64/data

    - packet:
        id          : MIRROR_PKT_TX_1 # Expected tx mirrored packet
        clone       : ref://testcase/packets/id=MIRROR_PKT_TX_BASE
        headers     :
            outerqtag:
                vlan    : callback://mirror/packets/GetRSPANVlanID/direction=tx,id=1
                prio    : 0

    - packet:
        id          : MIRROR_PKT_TX_2 # Expected tx mirrored packet
        clone       : ref://testcase/packets/id=MIRROR_PKT_TX_BASE
        headers     :
            outerqtag:
                vlan    : callback://mirror/packets/GetRSPANVlanID/direction=tx,id=2
                prio    : 0

    - packet:
        id          : MIRROR_PKT_TX_3 # Expected tx mirrored packet
        clone       : ref://testcase/packets/id=MIRROR_PKT_TX_BASE
        headers     :
            outerqtag:
                vlan    : callback://mirror/packets/GetRSPANVlanID/direction=tx,id=3
                prio    : 0

    - packet:
        id          : MIRROR_PKT_TX_4 # Expected tx mirrored packet
        clone       : ref://testcase/packets/id=MIRROR_PKT_TX_BASE
        headers     :
            outerqtag:
                vlan    : callback://mirror/packets/GetRSPANVlanID/direction=tx,id=4
                prio    : 0

    - packet:
        id          : MIRROR_PKT_TX_5 # Expected tx mirrored packet
        clone       : ref://testcase/packets/id=MIRROR_PKT_TX_BASE
        headers     :
            outerqtag:
                vlan    : callback://mirror/packets/GetRSPANVlanID/direction=tx,id=5
                prio    : 0

    - packet:
        id          : MIRROR_PKT_TX_6 # Expected tx mirrored packet
        clone       : ref://testcase/packets/id=MIRROR_PKT_TX_BASE
        headers     :
            outerqtag:
                vlan    : callback://mirror/packets/GetRSPANVlanID/direction=tx,id=6
                prio    : 0

    - packet:
        id          : MIRROR_PKT_TX_7 # Expected tx mirrored packet
        clone       : ref://testcase/packets/id=MIRROR_PKT_TX_BASE
        headers     :
            outerqtag:
                vlan    : callback://mirror/packets/GetRSPANVlanID/direction=tx,id=7
                prio    : 0

    - packet:
        id          : MIRROR_PKT_TX_8 # Expected tx mirrored packet
        clone       : ref://testcase/packets/id=MIRROR_PKT_TX_BASE
        headers     :
            outerqtag:
                vlan    : callback://mirror/packets/GetRSPANVlanID/direction=tx,id=8
                prio    : 0

    - packet:
        id          : MIRROR_PKT_RX_1 # Expected tx mirrored packet
        clone       : ref://testcase/packets/id=MIRROR_PKT_RX_BASE
        headers     :
            outerqtag:
                vlan    : callback://mirror/packets/GetRSPANVlanID/direction=rx,id=1
                prio    : 0
    - packet:
        id          : MIRROR_PKT_RX_2 # Expected tx mirrored packet
        clone       : ref://testcase/packets/id=MIRROR_PKT_RX_BASE
        headers     :
            outerqtag:
                vlan    : callback://mirror/packets/GetRSPANVlanID/direction=rx,id=2
                prio    : 0

    - packet:
        id          : MIRROR_PKT_RX_3 # Expected tx mirrored packet
        clone       : ref://testcase/packets/id=MIRROR_PKT_RX_BASE
        headers     :
            outerqtag:
                vlan    : callback://mirror/packets/GetRSPANVlanID/direction=rx,id=3
                prio    : 0

    - packet:
        id          : MIRROR_PKT_RX_4 # Expected tx mirrored packet
        clone       : ref://testcase/packets/id=MIRROR_PKT_RX_BASE
        headers     :
            outerqtag:
                vlan    : callback://mirror/packets/GetRSPANVlanID/direction=rx,id=4
                prio    : 0

    - packet:
        id          : MIRROR_PKT_RX_5 # Expected tx mirrored packet
        clone       : ref://testcase/packets/id=MIRROR_PKT_RX_BASE
        headers     :
            outerqtag:
                vlan    : callback://mirror/packets/GetRSPANVlanID/direction=rx,id=5
                prio    : 0

    - packet:
        id          : MIRROR_PKT_RX_6 # Expected tx mirrored packet
        clone       : ref://testcase/packets/id=MIRROR_PKT_RX_BASE
        headers     :
            outerqtag:
                vlan    : callback://mirror/packets/GetRSPANVlanID/direction=rx,id=6
                prio    : 0

    - packet:
        id          : MIRROR_PKT_RX_7 # Expected tx mirrored packet
        clone       : ref://testcase/packets/id=MIRROR_PKT_RX_BASE
        headers     :
            outerqtag:
                vlan    : callback://mirror/packets/GetRSPANVlanID/direction=rx,id=7
                prio    : 0

    - packet:
        id          : MIRROR_PKT_RX_8 # Expected tx mirrored packet
        clone       : ref://testcase/packets/id=MIRROR_PKT_RX_BASE
        headers     :
            outerqtag:
                vlan    : callback://mirror/packets/GetRSPANVlanID/direction=rx,id=8
                prio    : 0

trigger:
    packets:
        - packet:
            object  : ref://testcase/packets/id=FROM_HOST_PKT
            port    : ref://testcase/config/hostport

expect:
    packets:
        - packet:
            object  : ref://testcase/packets/id=TO_HOST_PKT
            port    : ref://testcase/config/hostport

        - packet:
            object  : callback://mirror/packets/GetExpectedPacket/direction=tx,id=1
            port    : callback://mirror/packets/GetRSPANPortID/direction=tx,id=1

        - packet:
            object  : callback://mirror/packets/GetExpectedPacket/direction=tx,id=2
