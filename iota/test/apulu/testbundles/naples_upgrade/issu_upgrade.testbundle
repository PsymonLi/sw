meta:
    name    : issu_upgrade
    os      : [ linux ]
    nics    : [ pensando, mellanox, intel ]
    targetcount : 1

testcases:
    -   name: ISSU_UPGRADE_FWONLY
        testcase    : testcases.naples_upgrade.issu_upgrade
        stress      : False
        ignore      : True
        iterators   :
            type    : simple
            selection : [ any ]  # upgrade all or any
        args        :
            sleep             : 300
            naples_upgr_pkg   : "naples_fw.tar"
