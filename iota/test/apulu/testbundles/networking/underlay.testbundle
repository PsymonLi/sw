meta:
    name    : underlay
    os      : [ linux ]
    nicmode : classic
    nics    : [ pensando ]

testcases:
    -   name    : Ping_Underlay_Workloads
        testcase: testcases.underlay.ping
        stress  : True
        args    :
            ping_count: 10
            ping_interval: 0.01
        iterators :
            type    : nested
            ipaf    : [ ipv4 ]
            pktsize : [ 64, 128, 1024 ]
            intf    : [ loopback, bgp_peer ]
            shut    : [ none, Uplink0, none, Uplink1, none, both, none ] 

