meta:
    name    : networking
    os      : [ linux ]
    nicmode : classic
    nics    : [ pensando ]

testcases:
    -   name    : Ping_VCN_Workloads
        testcase: testcases.networking.vcn_ping
        stress  : True
        args    :
            type: remote_only
        iterators :
            type    : nested
            ipaf    : [ ipv4 ]
            pktsize : [ 64, 1024 ]

    -   name    : Ping_Remote_Workloads
        testcase: testcases.networking.ping
        stress  : True
        args    :
            type: remote_only
        iterators :
            type    : nested
            #ipaf    : [ ipv4, ipv6 ]
            ipaf    : [ ipv4 ]
            #pktsize : [ 64, 128, 256, 512, 1024, 2048, 4096, 8192 ]
            pktsize : [ 64, 128, 256, 512, 1024 ]

    -   name    : Ping_IGW_NAPT_Workloads
        testcase: testcases.networking.ping
        stress  : True
        args    :
            type: igw_only
            nat_type: napt
        iterators :
            type    : nested
            ipaf    : [ ipv4 ]
            pktsize : [ 64 ]

    -   name    : Ping_IGW_NAPT_SERVICE_Workloads
        testcase: testcases.networking.ping
        stress  : True
        args    :
            type: igw_only
            nat_type: napt_service
        iterators :
            type    : nested
            ipaf    : [ ipv4 ]
            pktsize : [ 64 ]

    -   name    : Ping_IGW_PUBLIC_NAPT_SERVICE_Workloads
        testcase: testcases.networking.ping
        stress  : True
        args    :
            type: igw_only
            nat_type: napt_service
            public_vnic: true
        iterators :
            type    : nested
            ipaf    : [ ipv4 ]
            pktsize : [ 64 ]

    -   name    : Iperf_IGW_NAPT_TCP
        testcase: testcases.networking.iperf
        args    :
            type : igw_only
            nat_type: napt
            num_streams : 10
        iterators :
            type      : nested
            ipaf      : [ ipv4 ]
            pktsize   : [ 1024 ]
            protocol  : [ tcp ]

    -   name    : Iperf_IGW_NAPT_UDP
        testcase: testcases.networking.iperf
        args    :
            type : igw_only
            nat_type: napt
            num_streams : 10
        iterators :
            type      : nested
            ipaf      : [ ipv4 ]
            pktsize   : [ 1024 ]
            protocol  : [ udp ]

    -   name    : Iperf_IGW_NAPT_SERVICE_TCP
        testcase: testcases.networking.iperf
        args    :
            type : igw_only
            nat_type: napt_service
            num_streams : 10
        iterators :
            type      : nested
            ipaf      : [ ipv4 ]
            pktsize   : [ 512 ]
            protocol  : [ tcp ]

    -   name    : Iperf_IGW_NAPT_SERVICE_UDP
        testcase: testcases.networking.iperf
        args    :
            type : igw_only
            nat_type: napt_service
            num_streams : 10
        iterators :
            type      : nested
            ipaf      : [ ipv4 ]
            pktsize   : [ 512 ]
            protocol  : [ udp ]

    -   name    : Iperf_IGW_PUBLIC_NAPT_SERVICE_TCP
        testcase: testcases.networking.iperf
        args    :
            type : igw_only
            nat_type: napt_service
            public_vnic: true
            num_streams : 10
        iterators :
            type      : nested
            ipaf      : [ ipv4 ]
            pktsize   : [ 512 ]
            protocol  : [ tcp ]

    -   name    : Iperf_IGW_PUBLIC_NAPT_SERVICE_UDP
        testcase: testcases.networking.iperf
        args    :
            type : igw_only
            nat_type: napt_service
            public_vnic: true
            num_streams : 10
        iterators :
            type      : nested
            ipaf      : [ ipv4 ]
            pktsize   : [ 512 ]
            protocol  : [ udp ]


    -   name    : Ping_IGW_NAT_Workloads
        testcase: testcases.networking.ping
        stress  : True
        args    :
            type : igw_only
            nat_type: static
        iterators :
            type    : nested
            ipaf    : [ ipv4 ]
            pktsize : [ 64 ]

    -   name    : Iperf_IGW_NAT_UDP
        testcase: testcases.networking.iperf
        args    :
            type : igw_only
            nat_type: static
            num_streams : 10
        iterators :
            type      : nested
            ipaf      : [ ipv4 ]
            pktsize   : [ 512 ]
            protocol  : [ udp ]

    -   name    : Iperf_IGW_NAT_TCP
        testcase: testcases.networking.iperf
        args    :
            type : igw_only
            nat_type: static
            num_streams : 10
        iterators :
            type      : nested
            ipaf      : [ ipv4 ]
            pktsize   : [ 1024 ]
            protocol  : [ tcp ]

    -   name    : Ping_IGW_NAT_RX_Workloads
        testcase: testcases.networking.ping
        stress  : True
        args    :
            type : igw_only
            nat_type: static
            direction : rx
        iterators :
            type    : nested
            ipaf    : [ ipv4 ]
            pktsize : [ 64 ]

