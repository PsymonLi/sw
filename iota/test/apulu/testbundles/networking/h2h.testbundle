meta:
    name    : h2h
    os      : [ linux ]
    nicmode : classic
    nics    : [ pensando ]

testcases:
    -   name    : Workload_Connectivity
        testcase: testcases.networking.connectivity
        stress  : True
        iterators :
            type    : nested
            ipaf    : [ ipv4 ]
            proto   : [ icmp ]
            #pktsize : [ 64, 128, 256, 512, 1024 ]
            pktsize : [ 64, 1024 ]
            #workload_type : [ remote ]
            workload_type : [ local, remote ]
            #workload_scope : [ intra-subnet ]
            workload_scope : [ intra-subnet, inter-subnet ]
