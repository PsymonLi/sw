meta:
    name    : h2h
    os      : [ linux ]
    nicmode : classic
    nics    : [ pensando ]

testcases:
    -   name    : Workload_Connectivity
        testcase: testcases.networking.connectivity
        stress  : True
        iterators :
            type    : nested
            ipaf    : [ ipv4 ]
            proto   : [ icmp ]
            pktsize : [ 64 ]
            workload_type : [ local, remote ]
            workload_scope : [ intra-subnet, inter-subnet ]

    -   name    : Workload_Iperf
        testcase: testcases.networking.connectivity
        stress  : True
        iterators :
            type    : nested
            ipaf    : [ ipv4 ]
            proto   : [ udp, tcp ]
            pktsize : [ 1024 ]
            workload_type : [ remote, local ]
            workload_scope : [ intra-subnet, inter-subnet ]

    -   name    : DHCP_Workload_Connectivity
        testcase: testcases.networking.dhcp_connectivity
        stress  : True
        ignore  : True
        iterators :
            type    : nested
            ipaf    : [ ipv4 ]
            proto   : [ icmp ]
            pktsize : [ 64 ]
            workload_type : [ local ]
            workload_scope : [ intra-subnet, inter-subnet ]
            ipconfig : [ dhcp ]

