meta:
    name    : cps
    os      : [ linux ]
    nics    : [ pensando ]

testcases:
    -   name    : Trex_UDP_TCP_Remote_Workloads
        testcase: testcases.networking.conn_per_sec
        stress  : False
        ignore  : True
        args    :
            type: remote_only
        iterators:
            type      : simple
            proto     : [ tcp, udp, tcp ]
            cps       : [ 10000, 20000, 200 ]
            duration  : [ 10, 10, 3000 ]

