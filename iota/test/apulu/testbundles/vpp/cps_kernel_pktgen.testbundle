meta:
    name    : cps_kernel_pktgen
    os      : [ linux ]
    nics    : [ pensando ]

testcases:
    -   name    : CPS_UDP_kernel_pktgen
        testcase: testcases.networking.cps_kernel_pktgen
        stress  : False
        ignore  : True
        args    :
            type: remote_only
            timeout: 40000
            xmittime: 1
        iterators:
            type            : nested
            packet_count    : [ 563200 ]
            packet_size     : [ 64, 128, 256, 1024, 4096, 8192 ]
            cores           : [ 1, 2, 3, 4, 5 ]
            burst           : [ 1024, 2048, 3072, 4096, 5120, 6144, 7168, 8192, 9216, 14336, 18432, 36864 ]
