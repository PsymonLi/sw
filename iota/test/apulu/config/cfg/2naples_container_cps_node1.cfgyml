# VxLAN Topology Specification
meta:
    id: TOPO_VxLAN_HOSTMODE

device:
    id             : 1
    mode           : host # bitw (default), host
    stack          : ipv4 # ipv4 (default), ipv6
    bridging       : false # false (default), true
    learn-mode     : none # none (default), auto, notify
    overlayrouting : false # false (default), true
    ipaddress      : '30.30.30.30' # Taken from resmgr
    gateway        : '1.0.0.1' # Taken from resmgr
    macaddress     : macaddr/0000.aaaa.aaa1
    encap          : vxlan

uplink:
    - entry:
        id      : uplink0
        port    : 1
        mode    : switch
        status  : UP
    - entry:
        id      : uplink1
        port    : 2
        mode    : switch
        status  : UP

security_profile:
    id        : 1

vpc:
    - id         : 100
      stack      : ipv4 # dual, ipv4 , ipv6
      count      : 1
      type       : underlay
      interface:
        - iid           : 1
          iftype        : l3
          ifadminstatus : UP
          ipprefix      : '30.1.0.1\24'
          macaddress    : macaddr/00ae.cd00.4fde
          portid        : 1
          encap         : none
        - iid           : 2
          iftype        : l3
          ifadminstatus : UP
          ipprefix      : '30.2.0.1\24'
          macaddress    : macaddr/00ae.cd00.4fdd
          portid        : 2
          encap         : none
        - iid           : 3
          iftype        : loopback
          ifadminstatus : UP
          ipprefix      : '30.30.30.30\32'
          encap         : none
      nexthop:
        - id         : 1
          count      : 1
          type       : underlay
          l3intfid   : 1
          macaddress : macaddr/00ae.cd00.14ce
        - id         : 2
          count      : 1
          type       : underlay
          l3intfid   : 2
          macaddress : macaddr/00ae.cd00.14cd
      bgpglobal:
        - localasn      : 100
          routerid      : 30.30.30.30
      bgppeer:
        # underlay bgp peer 1
        - peeraddr      : 30.1.0.2
          localaddr     : 30.1.0.1
          adminstate    : 1
          sendcomm      : true
          sendextcomm   : true
          connectretry  : 5
          rrclient      : false
          # specify interface to override ip addresses from testbed json
          interface     : "Uplink0"
          remoteasn     : 100
          holdtime      : 180
          keepalive     : 60
          bgppeeraf:
              peeraddr  : 30.1.0.2
              localaddr : 30.1.0.1
              afi       : ipv4
              safi      : unicast
        # underlay bgp peer 2
        - peeraddr      : 30.2.0.2
          localaddr     : 30.2.0.1
          adminstate    : 1
          sendcomm      : true
          sendextcomm   : true
          connectretry  : 5
          rrclient      : false
          # specify interface to override ip addresses from testbed json
          interface     : "Uplink1"
          remoteasn     : 100
          holdtime      : 180
          keepalive     : 60
          bgppeeraf:
              peeraddr  : 30.2.0.2
              localaddr : 30.2.0.1
              afi       : ipv4
              safi      : unicast
    - id         : 1
      stack      : ipv4 # dual, ipv4 , ipv6
      count      : 1
      type       : tenant
      tunnel:
        - id         : 1
          count      : 1
          type       : underlay
          srcaddr    : '30.30.30.30'
          dstaddr    : '40.40.40.40'
          macaddress : macaddr/0000.aaaa.aaa1
          encap      : vxlan
