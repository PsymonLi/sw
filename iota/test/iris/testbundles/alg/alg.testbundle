meta:
    name    : alg 
    nics    : [ pensando, intel, mellanox, broadcom ]
    targetcount : 80 

testcases:
    -   name    : tftp_Local_Workloads
        testcase: testcases.alg.tftp.tftp
        args    :
            type: local_only

    -   name     : ftp_basic
        testcase : testcases.alg.ftp.ftp_basic
        iterators:
            type : simple
            mode : [ active, passive ]
        args     :
            type : local_only

    -   name    : ftp_test_memleak
        testcase: testcases.alg.ftp.ftp_test_memleak
        args    :
            type: local_only
            count: 15

    -   name     : ftp_epsv
        testcase : testcases.alg.ftp.ftp_epsv
        args     :
           type : local_only

    -   name     : ftp_test_control_ageout
        testcase : testcases.alg.ftp.ftp_test_control_ageout
        args     :
            type : local_only

    -   name     : ftp_deny_mismatch_ip
        testcase : testcases.alg.ftp.ftp_deny_mismatch_ip
        args     :
            type : local_only

    -   name     : ftp_allow_mismatch_ip
        testcase : testcases.alg.ftp.ftp_allow_mismatch_ip
        args     :
            type : local_only

    -   name     : ftp_data_conn_fail
        testcase : testcases.alg.ftp.ftp_data_conn_fail
        args     :
            type : local_only

    -   name     : ftp_nonstandard_port
        testcase : testcases.alg.ftp.ftp_nonstandard_port
        args     :
           type : local_only

    -   name    : rtsp_Local_Workloads
        testcase: testcases.alg.rtsp.rtsp
        args    :
            type: local_only

    -   name    : sunrpc_Local_Workloads
        testcase: testcases.alg.sunrpc.sunrpc
        args    :
            type: local_only

    -   name    : dns_Local_Workloads
        testcase: testcases.alg.dns.dns
        args    :
            type: local_only 
