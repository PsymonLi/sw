meta:
    name    : vmotion_basic_ping
    os      : [ esx ]
    nics    : [ pensando ]
    #targetcount : 35

testcases:
    -   name    : ping_oob_mnic0_intf
        testcase: testcases.mnic.ping_oob
        ignore: True
        args    :
            mtu: 1500
            verify: no
    -   name    : BASIC_PING_VM_OOB
        testcase: testcases.vmotion.basic_ping
        stress  : True
        args    :
            vm_type      : dsc_to_dsc
            type         : both
            num_sessions : 2
            num_moves    : 2
            #timeout : 60

