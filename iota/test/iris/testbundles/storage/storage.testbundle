meta:
    name        : storage
    os          : [ linux, freebsd ]
    nics        : [ pensando, intel, mellanox, broadcom ]
    targetcount : 600

aliases:
    - &STORAGE_TEST_DIR storage/offload/src/test
    - &CPUS_ITER_LIST_COMBO1 [ 1, 3 ]
    - &CPUS_ITER_LIST_COMBO2 [ 2, 4 ]
    - &BLOCK_SIZE_LIST_COMBO1 [ 4096, 8192 ]
    - &BLOCK_SIZE_LIST_COMBO2 [ 16384, 32768 ]
#    - &BATCH_DEPTH_ITER_LIST_COMBO1 [ 1, 31, 64, 256 ]
    - &BATCH_DEPTH_ITER_LIST_COMBO1 [ 1, 31, 64, 128 ]
#    - &BATCH_DEPTH_ITER_LIST_COMBO2 [ 32, 33, 128, 512 ]
    - &BATCH_DEPTH_ITER_LIST_COMBO2 [ 32, 33 ]
    - &MODE_ITER_LIST_COMBO1 [ sync, poll ]
    - &MODE_ITER_LIST_COMBO2 [ poll, async ]
    - &CPDC_TEST_YML cpdc.yml
    - &CP_FAIL_TEST_YML cp_fail.yml
    - &DC_FAIL_TEST_YML dc_fail.yml
    - &HASH_TEST_YML hash.yml
    - &CHKSUM_TEST_YML chksum.yml
    - &CPFLAGS_DEFAULT 0
    - &DCFLAGS_DEFAULT 0
    - &CPFLAGS_ZERO_PAD zero_pad
    - &DCFLAGS_ZERO_PAD 0
    - &CPFLAGS_WITH_HEADER insert_header
    - &DCFLAGS_WITH_HEADER header_present
    - &CPFLAGS_WITH_HEADER_ZERO_PAD insert_header,zero_pad
    - &DCFLAGS_WITH_HEADER_ZERO_PAD header_present
    - &HASH_FLAGS_LIST [ '0', 'per_block' ]
    - &HASH_ALGO_LIST [ 'sha2_256' ]
    - &CHKSUM_FLAGS_LIST [ '0', 'per_block' ]
    - &CHKSUM_ALGO_LIST [ 'crc32c' ]

testcases:
    -   name    : CompileInsmodSonicPencake
        testcase: testcases.storage.compile_install_drivers
        args    :
            package: storage/gen/storage-offload.tar.xz
            maxcpus: 4

    -   name: CpDcDefault4K8K
        testcase    : testcases.storage.ymltest2
        iterators   :
            type        : nested
            cpus        : *CPUS_ITER_LIST_COMBO1
            batch_depth : *BATCH_DEPTH_ITER_LIST_COMBO1
            mode        : *MODE_ITER_LIST_COMBO1
            blocksize   : *BLOCK_SIZE_LIST_COMBO1
        args    :
            test        : *CPDC_TEST_YML
            cpflags     : *CPFLAGS_DEFAULT
            dcflags     : *DCFLAGS_DEFAULT

    -   name: CpDcZeroPad16K32K
        testcase    : testcases.storage.ymltest2
        enable      : False
        iterators   :
            type        : nested
            cpus        : *CPUS_ITER_LIST_COMBO2
            batch_depth : *BATCH_DEPTH_ITER_LIST_COMBO2
            mode        : *MODE_ITER_LIST_COMBO2
            blocksize   : *BLOCK_SIZE_LIST_COMBO2
        args    :
            test        : *CPDC_TEST_YML
            cpflags     : *CPFLAGS_ZERO_PAD
            dcflags     : *DCFLAGS_ZERO_PAD

    -   name: CpDcWithHeader4K8K
        testcase    : testcases.storage.ymltest2
        enable      : False
        iterators   :
            type        : nested
            cpus        : *CPUS_ITER_LIST_COMBO1
            batch_depth : *BATCH_DEPTH_ITER_LIST_COMBO1
            mode        : *MODE_ITER_LIST_COMBO1
            blocksize   : *BLOCK_SIZE_LIST_COMBO1
        args    :
            test        : *CPDC_TEST_YML
            cpflags     : *CPFLAGS_WITH_HEADER
            dcflags     : *DCFLAGS_WITH_HEADER

    -   name: CpDcWithHeaderZeroPad16K32K
        testcase    : testcases.storage.ymltest2
        enable      : False
        iterators   :
            type        : nested
            cpus        : *CPUS_ITER_LIST_COMBO2
            batch_depth : *BATCH_DEPTH_ITER_LIST_COMBO2
            mode        : *MODE_ITER_LIST_COMBO2
            blocksize   : *BLOCK_SIZE_LIST_COMBO2
        args    :
            test        : *CPDC_TEST_YML
            cpflags     : *CPFLAGS_WITH_HEADER_ZERO_PAD
            dcflags     : *DCFLAGS_WITH_HEADER_ZERO_PAD

    -   name    : CompressFail
        testcase: testcases.storage.ymltest
        enable      : False
        args    :
            dir     : *STORAGE_TEST_DIR
            wait    : 1
            cfg     : [ 4k.yml, globals.yml ]
            test    : cp_fail.yml
            failtest: True

    -   name    : DecompressFail
        testcase: testcases.storage.ymltest
        enable      : False
        args    :
            dir     : *STORAGE_TEST_DIR
            wait    : 1
            cfg     : [ 4k.yml, globals.yml ]
            test    : dc_fail.yml
            failtest: True

    -   name        : Hash4K8K
        testcase    : testcases.storage.ymltest2
        enable      : False
        ignore      : True
        iterators   :
            type        : nested
            cpus        : *CPUS_ITER_LIST_COMBO1
            batch_depth : *BATCH_DEPTH_ITER_LIST_COMBO1
            mode        : *MODE_ITER_LIST_COMBO1
            blocksize   : *BLOCK_SIZE_LIST_COMBO1
            flags       : *HASH_FLAGS_LIST
            algo        : *HASH_ALGO_LIST
        args    :
            test        : *HASH_TEST_YML

    -   name        : Hash16K32K
        testcase    : testcases.storage.ymltest2
        enable      : False
        ignore      : True
        iterators   :
            type        : nested
            cpus        : *CPUS_ITER_LIST_COMBO2
            batch_depth : *BATCH_DEPTH_ITER_LIST_COMBO2
            mode        : *MODE_ITER_LIST_COMBO2
            blocksize   : *BLOCK_SIZE_LIST_COMBO2
            flags       : *HASH_FLAGS_LIST
            algo        : *HASH_ALGO_LIST
        args    :
            test        : *HASH_TEST_YML

    -   name        : Checksum4K8K
        testcase    : testcases.storage.ymltest2
        enable      : False
        ignore      : True
        iterators   :
            type        : nested
            cpus        : *CPUS_ITER_LIST_COMBO1
            batch_depth : *BATCH_DEPTH_ITER_LIST_COMBO1
            mode        : *MODE_ITER_LIST_COMBO1
            blocksize   : *BLOCK_SIZE_LIST_COMBO1
            flags       : *CHKSUM_FLAGS_LIST
            algo        : *CHKSUM_ALGO_LIST
        args    :
            test        : *CHKSUM_TEST_YML

    -   name        : Checksum16K32K
        testcase    : testcases.storage.ymltest2
        enable      : False
        ignore      : True
        iterators   :
            type        : nested
            cpus        : *CPUS_ITER_LIST_COMBO2
            batch_depth : *BATCH_DEPTH_ITER_LIST_COMBO1
            mode        : *MODE_ITER_LIST_COMBO1
            blocksize   : *BLOCK_SIZE_LIST_COMBO2
            flags       : *CHKSUM_FLAGS_LIST
            algo        : *CHKSUM_ALGO_LIST
        args    :
            test        : *CHKSUM_TEST_YML


