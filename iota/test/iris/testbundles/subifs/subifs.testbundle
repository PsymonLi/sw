meta:
    name    : subifs
    os      : [ linux, freebsd, esx ]
    nics    : [ pensando, mellanox, intel, broadcom ]
    targetcount : 4

testcases:
    -   name    : Memleak_Subifs
        testcase: testcases.subifs.memleak_subif
        stress  : False
        ignore  : True
        args    :
            type: remote_only
            ipv4: True
            ipv6: False

    -   name    : MacValidation_Subifs
        testcase: testcases.subifs.unimac_validation_subif
        stress  : False
        ignore  : True
        args    :
            type: remote_only
            ipv4: True
            ipv6: False

    -   name    : MacChange_Subifs
        testcase: testcases.subifs.unimac_change_subif
        stress  : False
        ignore  : True
        iterators:
            ipaf : [ ipv4 ]
            type : simple
        args    :
            type: remote_only

    -   name    : Multiple_Subifs_Stress
        testcase: testcases.subifs.subif
        stress  : False
        ignore  : True
        args    :
            type: remote_only
            loop_count: 3
            ipv4: True
            ipv6: False






