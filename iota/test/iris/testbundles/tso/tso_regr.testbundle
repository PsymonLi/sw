meta:
    name    : tso_regr
    os      : [ linux, freebsd ]
    nics    : [ pensando, intel, mellanox, broadcom ]
    targetcount : 160

testcases:
    -   name    : TSO_REGR
        testcase: testcases.drivers.drivertso
        disable : False
        ignore  : True
        iterators :
            type               : nested
            tso_offload        : [ 'on', 'off']
            lro_offload        : [ 'off']
            tx_check_sum       : [ 'on']
            mtu                : [ '4096', '9000']
            pktsize            : [  2048, 32768 ]
            vlantag            : [ 'off']
            vxlan              : [ 'off']
            proto              : [ 'tcp', 'udp']
            ipproto            : [ 'v4', 'v6']
        args    :
            timeout : 7200
            type: host
            iperfthreads     :  1
