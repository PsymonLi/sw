meta:
    name    : ipsec
    os      : [ linux, esx ]
    nics    : [ pensando, mellanox, intel, broadcom ]
    targetcount : 35

testcases:
    -   name    : iperf_over_ipsec
        testcase: testcases.ipsec.iperf
        iterators:
            type  : nested
            protocol : [tcp, udp]
            port  : [20001, 20002]
            pktsize : [96, 128, 256, 512, 1024, 2048, 4096, 8192]

    -   name    : nc_over_ipsec
        testcase: testcases.ipsec.nc_client
        iterators:
            type  : nested
            protocol : [tcp]
            port  : [20001, 20002]
 
