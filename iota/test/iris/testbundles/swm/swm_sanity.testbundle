meta:
    name    : swm_sanity
    os      : [ linux ]
    nics    : [ pensando, intel, mellanox, broadcom ]
    targetcount : 3

testcases:
    -   name    : download_ahs
        testcase: testcases.swm.download_ahs
        enable  : True
        iterators :
            type    : nested
            count   : [2]

    -   name    : vmedia_traffic
        testcase: testcases.swm.vmedia_traffic
        ignore  : False
        enable  : True

    -   name    : mode_switch
        testcase: testcases.swm.mode_switch 
        enable  : True
        iterators :
            type    : nested
            count   : [10]
        args    :
            timeout : 3600

    -   name    : server_reset
        testcase: testcases.swm.server_reset
        enable  : True
        iterators :
            type    : nested
            count   : [3]
            reboot_types : [cycle, reset]
        args    :
            timeout : 3600
    
    -   name    : apc_reset
        testcase: testcases.swm.apc_reset 
        enable  : False
        iterators :
            type    : nested
            count   : [1]
        args    :
            timeout : 3600
    
    -   name    : switch_port_flap
        testcase: testcases.swm.switch_port_flap
        enable  : False
        iterators :
            type    : nested
            count   : [5]
        args    :
            timeout : 3600
    
    -   name    : naples_freq_check
        testcase: testcases.swm.naples_freq_check
        enable  : True
    
    -   name    : oob_access
        testcase: testcases.swm.oob_access
        enable  : True

    -   name    : switch_ncsi_channels
        testcase: testcases.swm.switch_ncsi_channels
        enable  : True
        iterators :
            type    : nested
            count   : [5]
        args    :
            timeout : 3600