meta:
    name    : cps
    os      : [ esx ]
    nics    : [ pensando, mellanox, intel, broadcom ]

testcases:
    -   name    : Cps_Local_Workloads
        testcase: testcases.networking.conn_per_sec
        stress  : True
        args    :
            type: local_only
        iterators:
            type      : simple
            proto     : [ tcp, udp ]
            cps       : [ 900, 900 ]
            duration  : [ 300, 300 ]
    -   name    : Cps_Remote_Workloads
        testcase: testcases.networking.conn_per_sec
        stress  : True
        args    :
            type: remote_only
        iterators:
            type      : simple
            proto     : [ tcp, udp ]
            cps       : [ 900, 900 ]
            duration  : [ 300, 300 ]
    -   name    : session_scale
        testcase: testcases.networking.session_scale
        stress  : True
        ignore  : True
        args    :
             latency_pps: 10
             switch_port_flap_interval: 10
             sec_policy_interval: 30
        iterators:
            type             : simple
            max_active_flow  : [64000]
            cps              : [900]
            duration         : [300]
