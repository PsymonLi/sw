meta:
    name    : rdma_ext
    os      : [ linux, freebsd ]
    nics    : [ pensando, mellanox ]
    targetcount : 150

testcases:
    #=======================================
    # Init
    #=======================================
    -   name    : Build_Rdma
        testcase: testcases.rdma.build_rdma
        args    :
            type: remote_only

    -   name    : Insmod_Rdma
        testcase: testcases.rdma.insmod_rdma
        args    :
            type: remote_only

    -   name    : Show_Gid
        testcase: testcases.rdma.show_gid
        args    :
            type: remote_only

    -   name    : Intf_Mtu
        testcase: testcases.rdma.intf_mtu
        args    :
            type: remote_only

    #=======================================
    # Ping Pong
    #=======================================
    -   name    : Rc_PingPong
        testcase: testcases.rdma.rc_pingpong
        args    :
            type: remote_only

    -   name    : Ud_PingPong
        testcase: testcases.rdma.ud_pingpong
        args    :
            type: remote_only

    #=======================================
    # Send
    #=======================================
    -   name    : Rc_SendBw_Only_TcpCm
        testcase: testcases.rdma.rdma_perftest
        stress  : True
        iterators :
            type     : nested
            num_qp   : [1, 8]
            server   : ['yes', 'no']
            size     : [1024]
            command  : ['ib_send_bw']
        args    :
            type: remote_only

    -   name    : Rc_SendBw_Only_RdmaCm
        testcase: testcases.rdma.rdma_perftest
        stress  : True
        iterators :
            type     : nested
            num_qp   : [1]
            server   : ['yes', 'no']
            size     : [1024]
            duration : [5]
            rdma_cm  : ['yes']
            command  : ['ib_send_bw']
        args    :
            type: remote_only

    -   name    : Rc_SendBw_Fml
        testcase: testcases.rdma.rdma_perftest
        stress  : True
        iterators :
            type     : nested
            num_qp   : [1, 8]
            server   : ['yes', 'no']
            size     : [10000]
            duration : [5]
            command  : ['ib_send_bw']
        args    :
            type: remote_only

    -   name    : Rc_SendBw_Fml_Pad
        testcase: testcases.rdma.rdma_perftest
        stress  : True
        iterators :
            type     : nested
            num_qp   : [1, 8]
            server   : ['yes', 'no']
            size     : [12601]
            duration : [5]
            command  : ['ib_send_bw']
        args    :
            type: remote_only

    -   name    : Rc_SendBw_Only_Inline_TcpCm
        testcase: testcases.rdma.rdma_perftest
        stress  : True
        iterators :
            type     : nested
            num_qp   : [1, 8]
            server   : ['yes', 'no']
            size     : [32]
            command  : ['ib_send_bw']
        args    :
            type: remote_only

    -   name    : Rc_SendBw_Only_Inline_RdmaCm
        testcase: testcases.rdma.rdma_perftest
        stress  : True
        iterators :
            type     : nested
            num_qp   : [1]
            server   : ['yes', 'no']
            size     : [32]
            rdma_cm  : ['yes']
            command  : ['ib_send_bw']
        args    :
            type: remote_only

    -   name    : Rc_SendBw_Only_Bidir_TcpCm
        testcase: testcases.rdma.rdma_perftest
        stress  : True
        iterators :
            type     : nested
            num_qp   : [1, 8]
            size     : [1024]
            bidir    : ['yes']
            command  : ['ib_send_bw']
        args    :
            type: remote_only

    -   name    : Rc_SendBw_Only_Bidir_RdmaCm
        testcase: testcases.rdma.rdma_perftest
        stress  : True
        iterators :
            type     : nested
            num_qp   : [1]
            size     : [1024]
            bidir    : ['yes']
            rdma_cm  : ['yes']
            command  : ['ib_send_bw']
        args    :
            type: remote_only

    -   name    : Rc_SendBw_Fml_16Sge_Bidir
        testcase: testcases.rdma.rdma_perftest
        stress  : True
        iterators :
            type     : nested
            num_qp   : [1, 2]
            size     : [65536]
            bidir    : ['yes']
            numsges  : [16]
            command  : ['ib_send_bw']
        args    :
            type: remote_only

    -   name    : Rc_SendLat_Only
        testcase: testcases.rdma.rdma_perftest
        stress  : True
        iterators :
            type     : nested
            command  : ['ib_send_lat']
        args    :
            type: remote_only

    -   name    : Rc_SendLat_Fml_16Sge
        testcase: testcases.rdma.rdma_perftest
        stress  : True
        iterators :
            type     : nested
            size     : [65536]
            numsges  : [1, 16]
            command  : ['ib_send_lat']
        args    :
            type: remote_only

    #=======================================
    # Write
    #=======================================
    -   name    : Rc_WriteBw_Only
        testcase: testcases.rdma.rdma_perftest
        stress  : True
        iterators :
            type     : nested
            num_qp   : [1, 8]
            server   : ['yes', 'no']
            size     : [1024]
            command  : ['ib_write_bw']
        args    :
            type: remote_only

    -   name    : Rc_WriteBw_Fml_TcpCm
        testcase: testcases.rdma.rdma_perftest
        stress  : True
        iterators :
            type     : nested
            num_qp   : [1, 8]
            server   : ['yes', 'no']
            size     : [10000]
            command  : ['ib_write_bw']
        args    :
            type: remote_only

    -   name    : Rc_WriteBw_Fml_RdmaCm
        testcase: testcases.rdma.rdma_perftest
        stress  : True
        iterators :
            type     : nested
            num_qp   : [1]
            server   : ['yes', 'no']
            size     : [10000]
            rdma_cm  : ['yes']
            command  : ['ib_write_bw']
        args    :
            type: remote_only

    -   name    : Rc_WriteBw_Fml_Pad
        testcase: testcases.rdma.rdma_perftest
        stress  : True
        iterators :
            type     : nested
            num_qp   : [1, 8]
            server   : ['yes', 'no']
            size     : [12602]
            command  : ['ib_write_bw']
        args    :
            type: remote_only

    -   name    : Rc_WriteBw_Only_Inline
        testcase: testcases.rdma.rdma_perftest
        stress  : True
        iterators :
            type     : nested
            num_qp   : [1, 8]
            server   : ['yes', 'no']
            size     : [32]
            command  : ['ib_write_bw']
        args    :
            type: remote_only

    -   name    : Rc_WriteBw_Only_Bidir_TcpCm
        testcase: testcases.rdma.rdma_perftest
        stress  : True
        iterators :
            type     : nested
            num_qp   : [1, 8]
            size     : [1024]
            bidir    : ['yes']
            command  : ['ib_write_bw']
        args    :
            type: remote_only

    -   name    : Rc_WriteBw_Only_Bidir_RdmaCm
        testcase: testcases.rdma.rdma_perftest
        stress  : True
        iterators :
            type     : nested
            num_qp   : [1]
            size     : [1024]
            bidir    : ['yes']
            rdma_cm  : ['yes']
            command  : ['ib_write_bw']
        args    :
            type: remote_only

    -   name    : Rc_WriteLat_Only
        testcase: testcases.rdma.rdma_perftest
        stress  : True
        iterators :
            type     : nested
            size     : [32, 1024, 4096]
            command  : ['ib_write_lat']
        args    :
            type: remote_only

    #=======================================
    # Read
    #=======================================
    -   name    : Rc_ReadBw_Only_TcpCm
        testcase: testcases.rdma.rdma_perftest
        stress  : True
        iterators :
            type     : nested
            num_qp   : [1, 8]
            server   : ['yes', 'no']
            size     : [1024]
            command  : ['ib_read_bw']
        args    :
            type: remote_only

    -   name    : Rc_ReadBw_Only_RdmaCm
        testcase: testcases.rdma.rdma_perftest
        stress  : True
        iterators :
            type     : nested
            num_qp   : [1]
            server   : ['yes', 'no']
            size     : [1024]
            rdma_cm  : ['yes']
            command  : ['ib_read_bw']
        args    :
            type: remote_only

    -   name    : Rc_ReadBw_Fml
        testcase: testcases.rdma.rdma_perftest
        stress  : True
        iterators :
            type     : nested
            num_qp   : [1, 8]
            server   : ['yes', 'no']
            size     : [10000]
            command  : ['ib_read_bw']
        args    :
            type: remote_only

    -   name    : Rc_ReadBw_Fml_Pad
        testcase: testcases.rdma.rdma_perftest
        stress  : True
        iterators :
            type     : nested
            num_qp   : [1, 8]
            server   : ['yes', 'no']
            size     : [12603]
            command  : ['ib_read_bw']
        args    :
            type: remote_only

    -   name    : Rc_ReadBw_Only_Bidir_TcpCm
        testcase: testcases.rdma.rdma_perftest
        stress  : True
        iterators :
            type     : nested
            num_qp   : [1, 8]
            size     : [1024]
            bidir    : ['yes']
            command  : ['ib_read_bw']
        args    :
            type: remote_only

    -   name    : Rc_ReadBw_Only_Bidir_RdmaCm
        testcase: testcases.rdma.rdma_perftest
        stress  : True
        iterators :
            type     : nested
            num_qp   : [1]
            size     : [1024]
            bidir    : ['yes']
            rdma_cm  : ['yes']
            command  : ['ib_read_bw']
        args    :
            type: remote_only

    -   name    : Rc_ReadLat_Only
        testcase: testcases.rdma.rdma_perftest
        stress  : True
        iterators :
            type     : nested
            size     : [32, 1024, 4096]
            command  : ['ib_read_lat']
        args    :
            type: remote_only

    #=======================================
    # Vlan
    #=======================================
    -   name    : Rc_Bw_Only_Bidir_Vlan
        testcase: testcases.rdma.rc_bw_bidir_vlan
        stress  : True
        iterators :
            type     : nested
            num_qp   : [1]
            rdma_cm  : ['yes','no']
            flip     : ['yes','no']
            test     : ['send','write','read']
        args    :
            type: remote_only

    #=======================================
    # Atomic
    #=======================================
    -   name    : Rc_AtomicBw_FetchNAdd
        testcase: testcases.rdma.rdma_perftest
        stress  : True
        iterators :
            type     : nested
            num_qp   : [1, 8]
            server   : ['yes', 'no']
            size     : [1024]
            command  : ['ib_atomic_bw']
        args    :
            type: remote_only

    -   name    : Rc_AtomicBw_CmpNSwap
        testcase: testcases.rdma.rdma_perftest
        stress  : True
        iterators :
            type     : nested
            num_qp   : [1, 8]
            server   : ['yes', 'no']
            size     : [1024]
            cmp_swp  : ['yes']
            command  : ['ib_atomic_bw']
        args    :
            type: remote_only

    #=======================================
    # Multi SGE test: Send
    #=======================================
    -   name    : Rc_Bw_MultiSges_Send
        testcase: testcases.rdma.rdma_perftest
        stress  : True
        iterators :
            type     : nested
            server   : ['yes', 'no']
            size     : [65536, 32768, 16384]
            numsges  : [4, 8, 16]
            command  : ['ib_send_bw']
        args    :
            type: remote_only

    #=======================================
    # Multi SGE test: Read
    #=======================================
    -   name    : Rc_Bw_MultiSges_Read
        testcase: testcases.rdma.rdma_perftest
        stress  : True
        iterators :
            type     : nested
            server   : ['yes', 'no']
            size     : [65536, 32768]
            numsges  : [8, 16]
            command  : ['ib_read_bw']
        args    :
            type: remote_only

    #=======================================
    # Multi SGE test: Write
    #=======================================
    -   name    : Rc_Bw_MultiSges_Write
        testcase: testcases.rdma.rdma_perftest
        stress  : True
        iterators :
            type     : nested
            server   : ['yes', 'no']
            size     : [65536, 16384]
            numsges  : [4, 16]
            command  : ['ib_write_bw']
        args    :
            type: remote_only

    #=======================================
    # Multi SGE test, Multiple QPs, All Msgs
    #=======================================
    -   name    : Rc_Bw_MultiSges_MultiQPs
        testcase: testcases.rdma.rdma_perftest
        stress  : True
        iterators :
            type     : nested
            num_qp   : [8]
            server   : ['yes', 'no']
            size     : [65536, 16384]
            numsges  : [16]
            command  : ['ib_send_bw', 'ib_read_bw', 'ib_write_bw']
        args    :
            type: remote_only

    #=======================================
    # UD
    #=======================================

    -   name    : UD_TCPCM
        testcase: testcases.rdma.rdma_perftest
        stress  : True
        iterators :
            type     : nested
            num_qp   : [8]
            server   : ['yes', 'no']
            mtu      : [4096]
            transport: ['UD']
            command  : ['ib_send_bw']

    -   name    : UD_RDMACM
        testcase: testcases.rdma.rdma_perftest
        stress  : True
        iterators :
            type     : nested
            rdma_cm  : ['yes']
            server   : ['yes', 'no']
            mtu      : [4096]
            transport: ['UD']
            command  : ['ib_send_bw']

    #=======================================
    # RPing
    #=======================================
    -   name    : Rping
        testcase: testcases.rdma.rping
        stress  : True
        args    :
            type: remote_only
        iterators :
            type    : nested
            pktsize : [23, 32, 65, 1025, 2048, 4096, 16384, 65535]
            server  : ['yes', 'no']

    #=======================================
    # KRping
    #=======================================
    -   name    : KRping
        testcase: testcases.rdma.krping
        stress  : True
        args    :
            type: remote_only
        iterators :
            type    : nested
            compat  : ['yes', 'no']
            server_inv: ['yes', 'no']
            local_dma_lkey: ['yes', 'no']

    -   name    : KRping_Sweep
        testcase: testcases.rdma.krping
        stress  : True
        args    :
            type: remote_only
        iterators :
            type    : nested
            compat  : ['yes', 'no']
            server_inv: ['yes', 'no']
            local_dma_lkey: ['yes']
            size    : [4096, 65536]

    -   name    : KRping_TxDepth
        testcase: testcases.rdma.krping
        stress  : True
        args    :
            type: remote_only
        iterators :
            type    : nested
            compat  : ['yes', 'no']
            server_inv: ['yes', 'no']
            local_dma_lkey: ['yes']
            txdepth : [256, 1021]
            count   : [5, 100, 2000]

    -   name    : KRping_Fr
        testcase: testcases.rdma.krping
        stress  : True
        args    :
            type: remote_only
        iterators :
            type    : nested
            compat  : ['no']
            fr      : ['yes']
            txdepth : [512]
            count   : [1000]

    #=======================================
    # Teardown
    #=======================================
    -   name    : Rmmod_Rdma
        testcase: testcases.rdma.rmmod_rdma
        args    :
            type: remote_only

