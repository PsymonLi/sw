meta:
    name    : inband100g
    os      : [ linux ]
    nics    : [ pensando, intel, mellanox, broadcom ]    

testcases:
    -   name    : Inband100G_TX_CHECKSUM
        testcase: testcases.drivers.features
        disable : False
        iterators :
            type               : nested
            tx_check_sum       : [ 'off', 'on']
            mtu                : [ '1500' ]
            pktsize            : [ 1024 ]
            proto              : ['udp', 'tcp']
            ipproto            : [ 'v4']
        args    :
            test-type    : inb-100g
            #capture_pcap : true
    
    -   name    : Inband100G_RX_CHECKSUM
        testcase: testcases.drivers.features
        disable : False
        iterators :
            type               : nested
            rx_check_sum       : [ 'off', 'on']
            mtu                : [ '1500' ]
            pktsize            : [ 1024 ]
            proto              : ['udp', 'tcp']
            ipproto            : [ 'v4']
        args    :
            test-type    : inb-100g
            #capture_pcap : true
    
    -   name    : Inband100G_TSO
        testcase: testcases.drivers.features
        disable : False
        iterators :
            type               : nested
            mtu                : [ '8192']
            tso_offload        : [ 'off', 'on']
            #Sg seems to be on for TSO testing.
            sg_offload         : [ 'on']
            #TSO requires rx and tx cheksum
            rx_check_sum       : [ 'on']
            tx_check_sum       : [ 'on']
            pktsize            : [  8192 ]
            proto              : ['udp', 'tcp']
            ipproto            : [ 'v4']
        args    :
            test-type    : inb-100g
            #capture_pcap : true
