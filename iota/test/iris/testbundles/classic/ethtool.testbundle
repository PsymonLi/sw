meta:
    name    : ethtool
    os      : [ linux, freebsd ]
    nics    : [ pensando, intel, mellanox, broadcom ]
    targetcount : 25

testcases:
    -   name    : info
        testcase: testcases.drivers.ethtool.info
        enable  : True
        args    :
            type         : remote_only

    -   name    : transceiver
        testcase: testcases.drivers.ethtool.transceiver
        enable  : True
        args    :
            type         : remote_only

    -   name    : privflags
        testcase: testcases.drivers.ethtool.privflags
        enable  : True
        args    :
            type         : remote_only
