meta:
    name    : internalmgmt
    os      : [ linux ]

testcases:
    -   name    : InternalMgmt_TX_CHECKSUM
        testcase: testcases.drivers.features
        disable : False
        iterators :
            type               : nested
            tx_check_sum       : [ 'off', 'on']
            mtu                : [ '1500' ]
            pktsize            : [ 1024 ]
            proto              : ['udp', 'tcp']
            ipproto            : [ 'v4']
        args    :
            test-type    : int-mgmt
            #capture_pcap : true

    -   name    : InternalMgmt_RX_CHECKSUM
        testcase: testcases.drivers.features
        disable : False
        iterators :
            type               : nested
            rx_check_sum       : [ 'off', 'on']
            mtu                : [ '1500' ]
            pktsize            : [ 1024 ]
            proto              : ['udp', 'tcp']
            ipproto            : [ 'v4']
        args    :
            test-type    : int-mgmt
            #capture_pcap : true

    -   name    : InternalMgmt_TSO
        testcase: testcases.drivers.features
        disable : False
        iterators :
            type               : nested
            mtu                : [ '8192']
            tso_offload        : [ 'off', 'on']
            #Sg seems to be on for TSO testing.
            sg_offload         : [ 'on']
            #TSO requires rx and tx cheksum
            rx_check_sum       : [ 'on']
            tx_check_sum       : [ 'on']
            pktsize            : [  8192 ]
            proto              : ['udp', 'tcp']
            ipproto            : [ 'v4']
        args    :
            test-type    : int-mgmt
            #capture_pcap : true
