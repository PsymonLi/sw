meta:
    name    : oob1g
    os      : [ linux, windows ]
    nics    : [ pensando ]
    targetcount : 1418

testcases:
    -   name    : Oob1G_TX_CHECKSUM
        testcase: testcases.drivers.features
        disable : False
        ignore: False
        iterators :
            type               : nested
            tx_check_sum       : [ 'off', 'on']
            mtu                : [ '1500' ]
            pktsize            : [ 1024 ]
            proto              : ['udp', 'tcp']
            ipproto            : [ 'v4']
        args    :
            test-type    : oob-1g
            #capture_pcap : true

    -   name    : Oob1G_RX_CHECKSUM
        testcase: testcases.drivers.features
        disable : False
        ignore: False
        iterators :
            type               : nested
            rx_check_sum       : [ 'off', 'on']
            mtu                : [ '1500' ]
            pktsize            : [ 1024 ]
            proto              : ['udp', 'tcp']
            ipproto            : [ 'v4']
        args    :
            test-type    : oob-1g
            #capture_pcap : true

    -   name    : Oob1G_TSO
        testcase: testcases.drivers.features
        disable : False
        ignore: False
        iterators :
            type               : nested
            mtu                : [ '1500']
            tso_offload        : [ 'off', 'on']
            #Sg seems to be on for TSO testing.
            sg_offload         : [ 'on']
            #TSO requires rx and tx cheksum
            rx_check_sum       : [ 'on']
            tx_check_sum       : [ 'on']
            pktsize            : [  1024 ]
            proto              : ['udp', 'tcp']
            ipproto            : [ 'v4']
        args    :
            test-type    : oob-1g
            #capture_pcap : true
