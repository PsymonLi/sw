meta:
    name    : ionicdrv
    os      : [ linux, freebsd ]
    nics    : [ pensando, intel, mellanox, broadcom ]
    targetcount : 120

testcases:
    -   name    : UplinkIntf_TX_RING_SIZE
        testcase: testcases.drivers.features
        enable  : True
        iterators :
            type               : nested
            tx_ring_size       : [ 64, 128, 256, 1024, 2048, 4096, 8192]
            mtu                : [ '1500']
            pktsize            : [ 1024]
            proto              : [ 'udp', 'tcp']
            ipproto            : [ 'v4']
        args    :
            test-type         : host
            iperfthreads      : 1
            #capture_pcap : True

    -   name    : UplinkIntf_RX_RING_SIZE
        testcase: testcases.drivers.features
        enable  : True
        iterators :
            type               : nested
            rx_ring_size       : [ 64, 128, 256, 1024, 2048, 4096, 8192]
            mtu                : [ '1500']
            pktsize            : [ 1024 ]
            proto              : [ 'udp', 'tcp']
            ipproto            : [ 'v4']
        args    :
            test-type    : host
            iperfthreads     :  1
            #capture_pcap : true

    -   name    : UplinkIntf_TX_QUEUE_SIZE
        testcase: testcases.drivers.features
        enable  : False
        iterators :
            type               : nested
            tx_queue_size      : [ 4 ]
            mtu                : [ '1500' ]
            pktsize            : [ 1024 ]
            proto              : ['udp', 'tcp']
            ipproto            : [ 'v4']
            vlantag            : ['on', 'off']
        args    :
            test-type    : host
            iperfthreads     :  1
            #capture_pcap : true

    -   name    : UplinkIntf_RX_QUEUE_SIZE
        testcase: testcases.drivers.features
        enable  : False
        iterators :
            type               : nested
            rx_queue_size      : [ 4]
            mtu                : [ '1500' ]
            pktsize            : [ 1024 ]
            proto              : ['udp', 'tcp']
            ipproto            : [ 'v4']
            vlantag            : ['on', 'off']
        args    :
            test-type    : host
            iperfthreads     :  1
            #capture_pcap : true

    -   name    : UplinkIntf_RX_SGL_SIZE
        testcase: testcases.drivers.features
        enable : True
        iterators :
            type               : nested
            rx_sg_size         : [ 2048, 4096]
            mtu                : [ '9000' ]
            pktsize            : [ 1024 ]
            proto              : ['udp', 'tcp']
            ipproto            : [ 'v4']
            vlantag            : ['on', 'off']
        args    :
            test-type    : host
            iperfthreads     :  1
            #capture_pcap : true

    -   name    : UplinkIntf_TX_CHECKSUM
        testcase: testcases.drivers.features
        disable : False
        iterators :
            type               : nested
            tx_check_sum       : [ 'off', 'on']
            mtu                : [ '1500' ]
            pktsize            : [ 1024 ]
            proto              : ['udp', 'tcp']
            ipproto            : [ 'v4']
            vlantag            : ['on', 'off']
        args    :
            test-type    : host
            iperfthreads   : 2
            #capture_pcap : true

    -   name    : UplinkIntf_RX_CHECKSUM
        testcase: testcases.drivers.features
        disable : False
        iterators :
            type               : nested
            rx_check_sum       : [ 'off', 'on']
            mtu                : [ '1500' ]
            pktsize            : [ 1024 ]
            proto              : ['udp', 'tcp']
            ipproto            : [ 'v4']
            vlantag            : ['on', 'off']
        args    :
            test-type    : host
            iperfthreads     :  1
            #capture_pcap : true

    -   name    : UplinkIntf_TSO
        testcase: testcases.drivers.features
        disable : False
        iterators :
            type               : nested
            mtu                : [ '8192']
            tso_offload        : [ 'off', 'on']
            #Sg seems to be on for TSO testing.
            sg_offload         : [ 'on']
            #TSO requires rx and tx cheksum
            rx_check_sum       : [ 'on']
            tx_check_sum       : [ 'on']
            pktsize            : [  8192 ]
            proto              : ['udp', 'tcp']
            ipproto            : [ 'v4']
            vlantag            : ['on', 'off']
        args    :
            test-type    : host
            iperfthreads     :  1
            #capture_pcap : true

    -   name    : UplinkIntf_TSO_AFTER_RESTART
        testcase: testcases.drivers.features
        enable  : True
        iterators :
            type               : nested
            mtu                : [ '8192']
            tso_offload        : [ 'on']
            #Sg seems to be on for TSO testing.
            sg_offload         : [ 'on']
            #TSO requires rx and tx cheksum
            rx_check_sum       : [ 'on']
            tx_check_sum       : [ 'on']
            pktsize            : [  8192 ]
            proto              : [ 'tcp']
            ipproto            : [ 'v4']
            vlantag            : ['on', 'off']
        args    :
            test-type    : host
            iperfthreads     :  1
            restart      : true

    -   name    : UplinkIntf_RSS
        testcase: testcases.drivers.features
        disable : False
        ignore  : True
        iterators :
            type               : nested
            proto              : [ 'tcp']
            ipproto            : [ 'v4']
            hkey               : [ 'symmetric']
            indirection_table  : [ 'equal']
            rx-flow-hash       : [ 'tcp4']
            vlantag            : ['on', 'off']
        args    :
            type: host
            iperfthreads     :  1
            #capture_pcap : true

    -   name    : VLAN_RX_OFFLOAD
        testcase: testcases.drivers.features
        disable : False
        ignore  : True
        iterators :
            type               : nested
            proto              : [ 'tcp']
            ipproto            : [ 'v4']
            rx-vlan-offload    : [ 'on', 'off']
            vlantag            : [ 'on']
        args    :
            type: host
            iperfthreads     :  1
            #capture_pcap : true

    -   name    : VLAN_TX_OFFLOAD
        testcase: testcases.drivers.features
        disable : False
        ignore  : True
        iterators :
            type               : nested
            proto              : [ 'tcp']
            ipproto            : [ 'v4']
            tx-vlan-offload    : [ 'on', 'off']
            vlantag            : [ 'on']
        args    :
            type: host
            iperfthreads     :  1
            #capture_pcap : true

    -   name    : MACVLAN_OFFLOAD
        testcase: testcases.macvlan.macvlan_offload
        enable  : True
        ignore  : True
        iterators :
            type               : nested
            proto              : [ 'tcp']
            ipproto            : [ 'v4']
        args    :
            type: host
            #capture_pcap : true

    -   name    : DriverReloadStress
        testcase: testcases.drivers.driverreload
        disable : False
        iterators :
        args      :
            loops     : 100
