meta:
    name    : ionicdrv_regr
    os      : [ linux, freebsd ]
    nics    : [ pensando, intel, mellanox, broadcom ]
    targetcount : 120

testcases:
    -   name    : EventQ
        testcase: testcases.drivers.eventq
        enable : True

    -   name    : UplinkIntf_INTR_COAL
        testcase: testcases.drivers.interruptcoalescing
        disable : False
        ignore  : True
        iterators :
            type               : nested
            pktsize            : [ 64 ]
            coales_interval    : [ 0, 2, 3, 189, 193]
            proto              : [ 'tcp']
            ipproto            : [ 'v4']
        args    :
            coales_period        : 3
            max_coales_interval  : 192
            type           : remote_only
            #capture_pcap  : True
    -   name    : UplinkIntf_LEGACY_INTR
        testcase: testcases.drivers.features
        enable  : True
        iterators :
            type               : nested
           # Preserve the order of legacy value since last value is what will be used
           # for rest of runs.
            legacy             : ['on', 'off']
        args    :
            test-type    : host
            iperfthreads     :  1
            #capture_pcap : true


    -   name    : UplinkIntf_TX_RING_SIZE
        testcase: testcases.drivers.features
        disable : False
        iterators :
            type               : nested
            tx_ring_size       : [ 64, 128, 2048, 4096, 8192]
            mtu                : [ '1500', '8192']
            pktsize            : [ 128 ]
            proto              : [ 'tcp']
            ipproto            : [ 'v4']
        args    :
            type         : remote_only
            iperfthreads     : 16
            #capture_pcap : True

    -   name    : UplinkIntf_RX_RING_SIZE
        testcase: testcases.drivers.features
        disable : False
        iterators :
            type               : nested
            rx_ring_size       : [ 64, 256, 1024, 4096, 8192]
            mtu                : [ '1500', '8192']
            pktsize            : [ 128 ]
            proto              : [ 'tcp']
            ipproto            : [ 'v4']
        args    :
            type: remote_only
            iperfthreads     : 16
            #capture_pcap : true
    -   name    : UplinkIntf_TX_QUEUE_SIZE
        testcase: testcases.drivers.features
        disable : False
        iterators :
            type               : nested
            tx_queue_size      : [ 1, 8 ]
            mtu                : [ '1500', '8192']
            pktsize            : [ 128, 8192]
            proto              : ['udp', 'tcp']
            ipproto            : [ 'v4']
        args    :
            type: remote_only
            iperfthreads     : 16
            #capture_pcap : true
    -   name    : UplinkIntf_RX_QUEUE_SIZE
        testcase: testcases.drivers.features
        disable : False
        iterators :
            type               : nested
            rx_queue_size      : [ 1, 8 ]
            mtu                : [ '1500', '8192']
            pktsize            : [ 128, 8192]
            proto              : ['udp', 'tcp']
            ipproto            : [ 'v4']
        args    :
            type: remote_only
            iperfthreads     : 16
            #capture_pcap : true
    -   name    : UplinkIntf_TX_CHECKSUM
        testcase: testcases.drivers.features
        disable : False
        iterators :
            type               : nested
            tx_check_sum       : [ 'off', 'on']
            pktsize            : [64, 8192]
            pktsize            : [ 128, 8192]
            proto              : ['udp', 'tcp']
            ipproto            : [ 'v4']
        args    :
            type: remote_only
            iperfthreads     : 16
            #capture_pcap : true
    -   name    : UplinkIntf_RX_CHECKSUM
        testcase: testcases.drivers.features
        disable : False
        iterators :
            type               : nested
            rx_check_sum       : [ 'off', 'on']
            mtu                : [ '1500', '8192']
            pktsize            : [ 128, 8192]
            proto              : ['udp', 'tcp']
            ipproto            : [ 'v4']
        args    :
            type: remote_only
            iperfthreads     : 16
            #capture_pcap : true
    -   name    : UplinkIntf_TSO
        testcase: testcases.drivers.features
        disable : False
        iterators :
            type               : nested
            mtu                : [ '1500', '8192']
            tso_offload        : [ 'off', 'on']
            #Sg seems to be on for TSO testing.
            sg_offload         : [ 'on']
            #TSO requires rx and tx cheksum
            rx_check_sum       : [ 'on']
            tx_check_sum       : [ 'on']
            pktsize            : [ 128, 8192]
            proto              : ['udp', 'tcp']
            ipproto            : [ 'v4']
        args    :
            type: remote_only
            iperfthreads     : 16
            #capture_pcap : true
    -   name    : UplinkIntf_PKT_FILTER
        testcase: testcases.drivers.features
        disable : False
        iterators :
            type               : nested
            mtu                : [ '1500', '8192']
            filter_mcast       : [ 'off', 'on']
            filter_all_mcast   : [ 'off', 'on']
            filter_promiscuous : [ 'off', 'on']
            pktsize            : [ 128, 8192]
            proto              : ['udp', 'tcp']
            ipproto            : [ 'v4']
        args    :
            type: remote_only
            iperfthreads     : 16
            #capture_pcap : true
    -   name    : UplinkIntf_RSS
        testcase: testcases.drivers.features
        disable : False
        ignore  : True
        iterators :
            type               : nested
            proto              : ['udp', 'tcp']
            ipproto            : [ 'v4', 'v6']
            hkey               : [ 'symmetric', 'msft']
            indirection_table  : [ 'equal', 'w1', 'w2', 'default']
            rx-flow-hash       : [ 'tcp4', 'udp4', 'udp6', 'tcp6']
        args    :
            type: remote_only
            iperfthreads     : 16
            #capture_pcap : true

    -   name    : Load_Defaults
        testcase: testcases.drivers.load_defaults
        enable  : False

    -   name    : DriverReloadStress
        testcase: testcases.drivers.driverreload
        enable : true
        iterators :
        args      :
            loops     : 100

    -   name    : DriverWatchdog
        testcase: testcases.drivers.watchdog
        disable : False
        iterators :
        args    :

    -   name    : DriverFwUpdate
        testcase: testcases.drivers.driverfwupdate
        disable : False
        iterators :
        args      :

    -   name    : DriverStatsReset
        testcase: testcases.drivers.driverstatsreset
        disable : False
        iterators :
        args      :

    -   name    : DriverFlowCtrl
        testcase: testcases.drivers.driverflowctrl
        disable : False
        iterators :

    -   name    : DriverLifReset
        testcase: testcases.drivers.driverlifreset
        disable : True
