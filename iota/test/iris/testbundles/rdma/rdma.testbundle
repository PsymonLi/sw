meta:
    name    : rdma
    os      : [ linux, freebsd ]
    nics    : [ pensando, mellanox ]
    targetcount : 150

testcases:
    #=======================================
    # Init
    #=======================================
    -   name    : Build_Rdma
        testcase: testcases.rdma.build_rdma
        args    :
            type: remote_only
            timeout: 300

    -   name    : Insmod_Rdma
        testcase: testcases.rdma.insmod_rdma
        args    :
            type: remote_only
            timeout: 300

    -   name    : Show_Gid
        testcase: testcases.rdma.show_gid
        args    :
            type: remote_only
            timeout: 300

    -   name    : Intf_Mtu
        testcase: testcases.rdma.intf_mtu
        args    :
            type: remote_only
            timeout: 300

    -   name    : QoS_Switch_Config
        testcase: testcases.qos.qos_switch_config
        args    :
            type: remote_only
            intf_qos: False
            pause_enable: True
            pause_type: 0
            timeout: 300

    #=======================================
    # Ping Pong
    #=======================================
    -   name    : Rc_PingPong
        testcase: testcases.rdma.rc_pingpong
        args    :
            type: remote_only
            timeout: 300

    -   name    : Ud_PingPong
        testcase: testcases.rdma.ud_pingpong
        args    :
            type: remote_only
            timeout: 300

    #=======================================
    # Send
    #=======================================
    -   name    : Rc_SendBw_BiDir_Cm
        testcase: testcases.rdma.rdma_perftest
        stress  : True
        iterators :
            type     : nested
            num_qp   : [1]
            size     : [1024]
            bidir    : ['yes']
            rdma_cm  : ['yes', 'no']
            command  : ['ib_send_bw']
        args    :
            type: remote_only
            timeout: 300

    -   name    : Rc_SendBw_BiDir_Cm_Tcpdump
        testcase: testcases.rdma.rdma_perftest
        stress  : True
        iterators :
            type     : nested
            num_qp   : [1]
            size     : [1024]
            bidir    : ['yes']
            command  : ['ib_send_bw']
            tcpdump  : ['yes']
        args    :
            type: remote_only
            timeout: 300

    -   name    : Rc_SendBw_Sweep
        testcase: testcases.rdma.rdma_perftest
        stress  : True
        iterators :
            type     : nested
            num_qp   : [8]
            size     : [32, 1024, 10000, 12601]
            duration : [5]
            command  : ['ib_send_bw']
        args    :
            type: remote_only
            timeout: 300

    -   name    : Rc_SendBw_SQ_Drain
        testcase: testcases.rdma.rdma_perftest
        stress  : True
        iterators :
            type     : nested
            num_qp   : [8]
            size     : [4096, 65535]
            duration : [5]
            command  : ['ib_send_bw']
            sq_drain : ['yes']
            async_event_stats : ['yes']
        args    :
            type: remote_only
            timeout: 300

    -   name    : Rc_SendBw_BiDir_Check_Bw
        testcase: testcases.rdma.rdma_perftest
        stress  : True
        iterators :
            type     : nested
            num_qp   : [1]
            size     : [4096,8192,65536]
            bidir    : ['yes']
            command  : ['ib_send_bw']
            check_bw : ['yes']
        args    :
            type: remote_only
            timeout: 300

    -   name    : Rc_SendLat_Only
        testcase: testcases.rdma.rdma_perftest
        stress  : True
        iterators :
            type     : nested
            command  : ['ib_send_lat']
        args    :
            type: remote_only
            timeout: 300

    #=======================================
    # Reset
    #=======================================
    -   name    : Reset_Rdma
        testcase: testcases.rdma.rdma_reset
        args    :
            type: remote_only
            timeout: 300

    #=======================================
    # Write
    #=======================================
    -   name    : Rc_WriteBw_BiDir_Cm
        testcase: testcases.rdma.rdma_perftest
        stress  : True
        iterators :
            type     : nested
            num_qp   : [1]
            size     : [1024]
            bidir    : ['yes']
            rdma_cm  : ['yes', 'no']
            command  : ['ib_write_bw']
        args    :
            type: remote_only
            timeout: 300

    -   name    : Rc_WriteBw_Sweep
        testcase: testcases.rdma.rdma_perftest
        stress  : True
        iterators :
            type     : nested
            num_qp   : [8]
            size     : [32, 10000, 12602]
            command  : ['ib_write_bw']
        args    :
            type: remote_only
            timeout: 300

    -   name    : Rc_WriteBw_SQ_Drain
        testcase: testcases.rdma.rdma_perftest
        stress  : True
        iterators :
            type     : nested
            num_qp   : [8]
            size     : [4096, 65535]
            duration : [5]
            command  : ['ib_write_bw']
            sq_drain : ['yes']
            async_event_stats : ['yes']
        args    :
            type: remote_only
            timeout: 300

    -   name    : Rc_WriteLat_Only
        testcase: testcases.rdma.rdma_perftest
        stress  : True
        iterators :
            type     : nested
            size     : [32]
            command  : ['ib_write_lat']
        args    :
            type: remote_only
            timeout: 300

    #=======================================
    # Read
    #=======================================
    -   name    : Rc_ReadBw_BiDir_Cm
        testcase: testcases.rdma.rdma_perftest
        stress  : True
        iterators :
            type     : nested
            num_qp   : [1]
            size     : [1024]
            bidir    : ['yes']
            rdma_cm  : ['yes', 'no']
            command  : ['ib_read_bw']
        args    :
            type: remote_only
            timeout: 300

    -   name    : Rc_ReadBw_Sweep
        testcase: testcases.rdma.rdma_perftest
        stress  : True
        iterators :
            type     : nested
            num_qp   : [8]
            size     : [1024, 10000, 12603]
            command  : ['ib_read_bw']
        args    :
            type: remote_only
            timeout: 300

    -   name    : Rc_ReadBw_SQ_Drain
        testcase: testcases.rdma.rdma_perftest
        stress  : True
        iterators :
            type     : nested
            num_qp   : [8]
            size     : [4096, 65535]
            duration : [5]
            command  : ['ib_read_bw']
            sq_drain : ['yes']
            async_event_stats : ['yes']
        args    :
            type: remote_only
            timeout: 300

    -   name    : Rc_ReadLat_Only
        testcase: testcases.rdma.rdma_perftest
        stress  : True
        iterators :
            type     : nested
            size     : [32]
            command  : ['ib_read_lat']
        args    :
            type: remote_only
            timeout: 300

    #=======================================
    # Vlan
    #=======================================
    -   name    : Rc_Bw_Only_Bidir_Vlan
        testcase: testcases.rdma.rc_bw_bidir_vlan
        stress  : True
        iterators :
            type     : nested
            num_qp   : [1]
            rdma_cm  : ['yes', 'no']
            test     : ['send','write','read']
        args    :
            type: remote_only
            timeout: 300

    #=======================================
    # Atomic
    #=======================================
    -   name    : Rc_AtomicBw
        testcase: testcases.rdma.rdma_perftest
        stress  : True
        iterators :
            type     : nested
            num_qp   : [1]
            size     : [1024]
            cmp_swp  : ['yes', 'no']
            command  : ['ib_atomic_bw']
        args    :
            type: remote_only
            timeout: 300

    #=======================================
    # Multi SGE test, Multiple QPs, All Msgs
    #=======================================
    -   name    : Rc_Bw_MultiSges_MultiQPs
        testcase: testcases.rdma.rdma_perftest
        stress  : True
        iterators :
            type     : nested
            num_qp   : [8]
            size     : [65536]
            numsges  : [4, 8, 16]
            command  : ['ib_send_bw', 'ib_read_bw', 'ib_write_bw']
        args    :
            type: remote_only
            timeout: 300

    #=======================================
    # UD
    #=======================================

    -   name    : UD_TCPCM
        testcase: testcases.rdma.rdma_perftest
        stress  : True
        iterators :
            type     : nested
            num_qp   : [8]
            server   : [ 'yes', 'no']
            mtu      : [4096]
            transport: ['UD']
            command  : ['ib_send_bw']
        args    :
            timeout: 300

    -   name    : UD_RDMACM
        testcase: testcases.rdma.rdma_perftest
        stress  : True
        iterators :
            type     : nested
            rdma_cm  : [ 'yes']
            server   : [ 'yes', 'no']
            mtu      : [4096]
            transport: ['UD']
            command  : ['ib_send_bw']
        args    :
            timeout: 300

    #=======================================
    # RPing
    #=======================================
    -   name    : Rping
        testcase: testcases.rdma.rping
        stress  : True
        args    :
            type: remote_only
            timeout: 300
        iterators :
            type    : nested
            pktsize : [23, 65535]
            enable_dcqcn : ['yes']

    #=======================================
    # KRping
    #=======================================
    -   name    : KRping
        testcase: testcases.rdma.krping
        stress  : True
        args    :
            type: remote_only
            timeout: 300
        iterators :
            type    : nested
            server_inv: ['yes', 'no']
            local_dma_lkey: ['yes', 'no']
            enable_dcqcn : ['yes']

    -   name    : KRping_Sweep
        testcase: testcases.rdma.krping
        stress  : True
        args    :
            type: remote_only
            timeout: 300
        iterators :
            type    : nested
            server_inv: ['yes', 'no']
            local_dma_lkey: ['yes']
            size    : [32, 4096, 65536]

    -   name    : KRping_TxDepth
        testcase: testcases.rdma.krping
        stress  : True
        args    :
            type: remote_only
            timeout: 300
        iterators :
            type    : nested
            server_inv: ['yes', 'no']
            local_dma_lkey: ['yes']
            txdepth : [256, 1021]
            count   : [2000]

    -   name    : KRping_Fr
        testcase: testcases.rdma.krping
        stress  : True
        args    :
            type: remote_only
            timeout: 300
        iterators :
            type    : nested
            fr      : ['yes']
            txdepth : [512]
            count   : [1000]

    -   name    : KRping_Bw
        testcase: testcases.rdma.krping
        stress  : True
        args    :
            type: remote_only
            timeout: 300
        iterators :
            type    : nested
            bw      : ['yes']
            duplex  : ['yes']
            size    : [4096]
            txdepth : [512]
            count   : [100000]

    -   name    : KRping_Wlat
        testcase: testcases.rdma.krping
        stress  : True
        args    :
            type: remote_only
            timeout: 300
        iterators :
            type    : nested
            wlat    : ['yes']
            size    : [1024]
            txdepth : [512]
            count   : [10000]

    -   name    : KRping_Rlat
        testcase: testcases.rdma.krping
        stress  : True
        args    :
            type: remote_only
            timeout: 300
        iterators :
            type    : nested
            rlat    : ['yes']
            size    : [1024]
            txdepth : [512]
            count   : [10000]

    #=======================================
    # DCQCN
    #=======================================
    -   name    : Rc_SendBw_Sweep
        testcase: testcases.rdma.rdma_perftest
        stress  : True
        iterators :
            type     : nested
            num_qp   : [8]
            size     : [1024, 10000]
            count    : [110000]
            bidir    : ['yes']
            enable_dcqcn : ['yes']
            command  : ['ib_send_bw']
        args    :
            type: remote_only
            timeout: 300

    -   name    : Rc_SendBw_SQ_Drain
        testcase: testcases.rdma.rdma_perftest
        stress  : True
        iterators :
            type     : nested
            num_qp   : [8]
            size     : [65535]
            count    : [110000]
            bidir    : ['yes']
            enable_dcqcn : ['yes']
            command  : ['ib_send_bw']
            sq_drain : ['yes']
            async_event_stats : ['yes']
        args    :
            type: remote_only
            timeout: 300

    -   name    : Rc_WriteBw_Sweep
        testcase: testcases.rdma.rdma_perftest
        stress  : True
        iterators :
            type     : nested
            num_qp   : [8]
            size     : [1024, 10000]
            bidir    : ['yes']
            enable_dcqcn : ['yes']
            command  : ['ib_write_bw']
        args    :
            type: remote_only
            timeout: 300

    -   name    : Rc_WriteBw_SQ_Drain
        testcase: testcases.rdma.rdma_perftest
        stress  : True
        iterators :
            type     : nested
            num_qp   : [8]
            size     : [65535]
            count    : [110000]
            bidir    : ['yes']
            enable_dcqcn : ['yes']
            command  : ['ib_write_bw']
            sq_drain : ['yes']
            async_event_stats : ['yes']
        args    :
            type: remote_only
            timeout: 300

    -   name    : Rc_ReadBw_Sweep
        testcase: testcases.rdma.rdma_perftest
        stress  : True
        iterators :
            type     : nested
            num_qp   : [8]
            size     : [1024, 10000]
            bidir    : ['yes']
            enable_dcqcn : ['yes']
            command  : ['ib_read_bw']
        args    :
            type: remote_only
            timeout: 300

    -   name    : Rc_ReadBw_SQ_Drain
        testcase: testcases.rdma.rdma_perftest
        stress  : True
        iterators :
            type     : nested
            num_qp   : [8]
            size     : [65535]
            count    : [110000]
            bidir    : ['yes']
            enable_dcqcn : ['yes']
            command  : ['ib_read_bw']
            sq_drain : ['yes']
            async_event_stats : ['yes']
        args    :
            type: remote_only
            timeout: 300

    #=======================================
    # Teardown
    #=======================================
    -   name    : QoS_Switch_Unconfig
        testcase: testcases.qos.qos_switch_config
        args    :
            type: remote_only
            intf_qos: False
            pause_enable: False
            pause_type: 0
            timeout: 300

    -   name    : Rmmod_Rdma
        testcase: testcases.rdma.rmmod_rdma
        args    :
            type: remote_only
            timeout: 300
