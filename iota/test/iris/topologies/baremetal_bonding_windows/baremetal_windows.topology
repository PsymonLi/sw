meta:
    name: BM-windows
    description: Classic NIC Baremetal Topology for windows

nodes:
    - node: node1
      name: node1
      role: auto
      mode: bond
      bonding: 
        pairing         : auto 
        lb_scheme       : Dynamic
        teaming_mode    : Static
      workloads:
        windows:
            type    : WORKLOAD_TYPE_BARE_METAL
            image   : "N/A"

    - node: node2
      name: node2
      role: auto
      mode: bond
      bonding: 
        pairing         : auto 
        lb_scheme       : Dynamic
        teaming_mode    : Static
      workloads:
        windows:
            type    : WORKLOAD_TYPE_BARE_METAL
            image   : "N/A"


