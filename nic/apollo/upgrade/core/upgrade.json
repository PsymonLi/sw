{
  "svc": {
    "event_sequence": "parallel",
    "names": [
      "sysmgr",
      "pdsagent",
      "vpp"
    ]
  },
  "stages": {
    "validation": {
      "rsp_timeout": "100ms",
      "events": [
        {
          "name": "svc_rsp_ok",
          "next_stage": "start"
        },
        {
          "name": "svc_rsp_fail",
          "next_stage": "exit"
        },
        {
          "name": "svc_rsp_crit",
          "next_stage": "exit"
        },
        {
          "name": "svc_no_rsp",
          "next_stage": "exit"
        }
      ]
    },
    "start": {
      "rsp_timeout": "100ms",
      "events": [
        {
          "name": "svc_rsp_ok",
          "next_stage": "prepare"
        },
        {
          "name": "svc_rsp_fail",
          "next_stage": "abort"
        },
        {
          "name": "svc_rsp_crit",
          "next_stage": "critical"
        },
        {
          "name": "svc_no_rsp",
          "next_stage": "abort"
        }
      ],
      "pre_hooks": [
        "pre_hook_script1.sh",
        "pre_hook_script2.sh"
      ],
      "post_hooks": [
        "post_hook_script1.sh",
        "post_hook_script2.sh"
      ]
    },
    "prepare": {
      "rsp_timeout": "100ms",
      "events": [
        {
          "name": "svc_rsp_ok",
          "next_stage": "backup"
        },
        {
          "name": "svc_rsp_fail",
          "next_stage": "abort"
        },
        {
          "name": "svc_rsp_crit",
          "next_stage": "critical"
        },
        {
          "name": "svc_no_rsp",
          "next_stage": "abort"
        }
      ]
    },
    "backup": {
      "rsp_timeout": "100ms",
      "events": [
        {
          "name": "svc_rsp_ok",
          "next_stage": "upgrade"
        },
        {
          "name": "svc_rsp_fail",
          "next_stage": "rollback"
        },
        {
          "name": "svc_rsp_crit",
          "next_stage": "critical"
        },
        {
          "name": "svc_no_rsp",
          "next_stage": "rollback"
        }
      ]
    },
    "upgrade": {
      "rsp_timeout": "100ms",
      "events": [
        {
          "name": "svc_rsp_ok",
          "next_stage": "verify"
        },
        {
          "name": "svc_rsp_fail",
          "next_stage": "rollback"
        },
        {
          "name": "svc_rsp_crit",
          "next_stage": "critical"
        },
        {
          "name": "svc_no_rsp",
          "next_stage": "rollback"
        }
      ]
    },
    "verify": {
      "rsp_timeout": "100ms",
      "events": [
        {
          "name": "svc_rsp_ok",
          "next_stage": "finish"
        },
        {
          "name": "svc_rsp_fail",
          "next_stage": "rollback"
        },
        {
          "name": "svc_rsp_crit",
          "next_stage": "critical"
        },
        {
          "name": "svc_no_rsp",
          "next_stage": "rollback"
        }
      ]
    },
    "finish": {
      "rsp_timeout": "100ms",
      "events": [
        {
          "name": "svc_rsp_ok",
          "next_stage": "exit"
        },
        {
          "name": "svc_rsp_fail",
          "next_stage": "exit"
        },
        {
          "name": "svc_rsp_crit",
          "next_stage": "critical"
        },
        {
          "name": "svc_no_rsp",
          "next_stage": "exit"
        }
      ]
    },
    "abort": {
      "rsp_timeout": "100ms",
      "events": [
        {
          "name": "svc_rsp_ok",
          "next_stage": "finish"
        },
        {
          "name": "svc_rsp_fail",
          "next_stage": "critical"
        },
        {
          "name": "svc_rsp_crit",
          "next_stage": "critical"
        },
        {
          "name": "svc_no_rsp",
          "next_stage": "critical"
        }
      ]
    },
    "rollback": {
      "rsp_timeout": "100ms",
      "events": [
        {
          "name": "svc_rsp_ok",
          "next_stage": "finish"
        },
        {
          "name": "svc_rsp_fail",
          "next_stage": "critical"
        },
        {
          "name": "svc_rsp_crit",
          "next_stage": "critical"
        },
        {
          "name": "svc_no_rsp",
          "next_stage": "critical"
        }
      ]
    },
    "critical": {
      "rsp_timeout": "100ms",
      "events": [
        {
          "name": "svc_rsp_ok",
          "next_stage": "exit"
        },
        {
          "name": "svc_rsp_fail",
          "next_stage": "exit"
        },
        {
          "name": "svc_rsp_crit",
          "next_stage": "exit"
        },
        {
          "name": "svc_no_rsp",
          "next_stage": "exit"
        }
      ]
    },
    "exit": {
      "rsp_timeout": "100ms",
      "events": [
        {
          "name": "svc_rsp_ok",
          "next_stage": "exit"
        },
        {
          "name": "svc_rsp_fail",
          "next_stage": "exit"
        },
        {
          "name": "svc_rsp_crit",
          "next_stage": "exit"
        },
        {
          "name": "svc_no_rsp",
          "next_stage": "exit"
        }
      ]
    }
  }
}
