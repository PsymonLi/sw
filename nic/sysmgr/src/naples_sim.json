    "flags": ["no_watchdog", "non_critical"]
}, {
    "name": "nmd",
    "command": "/naples/nic/bin/nmd -hostif lo -standalone=false -mode network",
    "dependencies": ["delphi"],
    "flags": ["no_watchdog", "non_critical"]
}, {
    "name": "tmagent",
    "command": "/naples/nic/bin/tmagent -hostif lo",
    "dependencies": ["delphi", "hal"],
    "flags": ["no_watchdog", "non_critical"]
}, {
    "name": "nevtsproxy",
    "command": "/naples/nic/bin/nevtsproxy",
    "dependencies": ["delphi",  "hal"],
    "flags": ["no_watchdog", "non_critical"]
}, {
    "name": "upgrade",
    "command": "/naples/nic/bin/upgrade",
    "dependencies": ["delphi"],
    "flags": ["no_watchdog", "non_critical"]
}]
