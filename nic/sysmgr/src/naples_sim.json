[{
    "name": "delphi",
    "command": "/naples/nic/bin/delphi_hub /tmp/delphi.dat",
    "dependencies": [],
    "flags": ["no_watchdog", "non_critical"]
}, {
    "name": "hal",
    "command": "/naples/nic/bin/hal -c hal.json",
    "dependencies": ["delphi"],
    "flags": ["no_watchdog", "non_critical"]
}, {
    "name": "netagent",
    "command": "/naples/nic/bin/netagent -datapath hal",
