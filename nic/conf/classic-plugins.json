{
    "plugins": [
        {
            "name"      : "classic",
            "provider"  : "pensando.io",
            "lib"       : "libplugin_classic.so",
            "init_func" : "classic_init",
            "exit_func" : "classic_exit",
            "auto_load" : "true",
            "deps"      : ["pensando.io/nw"],
            "features"  : ["fwding"]
        },
        {
            "name"      : "lif",
            "provider"  : "pensando.io",
            "lib"       : "libcfg_plugin_lif.so",
            "init_func" : "lif_init",
            "exit_func" : "lif_exit",
            "auto_load" : "true"
        },
        {
            "name"      : "nw",
            "provider"  : "pensando.io",
            "lib"       : "libcfg_plugin_nw.so",
            "auto_load" : "true",
            "deps"      : ["pensando.io/lif"]
        },
        {
            "name"      : "mcast",
            "provider"  : "pensando.io",
            "lib"       : "libcfg_plugin_mcast.so",
            "init_func" : "mcast_init",
            "exit_func" : "mcast_exit",
            "auto_load" : "true",
            "deps"      : ["pensando.io/nw"]
        }
    ],
    "pipelines": [
        {
            "name": "flow-miss",
            "selector": { "lif": "SERVICE_LIF_CPU", "qid": "CPUCB_ID_FLOWMISS" },
            "outbound_features": [
                "pensando.io/classic:fwding"
            ]
        }
    ]
}
