{
  "make_cmd": "make V=1 GCOV=\"--coverage -DCOVERAGE\" -j8",
  "nic_dirs": [
    "hal",
    "fte",
    "utils",
    "proto",
    "agents"
  ],
  "remove_files": [
    "/usr/include/*",
    "/usr/local/*",
    "third-party/*"
  ],
  "file_patterns": [
    ".c",
    ".cc"
  ],
  "cov_tests": [
    {
      "name": "gtest",
      "cmd": "make gtest",
      "output_dir_name": "gtest_cov"
    },
    {
      "name": "dol",
      "cmd": "./run.py --coveragerun",
      "output_dir_name": "dol_cov"
    }
  ]
}
