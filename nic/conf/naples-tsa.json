{
	"Name" : "ExampleConfig",
	"FilesystemRoot" : "/data/techsupport",
	"Retention" : 0,
	"PrepActions" : [
				{
					"Name" : "Action1",
					"Method" : 0,
					"Command" : "ls -l"
				}
			],

	"CollectActions" : [
                                {
                                        "Name" : "All Processes",
                                        "Method" : 0,
                                        "Command" : "ps -aefl"
                                },
                                {
                                        "Name" : "Storage",
                                        "Method" : 0,
                                        "Command" : "df -k"
                                },
                                {
                                        "Name" : "CPU Info",
                                        "Method" : 0,
                                        "Command" : "cat /proc/cpuinfo"
                                },
                                {
                                        "Name" : "Collect Core",
                                        "Method" : 0,
					"Command" : "mkdir core && cp /data/core/* core"
                                },
				{
                                        "Name" : "Collect Logs",
                                        "Method" : 0,
                                        "Command" : "mkdir logs && cp /var/log/pensando/* logs/"
                                },
                                {
                                        "Name" : "Collect Sysmgr Logs",
                                        "Method" : 0,
                                        "Command" : "mkdir sysmgr-logs && cp /var/log/sysmgr/* /var/log/sysmgr.log sysmgr-logs/"
                                },
                                {
                                        "Name" : "Collect OBFL Logs",
                                        "Method" : 0,
                                        "Command" : "mkdir obfl-logs && cp /obfl/* obfl-logs/"
                                },
                                {
                                        "Name" : "HBM Bandwidth",
                                        "Method" : 0,
                                        "Command" : "halctl show platform hbm bandwidth"
                                },
                                {
                                        "Name" : "LLC Stats",
                                        "Method" : 0,
                                        "Command" : "halctl show hbm llc-stats"
                                },
                                {
                                        "Name" : "Show Lif",
                                        "Method" : 0,
                                        "Command" : "halctl show lif"
                                },                                
				{
                                        "Name" : "Table Info",
                                        "Method" : 0,
                                        "Command" : "halctl show table info"
				},
				{
                                        "Name" : "NaplesStatus Delphi Object",
                                        "Method" : 0,
                                        "Command" : "delphictl db get NaplesStatus"
                                }
			],

	"ExportActions" : [
			],

	"RetentionPolicy" : 0
}				
